\\ub2e4. \\u2462 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\\uc740 \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00 \\uac00\\uc785\\uc790\\uc5d0 \\ud55c\\ud558\\uc5ec \\uac00\\uc785\\uc774 \\uac00\\ub2a5\\ud569\\ub2c8\\ub2e4. \\u2463 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0 \\uac00\\uc785\\ud558\\uace0 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uae30\\uac04 \\uc911 \\ud53c\\ubcf4 \\ud5d8\\uc790\\uc5d0\\uac8c \\ub0a9\\uc785\\uba74\\uc81c \\uc0ac\\uc720\\uac00 \\ubc1c\\uc0dd\\ud55c \\uacbd\\uc6b0, \\uae30\\ubcf8\\uacc4\\uc57d\\uc758 \\ucc28\\ud68c \\uc774\\ud6c4 \\ubcf4\\ud5d8\\ub8cc\\uc758 \\ub0a9\\uc785\\uc744 \\uba74\\uc81c\\ud558\\uc5ec \\ub4dc\\ub9bd\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720, \\uc9c0\\uae09\\uae08\\uc561 \\ubc0f \\uc9c0\\uae09\\uc81c\\ud55c\\uc0ac\\ud56d\", \"subsubtitle\": \"\\u25a1 \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720 \\ubc0f \\uc9c0\\uae09\\uae08\\uc561\", \"Head\": \"\", \"text\": \"\\ud5d8\\ub8cc\\uac00 \\uc81c\\ud55c\\ub418\\uac70\\ub098 \\uc778\\uc218\\uac00 \\ubd88\\uac00\\ub2a5\\ud560 \\uc218 \\uc788\\uc73c\\uba70, \\uacbd\\uc6b0\\uc5d0 \\ub530\\ub77c\\uc11c\\ub294 \\uac74\\uac15\\uc9c4 \\ub2e8 \\uacb0\\uacfc\\ub97c \\uc694\\uad6c\\ud560 \\uc218 \\uc788\\uc2b5\\ub2c8\\ub2e4. \\u261e \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uac00\\uc785\\uae08\\uc561\\uc740 \\uae30\\ubcf8\\ubcf4\\ud5d8\\ub8cc(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubcf4\\ud5d8\\ub8cc \\uc81c\\uc678)\\uc640 \\ub3d9\\uc77c\\ud558\\uac8c \\uc801\\uc6a9\\ud558\\uba70, \\uae30\\ubcf8\\ubcf4\\ud5d8\\ub8cc(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubcf4\\ud5d8\\ub8cc \\uc81c\\uc678)\\uac00 \\ubcc0 \\ub3d9\\ub418\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uac00\\uc785\\uae08\\uc561\\ub3c4 \\ub3d9\\uc77c\\ud558\\uac8c \\ubcc0 \\ub3d9\\ud558\\uc5ec \\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720, \\uc9c0\\uae09\\uae08\\uc561 \\ubc0f \\uc9c0\\uae09\\uc81c\\ud55c\\uc0ac\\ud56d\", \"subsubtitle\": \"\\u25a1 \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc81c\\ud55c\\uc0ac\\ud56d\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294 \\uc81c 1 \\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ubc1b\\uc740 \\ub54c\\ubd80\\ud130 \\uc774 \\uc0c1\\ud488\\uc758 \\uc57d\\uad00\\uc774 \\uc815\\ud55c \\ubc14 \\uc5d0 \\ub530\\ub77c \\ubcf4\\uc7a5\\uc744 \\ud569\\ub2c8\\ub2e4. \\ud68c\\uc0ac\\uc758 \\ucc45\\uc784\\uc740 \\ubcf4\\ud5d8\\uc99d\\uad8c(\\ubcf4\\ud5d8\\uac00\\uc785\\uc99d\\uc11c) \\uc5d0 \\uae30\\uc7ac\\ub41c \\ubcf4\\ud5d8\\uae30\\uac04\\uc758 \\uccab\\ub0a0\\uc5d0 \\uc2dc\\uc791\\ud569\\ub2c8\\ub2e4. \\ub2e8, \\uc81c 1 \\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ub0a9\\uc785\\ud558\\uae30 \\uc804 \\uc0ac\\uace0\\uc5d0 \\ub300\\ud574\\uc11c\\ub294 \\ubcf4\\uc0c1\\ud558\\uc9c0 \\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2461 \\ud558\\ub098\\uc758 \\uc0c1\\ud574\\ub85c \\uc778\\ud558\\uc5ec \\ud68c\\uc0ac\\uac00 \\uc9c0\\uae09\\ud558\\ub294 \\ud6c4\\uc720\\uc7a5\\ud574\\ubcf4\\ud5d8\\uae08\\uc740 \\ud6c4\\uc720\\uc7a5 \\ud574\\ubcf4\\ud5d8\\uac00\\uc785\\uae08\\uc561\\uc744 \\ud55c\\ub3c4\\ub85c \\ud569\\ub2c8\\ub2e4. \\u2462 \\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud558\\uc9c0 \\uc54a\\ub294 \\uc0ac\\uc720 \\ub4f1 \\uae30\\ud0c0 \\uc138\\ubd80\\uc801\\uc778 \\uc0ac\\ud56d\\uc740 \\uae30\\ubcf8\\uacc4\\uc57d \\ubc0f \\uc120\\ud0dd\\uacc4\\uc57d \\ub0b4\\uc6a9\\uc5d0 \\ub530\\ub77c \\ubcf4\\ud5d8\\uae08\\uc758 \\uc9c0\\uae09\\uc774 \\uc81c\\ud55c\\ub420 \\uc218 \\uc788\\uc2b5\\ub2c8\\ub2e4\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ubcf4\\ud5d8\\ub8cc \\uc0b0\\ucd9c\\uae30\\ucd08 \\ubc0f \\uacf5\\uc2dc\\uc774\\uc728\", \"subsubtitle\": \"\\u25a1 \\ubcf4\\ud5d8\\ub8cc \\uad6c\\uc131\", \"Head\": \"\", \"text\": \"\\uacc4\\uc57d\\uc790\\uac00 \\ub0a9\\uc785\\ud558\\ub294 \\ubcf4\\ud5d8\\ub8cc\\ub294 \\ubcf4\\ud5d8\\uc0ac\\uace0 \\ubc1c\\uc0dd\\uc2dc \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc744 \\uc704\\ud55c \\uc704\\ud5d8\\ubcf4\\ud5d8\\ub8cc, \\uc5f0\\uae08\\uc744 \\uc9c0\\uae09\\ud558\\uae30 \\uc704\\ud55c \\uc800\\ucd95\\ubcf4\\ud5d8\\ub8cc, \\uadf8\\ub9ac\\uace0 \\ubcf4\\ud5d8\\ud68c\\uc0ac\\uc758 \\uc0ac\\uc5c5\\uacbd\\ube44\\ub97c \\uc704\\ud55c \\ubd80\\uac00\\ubcf4\\ud5d8\\ub8cc\\ub85c \\uad6c\\uc131\\ub429\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ubcf4\\ud5d8\\ub8cc \\uc0b0\\ucd9c\\uae30\\ucd08 \\ubc0f \\uacf5\\uc2dc\\uc774\\uc728\", \"subsubtitle\": \"\\u25a1 \\uc801\\uc6a9\\uc774\\uc728\", \"Head\": \"\", \"text\": \"\\ubcf4\\uc7a5\\ubd80\\ubd84 \\uc801\\uc6a9\\uc774\\uc728\\uc774\\ub780? \\ubcf4\\ud5d8\\ud68c\\uc0ac\\ub294 \\uc7a5\\ub798\\uc758 \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc744 \\ub300\\ube44\\ud558\\uc5ec \\uacc4\\uc57d\\uc790\\uac00 \\ub0a9\\uc785\\ud55c \\ubcf4\\ud5d8 \\ub8cc\\ub97c \\uc801\\ub9bd\\ud574 \\ub450\\ub294\\ub370 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uc2dc\\uc810\\uacfc \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc2dc\\uc810\\uc5d0\\ub294 \\uc2dc\\ucc28 \\uac00 \\ubc1c\\uc0dd\\ud558\\uac8c \\ub429\\ub2c8\\ub2e4, \\uc774 \\uae30\\uac04\\ub3d9\\uc548 \\ubcf4\\ud5d8\\ud68c\\uc0ac\\ub294 \\uc801\\ub9bd\\ub41c \\uae08\\uc561\\uc744 \\uc6b4\\uc6a9 \\ud560 \\uc218 \\uc788\\uc73c\\ubbc0\\ub85c \\uc6b4\\uc6a9\\uc5d0 \\ub530\\ub77c \\uae30\\ub300\\ub418\\ub294 \\uc218\\uc775\\uc744 \\ubbf8\\ub9ac \\uc608\\uc0c1\\ud558\\uc5ec \\uc77c\\uc815 \\ud55c \\ube44\\uc728\\ub85c \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ud560\\uc778\\ud574 \\uc8fc\\ub294\\ub370, \\uc774\\ub7ec\\ud55c \\ud560\\uc778\\uc728\\uc744 \\\"\\ubcf4\\uc7a5\\ubd80\\ubd84 \\uc801 \\uc6a9\\uc774\\uc728\\\"\\uc774\\ub77c\\uace0 \\ud569\\ub2c8\\ub2e4. \\uc77c\\ubc18\\uc801\\uc73c\\ub85c \\ubcf4\\uc7a5\\ubd80\\ubd84 \\uc801\\uc6a9\\uc774\\uc728\\uc774 \\ub192\\uc73c\\uba74 \\ubcf4 \\ud5d8\\ub8cc\\ub294 \\ub0b4\\ub824\\uac00\\uace0, \\ubcf4\\uc7a5\\ubd80\\ubd84 \\uc801\\uc6a9\\uc774\\uc728\\uc774 \\ub0ae\\uc73c\\uba74 \\ubcf4\\ud5d8\\ub8cc\\ub294 \\uc62c\\ub77c\\uac11\\ub2c8 \\ub2e4. \\ubb34\\ubc30\\ub2f9 \\uc5f0\\uae08\\uc800\\ucd95 \\uacf5\\uc2dc\\uc774\\uc728\\u2160\\ub780? \\uc774 \\ubcf4\\ud5d8\\uc5d0 \\uc801\\uc6a9\\ub418\\ub294 \\ubb34\\ubc30\\ub2f9 \\uc5f0\\uae08\\uc800\\ucd95 \\uacf5\\uc2dc\\uc774\\uc728\\u2160\\uc740 \\uc774 \\ubcf4\\ud5d8\\uc758 \\uc0ac\\uc5c5\\ubc29 \\ubc95\\uc11c\\uc5d0\\uc11c \\uc815\\ud55c \\ubc29\\ubc95\\uc5d0 \\ub530\\ub77c \\ud68c\\uc0ac\\uc758 \\uc6b4\\uc6a9\\uc790\\uc0b0\\uc774\\uc775\\ub960\\uacfc \\uac1d\\uad00\\uc801\\uc778 \\uc678\\ubd80 \\uc9c0\\ud45c\\uae08\\ub9ac\\ub97c \\uac00\\uc911 \\ud3c9\\uade0\\ud558\\uc5ec \\uc0b0\\ucd9c\\ud55c \\uacf5\\uc2dc\\uae30\\uc900\\uc774\\uc728\\uc5d0 \\uc870\\uc815\\ub960\\uc744 \\uac00\\uac10\\ud558 \\uc5ec \\uacb0\\uc815\\ud558\\uba70, \\uacf5\\uc2dc\\uae30\\uc900\\uc774\\uc728\\uc758 80~120% \\ubc94\\uc704 \\ub0b4\\uc5d0\\uc11c \\uc815\\ud569\\ub2c8\\ub2e4. \\u203b \\uc774\\uc5d0 \\ub300\\ud55c \\ubcf4\\ub2e4 \\uc790\\uc138\\ud55c \\ub0b4\\uc6a9\\uc740 \\uc778\\ud130\\ub137\\ud648\\ud398\\uc774\\uc9c0 \\uc0c1\\ud488\\uacf5\\uc2dc\\uc2e4\\uc5d0\\uc11c \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc758 \\uc0ac\\uc5c5\\ubc29\\ubc95\\uc11c\\ub97c \\ucc38\\uc870\\ud558\\uc2dc\\uae30 \\ubc14\\ub78d\\ub2c8\\ub2e4. \\uc774 \\ubcf4\\ud5d8\\uc0c1\\ud488\\uc758 \\ubcf4\\uc7a5\\ubd80\\ubd84 \\uc801\\uc6a9\\uc774\\uc728\\uc740 \\uc5f0\\ub2e8\\uc704 \\ubcf5\\ub9ac 3.25%\\uc785\\ub2c8\\ub2e4. \\uc774 \\ubcf4\\ud5d8\\uc758 \\uc801\\ub9bd\\uc21c\\ubcf4\\ud5d8\\ub8cc\\ub294 \\uc774 \\ubcf4\\ud5d8\\uc758 \\\"\\ubb34\\ubc30\\ub2f9 \\uc5f0\\uae08\\uc800\\ucd95 \\uacf5\\uc2dc\\uc774\\uc728\\u2160 \\\"(\\uc774\\ud558 \\uacf5\\uc2dc\\uc774\\uc728\\uc774\\ub77c \\ud569\\ub2c8\\ub2e4)\\uc5d0 \\uc758\\ud574 \\uc5f0\\ub3d9\\ub418\\uc5b4 \\uc801\\ub9bd\\ub429\\ub2c8\\ub2e4. \\ub2e8, \\ucd5c\\uc800 \\ubcf4\\uc99d\\uc774\\uc728\\uc740 \\uac00\\uc785\\ud6c4 5 \\ub144 \\uc774\\ud558\\uc5d0\\ub294 \\uc5f0\\ub2e8\\uc704 \\ubcf5\\ub9ac 2.50%, 5 \\ub144 \\ucd08\\uacfc 10 \\ub144 \\uc774\\ud558\\uc5d0\\ub294 \\uc5f0\\ub2e8\\uc704 \\ubcf5\\ub9ac 2.00%, 10 \\ub144 \\ucd08\\uacfc\\uc5d0\\ub294 \\uc5f0\\ub2e8\\uc704 \\ubcf5\\ub9ac 1.25%\\ub85c \\ud569 \\ub2c8\\ub2e4. \\uacf5\\uc2dc\\uc774\\uc728\\uc758 \\uc801\\uc6a9\\uc740 \\ub9e4\\uc6d4 1 \\uc77c \\ud68c\\uc0ac\\uac00 \\uc815\\ud55c \\uacf5\\uc2dc\\uc774\\uc728\\ub85c \\ud558\\uba70, \\ub9e4\\uc6d4 1 \\uc77c\\ubd80\\ud130 \\ud574\\ub2f9\\uc6d4 \\ub9c8\\uc9c0\\ub9c9 \\ub0a0\\uae4c\\uc9c0 1 \\uac1c\\uc6d4\\uac04 \\ud655\\uc815 \\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4. \\ucd5c\\uc800\\ubcf4\\uc99d\\uc774\\uc728\\uc774\\ub780? \\ud68c\\uc0ac\\uc758 \\uc6b4\\uc6a9\\uc790\\uc0b0 \\uc774\\uc775\\ub960 \\ubc0f \\uc2dc\\uc911\\uae08\\ub9ac\\uac00 \\ud558\\ub77d\\ud558\\ub354\\ub77c\\ub3c4 \\ud68c\\uc0ac\\uc5d0\\uc11c \\uc9c0\\uae09\\uc744 \\ubcf4\\uc99d\\ud558\\ub294 \\ucd5c\\uc800\\ud55c\\ub3c4\\uc758 \\uc801\\uc6a9\\uc774\\uc728\\uc785\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ubcf4\\ud5d8\\ub8cc \\uc0b0\\ucd9c\\uae30\\ucd08 \\ubc0f \\uacf5\\uc2dc\\uc774\\uc728\", \"subsubtitle\": \"\\u25a1 \\uacc4\\uc57d\\uccb4\\uacb0\\ube44\\uc6a9 \\ubc0f \\uacc4\\uc57d\\uad00\\ub9ac\\ube44\\uc6a9\", \"Head\": \"\", \"text\": \"\\uacc4\\uc57d\\uccb4\\uacb0\\ube44\\uc6a9 \\ubc0f \\uacc4\\uc57d\\uad00\\ub9ac\\ube44\\uc6a9\\uc774\\ub780? \\ubcf4\\ud5d8\\ud68c\\uc0ac\\uac00 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc758 \\uccb4\\uacb0, \\uc720\\uc9c0 \\ubc0f \\uad00\\ub9ac \\ub4f1\\uc5d0 \\ud544\\uc694\\ud55c \\uacbd\\ube44\\ub85c \\uc0ac\\uc6a9\\ud558\\uae30 \\uc704\\ud558\\uc5ec \\ubcf4\\ud5d8\\ub8cc \\uc911 \\uc77c\\uc815\\ube44\\uc728\\uc744 \\ube44\\uc6a9\\uc73c\\ub85c \\ucc45\\uc815\\ud55c \\uac83\\uc744 \\ub9d0\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\uacc4\\uc57d\\uc790\\ubc30\\ub2f9\\uc5d0 \\uad00\\ud55c \\uc0ac\\ud56d\", \"subsubtitle\": \"\", \"Head\": \"\", \"text\": \"\\u25a1 \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc740 \\ubb34\\ubc30\\ub2f9 \\uc0c1\\ud488\\uc73c\\ub85c\\uc11c \\ubc30\\ub2f9\\uc744 \\ud558\\uc9c0 \\uc54a\\uc2b5\\ub2c8\\ub2e4. \\uadf8\\ub7ec\\ub098 \\ubb34\\ubc30\\ub2f9 \\uc0c1\\ud488 \\uc740 \\ubc30\\ub2f9 \\uc0c1\\ud488\\uc5d0 \\ube44\\ud574 \\ubcf4\\ud5d8\\ub8cc\\uac00 \\uc0c1\\ub300\\uc801\\uc73c\\ub85c \\uc800\\ub834\\ud558\\ub2e4\\ub294 \\ud2b9\\uc9d5\\uc774 \\uc788\\uc2b5 \\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc5d0 \\uad00\\ud55c \\uc0ac\\ud56d\", \"subsubtitle\": \"\\u25a1 \\ud574\\uc9c0\\ud658\\uae09\\uae08 \\uc0b0\\ucd9c\\uae30\\uc900\", \"Head\": \"\", \"text\": \"\\ud68c\\uc0ac\\ub294 \\uae08\\uc735\\uac10\\ub3c5\\uc6d0\\uc7a5\\uc774 \\uc778\\uac00\\ud55c \\uc0b0\\ucd9c\\uae30\\uc900\\uc5d0 \\ub530\\ub77c \\uacc4\\uc0b0\\ud55c \\uc774 \\ubcf4\\ud5d8\\uc758 \\ubcf4\\ud5d8\\ub8cc\\uc801\\ub9bd\\uae08(\\uc21c\\ubcf4\\ud5d8\\ub8cc\\uc2dd \\ucc45\\uc784\\uc900\\ube44\\uae08\\uc5d0\\uc11c \\ud574\\uc9c0\\uacf5\\uc81c\\uae08\\uc561\\uc744 \\ucc28\\uac10\\ud55c \\uae08 \\uc561)\\uacfc \\ubbf8\\uacbd\\uacfc\\ubcf4\\ud5d8\\ub8cc\\ub97c \\ub354\\ud55c \\uae08\\uc561\\uc744 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc73c\\ub85c \\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc8fc\\uc694\\ub0b4\\uc6a9\", \"subtitle\": \"\\u25cf \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc5d0 \\uad00\\ud55c \\uc0ac\\ud56d\", \"subsubtitle\": \"\\u25a1 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc774 \\uc801\\uc740 \\uc774\\uc720\", \"Head\": \"\", \"text\": \"\\uc190\\ud574\\ubcf4\\ud5d8\\uc0c1\\ud488\\uc740 \\uc740\\ud589\\uc758 \\uc800\\ucd95\\uacfc\\ub294 \\ub2ec\\ub9ac \\uc704\\ud5d8\\ubcf4\\uc7a5\\uacfc \\uc800\\ucd95\\uc744 \\uacb8\\ud55c \\uc81c\\ub3c4 \\ub85c\\uc11c \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc790\\uac00 \\ub0a9\\uc785\\ud55c \\ubcf4\\ud5d8\\ub8cc \\uc911 \\uc77c\\ubd80\\ub294 \\ubd88\\uc758\\uc758 \\uc0ac\\uace0\\ub97c \\ub2f9\\ud55c \\ub2e4 \\ub978 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c \\uc9c0\\uae09\\ub418\\ub294 \\ubcf4\\ud5d8\\uae08\\uc73c\\ub85c, \\ub610 \\ub2e4\\ub978 \\uc77c\\ubd80\\ub294 \\ubcf4\\ud5d8\\ud68c\\uc0ac \\uc6b4\\uc601\\uc5d0 \\ud544\\uc694\\ud55c \\uc0ac\\uc5c5\\uacbd\\ube44\\ub85c \\uc0ac\\uc6a9\\ub418\\ubbc0\\ub85c \\uc911\\ub3c4\\ud574\\uc9c0\\ud558\\ub294 \\uacbd\\uc6b0 \\uc9c0\\uae09\\ub418\\ub294 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc740 \\ub0a9\\uc785\\ud55c \\ubcf4\\ud5d8\\ub8cc\\ubcf4\\ub2e4 \\uc801\\uac70\\ub098 \\uc5c6\\uc744 \\uc218\\ub3c4 \\uc788\\uc2b5\\ub2c8\\ub2e4. \\uad6c\\uccb4\\uc801\\uc778 \\uc0c1\\ud488\\ub0b4\\uc6a9\\uc740 \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1)\\uc758 \\ubcf4\\ud5d8\\uc57d\\uad00\\uc744 \\ucc38\\uc870\\ud558\\uc2dc\\uae30 \\ubc14\\ub78d\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc57d\\uad00\\uc5d0\\uc11c \\uc778\\uc6a9\\ub41c \\ubc95\\ub839 \\ub0b4\\uc6a9\", \"subtitle\": \"\\u3010\\uc758\\ub8cc\\ubc95\\u3011\", \"subsubtitle\": \"\", \"Head\": \"\", \"text\": \"\\uc81c 3\\uc870 (\\uc758\\ub8cc\\uae30\\uad00) \\u2460 \\uc774 \\ubc95\\uc5d0\\uc11c \\\"\\uc758\\ub8cc\\uae30\\uad00\\\"\\uc774\\ub780 \\uc758\\ub8cc\\uc778\\uc774 \\uacf5\\uc911(\\u516c\\u8846) \\ub610\\ub294 \\ud2b9\\uc815 \\ub2e4\\uc218\\uc778\\uc744 \\uc704\\ud558\\uc5ec \\uc758\\ub8cc\\u00b7\\uc870\\uc0b0\\uc758 \\uc5c5(\\uc774\\ud558 \\\"\\uc758\\ub8cc\\uc5c5\\\"\\uc774\\ub77c \\ud55c\\ub2e4)\\uc744 \\ud558\\ub294 \\uacf3\\uc744 \\ub9d0\\ud55c\\ub2e4. \\u2461 \\uc758\\ub8cc\\uae30\\uad00\\uc740 \\ub2e4\\uc74c \\uac01 \\ud638\\uc640 \\uac19\\uc774 \\uad6c\\ubd84\\ud55c\\ub2e4. 1. \\uc758\\uc6d0\\uae09 \\uc758\\ub8cc\\uae30\\uad00: \\uc758\\uc0ac, \\uce58\\uacfc\\uc758\\uc0ac \\ub610\\ub294 \\ud55c\\uc758\\uc0ac\\uac00 \\uc8fc\\ub85c \\uc678\\ub798\\ud658\\uc790\\ub97c \\ub300\\uc0c1\\uc73c\\ub85c \\uac01\\uac01 \\uadf8 \\uc758\\ub8cc\\ud589\\uc704\\ub97c \\ud558\\ub294 \\uc758\\ub8cc\\uae30\\uad00\\uc73c\\ub85c\\uc11c \\uadf8 \\uc885\\ub958\\ub294 \\ub2e4\\uc74c \\uac01 \\ubaa9\\uacfc \\uac19\\ub2e4. \\uac00. \\uc758\\uc6d0 \\ub098. \\uce58\\uacfc\\uc758\\uc6d0 \\ub2e4. \\ud55c\\uc758\\uc6d0 2. \\uc870\\uc0b0\\uc6d0: \\uc870\\uc0b0\\uc0ac\\uac00 \\uc870\\uc0b0\\uacfc \\uc784\\ubd80\\u00b7\\ud574\\uc0b0\\ubd80\\u00b7\\uc0b0\\uc695\\ubd80 \\ubc0f \\uc2e0\\uc0dd\\uc544\\ub97c \\ub300\\uc0c1\\uc73c\\ub85c \\ubcf4\\uac74\\ud65c\\ub3d9\\uacfc \\uad50\\uc721\\u00b7\\uc0c1\\ub2f4\\uc744 \\ud558\\ub294 \\uc758\\ub8cc\\uae30\\uad00\\uc744 \\ub9d0\\ud55c\\ub2e4. 3. \\ubcd1\\uc6d0\\uae09 \\uc758\\ub8cc\\uae30\\uad00: \\uc758\\uc0ac, \\uce58\\uacfc\\uc758\\uc0ac \\ub610\\ub294 \\ud55c\\uc758\\uc0ac\\uac00 \\uc8fc\\ub85c \\uc785\\uc6d0\\ud658\\uc790\\ub97c \\ub300\\uc0c1\\uc73c\\ub85c \\uc758\\ub8cc\\ud589\\uc704\\ub97c \\ud558\\ub294 \\uc758\\ub8cc\\uae30\\uad00\\uc73c\\ub85c\\uc11c \\uadf8 \\uc885\\ub958\\ub294 \\ub2e4\\uc74c \\uac01 \\ubaa9\\uacfc \\uac19\\ub2e4. \\uac00. \\ubcd1\\uc6d0 \\ub098. \\uce58\\uacfc\\ubcd1\\uc6d0 \\ub2e4. \\ud55c\\ubc29\\ubcd1\\uc6d0 \\ub77c. \\uc694\\uc591\\ubcd1\\uc6d0(\\u300c\\uc815\\uc2e0\\ubcf4\\uac74\\ubc95\\u300d \\uc81c 3 \\uc870\\uc81c 3 \\ud638\\uc5d0 \\ub530\\ub978 \\uc815\\uc2e0\\uc758\\ub8cc\\uae30\\uad00 \\uc911 \\uc815\\uc2e0\\ubcd1\\uc6d0, \\u300c\\uc7a5\\uc560\\uc778\\ubcf5\\uc9c0\\ubc95\\u300d \\uc81c 58 \\uc870\\uc81c 1\\ud56d\\uc81c 2\\ud638\\uc5d0 \\ub530\\ub978 \\uc758\\ub8cc\\uc7ac\\ud65c\\uc2dc\\uc124\\ub85c\\uc11c \\uc81c 3\\uc870\\uc758 2\\uc758 \\uc694\\uac74\\uc744 \\uac16\\ucd98 \\uc758\\ub8cc\\uae30\\uad00\\uc744 \\ud3ec\\ud568\\ud55c\\ub2e4. \\uc774\\ud558 \\uac19\\ub2e4) \\ub9c8. \\uc885\\ud569\\ubcd1\\uc6d0 \\u2462 \\ubcf4\\uac74\\ubcf5\\uc9c0\\ubd80\\uc7a5\\uad00\\uc740 \\ubcf4\\uac74\\uc758\\ub8cc\\uc815\\ucc45\\uc5d0 \\ud544\\uc694\\ud558\\ub2e4\\uace0 \\uc778\\uc815\\ud558\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294 \\uc81c 2\\ud56d\\uc81c 1\\ud638\\ubd80\\ud130 \\uc81c 3\\ud638\\uae4c\\uc9c0\\uc758 \\uaddc\\uc815\\uc5d0 \\ub530\\ub978 \\uc758\\ub8cc\\uae30\\uad00\\uc758 \\uc885\\ub958\\ubcc4 \\ud45c\\uc900\\uc5c5\\ubb34\\ub97c \\uc815\\ud558\\uc5ec \\uace0\\uc2dc\\ud560 \\uc218 \\uc788\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc57d\\uad00\\uc5d0\\uc11c \\uc778\\uc6a9\\ub41c \\ubc95\\ub839 \\ub0b4\\uc6a9\", \"subtitle\": \"\\u3010\\uc0c1\\ubc95\\u3011\", \"subsubtitle\": \"\", \"Head\": \"\", \"text\": \"\\uc81c 651 \\uc870(\\uace0\\uc9c0\\uc758\\ubb34\\uc704\\ubc18\\uc73c\\ub85c \\uc778\\ud55c \\uacc4\\uc57d\\ud574\\uc9c0) \\ubcf4\\ud5d8\\uacc4\\uc57d\\ub2f9\\uc2dc\\uc5d0 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc790 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\uace0\\uc758 \\ub610\\ub294 \\uc911\\ub300\\ud55c \\uacfc\\uc2e4\\ub85c \\uc778\\ud558\\uc5ec \\uc911\\uc694\\ud55c \\uc0ac\\ud56d\\uc744 \\uace0\\uc9c0\\ud558\\uc9c0 \\uc544\\ub2c8\\ud558\\uac70\\ub098 \\ubd80\\uc2e4\\uc758 \\uace0\\uc9c0\\ub97c \\ud55c \\ub54c\\uc5d0\\ub294 \\ubcf4\\ud5d8\\uc790\\ub294 \\uadf8 \\uc0ac\\uc2e4\\uc744 \\uc548 \\ub0a0\\ub85c\\ubd80\\ud130 1 \\uc6d4\\ub0b4\\uc5d0, \\uacc4\\uc57d\\uc744 \\uccb4\\uacb0\\ud55c \\ub0a0\\ub85c\\ubd80\\ud130 3\\ub144\\ub0b4\\uc5d0 \\ud55c\\ud558\\uc5ec \\uacc4\\uc57d\\uc744 \\ud574\\uc9c0\\ud560 \\uc218 \\uc788\\ub2e4. \\uadf8\\ub7ec\\ub098 \\ubcf4\\ud5d8\\uc790\\uac00 \\uacc4\\uc57d\\ub2f9\\uc2dc\\uc5d0 \\uadf8 \\uc0ac\\uc2e4\\uc744 \\uc54c\\uc558\\uac70\\ub098 \\uc911\\ub300\\ud55c \\uacfc\\uc2e4\\ub85c \\uc778\\ud558\\uc5ec \\uc54c\\uc9c0 \\ubabb\\ud55c \\ub54c\\uc5d0\\ub294 \\uadf8\\ub7ec\\ud558\\uc9c0 \\uc544\\ub2c8\\ud558\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc57d\\uad00\\uc5d0\\uc11c \\uc778\\uc6a9\\ub41c \\ubc95\\ub839 \\ub0b4\\uc6a9\", \"subtitle\": \"\\u3010\\uc804\\uc790\\uc11c\\uba85\\ubc95\\u3011\", \"subsubtitle\": \"\", \"Head\": \"\", \"text\": \"\\uc81c 2\\uc870(\\uc815\\uc758) \\uc774 \\ubc95\\uc5d0\\uc11c \\uc0ac\\uc6a9\\ud558\\ub294 \\uc6a9\\uc5b4\\uc758 \\uc815\\uc758\\ub294 \\ub2e4\\uc74c\\uacfc \\uac19\\ub2e4. 1. (\\uc0dd\\ub7b5) 2. \\\"\\uc804\\uc790\\uc11c\\uba85\\\"\\uc774\\ub77c \\ud568\\uc740 \\uc11c\\uba85\\uc790\\ub97c \\ud655\\uc778\\ud558\\uace0 \\uc11c\\uba85\\uc790\\uac00 \\ub2f9\\ud574 \\uc804\\uc790\\ubb38\\uc11c\\uc5d0 \\uc11c\\uba85\\uc744 \\ud558\\uc600\\uc74c\\uc744 \\ub098\\ud0c0\\ub0b4\\ub294\\ub370 \\uc774\\uc6a9\\ud558\\uae30 \\uc704\\ud558\\uc5ec \\ub2f9\\ud574 \\uc804\\uc790\\ubb38\\uc11c\\uc5d0 \\ucca8\\ubd80\\ub418\\uac70\\ub098 \\ub17c\\ub9ac\\uc801\\uc73c\\ub85c \\uacb0\\ud569\\ub41c \\uc804\\uc790\\uc801 \\ud615\\ud0dc\\uc758 \\uc815\\ubcf4\\ub97c \\ub9d0\\ud55c\\ub2e4. 3. \\\"\\uacf5\\uc778\\uc804\\uc790\\uc11c\\uba85\\\"\\uc774\\ub77c \\ud568\\uc740 \\ub2e4\\uc74c \\uac01\\ubaa9\\uc758 \\uc694\\uac74\\uc744 \\uac16\\ucd94\\uace0 \\uacf5\\uc778\\uc778\\uc99d\\uc11c\\uc5d0 \\uae30\\ucd08\\ud55c \\uc804\\uc790\\uc11c\\uba85\\uc744 \\ub9d0\\ud55c\\ub2e4. \\uac00. \\uc804\\uc790\\uc11c\\uba85\\uc0dd\\uc131\\uc815\\ubcf4\\uac00 \\uac00\\uc785\\uc790\\uc5d0\\uac8c \\uc720\\uc77c\\ud558\\uac8c \\uc18d\\ud560 \\uac83 \\ub098. \\uc11c\\uba85 \\ub2f9\\uc2dc \\uac00\\uc785\\uc790\\uac00 \\uc804\\uc790\\uc11c\\uba85\\uc0dd\\uc131\\uc815\\ubcf4\\ub97c \\uc9c0\\ubc30\\u00b7\\uad00\\ub9ac\\ud558\\uace0 \\uc788\\uc744 \\uac83 \\ub2e4. \\uc804\\uc790\\uc11c\\uba85\\uc774 \\uc788\\uc740 \\ud6c4\\uc5d0 \\ub2f9\\ud574 \\uc804\\uc790\\uc11c\\uba85\\uc5d0 \\ub300\\ud55c \\ubcc0\\uacbd\\uc5ec\\ubd80\\ub97c \\ud655\\uc778\\ud560 \\uc218 \\uc788\\uc744 \\uac83 \\ub77c. \\uc804\\uc790\\uc11c\\uba85\\uc774 \\uc788\\uc740 \\ud6c4\\uc5d0 \\ub2f9\\ud574 \\uc804\\uc790\\ubb38\\uc11c\\uc758 \\ubcc0\\uacbd\\uc5ec\\ubd80\\ub97c \\ud655\\uc778\\ud560 \\uc218 \\uc788\\uc744 \\uac83 (\\uc774\\ud558 \\uc0dd\\ub7b5)\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uac1c\\uc778\\uc2e0\\uc6a9\\uc815\\ubcf4 \\uc81c\\uacf5\\u318d\\uc774\\uc6a9\\uc5d0 \\ub300\\ud55c \\uace0\\uac1d \\uad8c\\ub9ac \\uc548\\ub0b4\\ubb38\", \"subtitle\": \"1. \\uae08\\uc735\\uc11c\\ube44\\uc2a4 \\uc774\\uc6a9 \\ubc94\\uc704\", \"subsubtitle\": \"\\uac00. \\uace0\\uac1d\\uc758 \\uac1c\\uc778\\uc2e0\\uc6a9\\uc815\\ubcf4\\ub294 \\uae08\\uc735\\uac70\\ub798\\uc758 \\uc124\\uc815\\u00b7\\uc720\\uc9c0\\uc5ec\\ubd80 \\ud310\\ub2e8 \\ubaa9\\uc801 \\ubc0f \\uace0\\uac1d\\uc774\", \"Head\": \"\", \"text\": \"\\ub3d9\\uc758\\ud55c \\ubaa9\\uc801\\ub9cc\\uc73c\\ub85c\\uc774\\uc6a9\\ub429\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uac1c\\uc778\\uc2e0\\uc6a9\\uc815\\ubcf4 \\uc81c\\uacf5\\u318d\\uc774\\uc6a9\\uc5d0 \\ub300\\ud55c \\uace0\\uac1d \\uad8c\\ub9ac \\uc548\\ub0b4\\ubb38\", \"subtitle\": \"1. \\uae08\\uc735\\uc11c\\ube44\\uc2a4 \\uc774\\uc6a9 \\ubc94\\uc704\", \"subsubtitle\": \"\\ub098. \\uace0\\uac1d\\uc740 \\uc601\\uc5c5\\uc7a5\\u318d\\uc778\\ud130\\ub137 \\ub4f1 \\ub2e4\\uc591\\ud55c \\ucc44\\ub110\\uc744 \\ud1b5\\ud574 \\uae08\\uc735\\uac70\\ub798\\ub97c \\uccb4\\uacb0\\ud558\\uac70\\ub098\", \"Head\": \"\", \"text\": \"\\uae08\\uc735\\uc11c\\ube44\\uc2a4\\ub97c \\uc81c\\uacf5\\ubc1b\\ub294 \\uacfc\\uc815\\uc5d0\\uc11c 1) \\ud68c\\uc0ac\\uac00 \\ubcf8\\uc778\\uc758 \\uac1c\\uc778\\uc2e0\\uc6a9\\uc815\\ubcf4(\\uc774\\ud558 ' \\ubcf8\\uc778\\uc815\\ubcf4')\\ub97c \\uc81c\\ud734\\u00b7\\ubd80\\uac00\\uc11c\\ube44\\uc2a4 \\ub4f1\\uc744 \\uc704\\ud574 \\uc81c\\ud734\\ud68c\\uc0ac \\ub4f1\\uc5d0 \