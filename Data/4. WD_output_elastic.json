\\ud569\\ub2c8\\ub2e4. \\uc774\\ud558 \\u300c\\ubcf4\\ud5d8\\uacc4\\uc57d\\u300d\\uc774\\ub77c \\ud569\\ub2c8\\ub2e4)\\u300c\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc758 \\uc131\\ub9bd\\u300d\\uc758 \\uaddc\\uc815\\uc744\\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\uc790\\ub3d9\\ub0a9\\uc785\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c2\\uc870 (\\ubcf4\\ud5d8\\ub8cc\\uc758 \\uc601\\uc218)\", \"Head\": \"\", \"text\": \"\\uc790\\ub3d9\\ub0a9\\uc785\\uc77c\\uc790\\ub610\\ub294\\uae09\\uc5ec\\uc774\\uccb4\\uc77c\\uc790\\ub294\\uc774\\uccad\\uc57d\\uc11c\\uc5d0 \\uae30\\uc7ac \\ub41c\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\ud574\\ub2f9\\uc77c\\uc5d0\\ub3c4\\ubd88\\uad6c\\ud558\\uace0 \\ud68c\\uc0ac\\uc640\\uacc4\\uc57d\\uc790\\uac00 \\ubcc4\\ub3c4\\ub85c\\uc57d\\uc815\\ud55c\\uc77c\\uc790\\ub85c\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\uc790\\ub3d9\\ub0a9\\uc785\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c3\\uc870 (\\uacc4\\uc57d \\ud6c4 \\uc54c\\ub9b4 \\uc758\\ubb34)\", \"Head\": \"\", \"text\": \"\\uacc4\\uc57d\\uc790\\ub294\\uc9c0\\uc815\\uacc4\\uc88c\\uc758\\ubc88\\ud638\\uac00\\ubcc0\\uacbd\\ub610\\ub294\\uac70\\ub798\\uc815\\uc9c0\\ub41c\\uacbd \\uc6b0\\uc5d0\\ub294\\uadf8\\uc0ac\\uc2e4\\uc744\\uc989\\uc2dc\\ud68c\\uc0ac\\uc5d0\\uc54c\\ub824\\uc57c\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\uc790\\ub3d9\\ub0a9\\uc785\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c4\\uc870 (\\uc900\\uc6a9\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc11c\\uc815\\ud558\\uc9c0\\uc54a\\uc740\\uc0ac\\ud56d\\uc740\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc744\\ub530\\ub985 \\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c1\\uc870 (\\uc801\\uc6a9\\ubc94\\uc704)\", \"Head\": \"\", \"text\": \"\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc740\\ubcf4\\ud5d8\\uacc4\\uc57d(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\ubd80\\uac00\\ub41c\\uacbd\\uc6b0\\uc5d0\\ub294 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc744 \\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4. \\uc774\\ud558\\u300c\\ubcf4\\ud5d8\\uacc4\\uc57d\\u300d\\uc774\\ub77c \\ud569\\ub2c8 \\ub2e4)\\uc744 \\uccb4\\uacb0\\ud560\\ub54c\\ub2e4\\uc74c\\uc870\\uac74\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uacc4\\uc57d(\\uc774\\ud558\\u300c\\ub2e8 \\uccb4\\ucde8\\uae09\\uacc4\\uc57d\\u300d\\uc774\\ub77c\\ud569\\ub2c8\\ub2e4)\\uc5d0\\ub300\\ud558\\uc5ec\\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4. \\u2460 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc790(\\uc774\\ud558\\u300c\\uacc4\\uc57d\\uc790\\u300d\\ub77c\\ud569\\ub2c8\\ub2e4) \\ub610\\ub294\\ud53c\\ubcf4\\ud5d8 \\uc790\\ub294\\ub2e4\\uc74c\\uc911\\ud55c\\uac00\\uc9c0\\uc758\\ub2e8\\uccb4\\uc5d0\\uc18c\\uc18d\\ub418\\uc5b4\\uc57c\\ud569\\ub2c8\\ub2e4. 1.\\ub3d9\\uc77c\\ud55c\\ud68c\\uc0ac, \\uc0ac\\uc5c5\\uc7a5, \\uad00\\uacf5\\uc11c, \\uad6d\\uc601\\uae30\\uc5c5\\uccb4, \\uc870\\ud569 \\ub4f1 5\\uba85 \\uc774\\uc0c1\\uc758 \\uadfc\\ub85c\\uc790\\ub97c \\uace0\\uc6a9\\ud558\\uace0 \\uc788\\ub294 \\ub2e8\\uccb4. \\ub2e4 \\ub9cc, \\uc0ac\\uc5c5\\uc7a5, \\uc9c1\\uc81c, \\uc9c1\\uc885\\ub4f1\\uc73c\\ub85c\\uad6c\\ubd84\\ub418\\uc5b4\\uc788\\ub294\\uacbd \\uc6b0\\uc758 \\ub2e8\\uccb4\\uc18c\\uc18d \\uc5ec\\ubd80\\ub294 \\uad00\\ub828 \\ubc95\\ub839 \\ub4f1\\uc5d0\\uc11c \\uc815\\ud558\\ub294 \\ubc14\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. 2.\\ube44\\uc601\\ub9ac\\ubc95\\uc778\\ub2e8\\uccb4 \\ub610\\ub294 \\ubcc0\\ud638\\uc0ac\\ud68c, \\uc758\\uc0ac\\ud68c \\ub4f1 \\ub3d9\\uc5c5 \\uc790\\ub2e8\\uccb4\\ub85c\\uc11c 5\\uba85\\uc774\\uc0c1\\uc758\\uad6c\\uc131\\uc6d0\\uc774\\uc788\\ub294\\ub2e8\\uccb4 3.\\uadf8\\ubc16\\uc5d0\\ub2e8\\uccb4\\uc758\\uad6c\\uc131\\uc6d0\\uc774\\uba85\\ud655\\ud558\\uace0\\uc704\\ud5d8\\uc758\\ub3d9\\uc9c8\\uc131 \\uc774\\ud655\\ubcf4\\ub418\\uc5b4\\uacc4\\uc57d\\uc758\\uc77c\\uad04\\uc801\\uc778\\uad00\\ub9ac\\uac00\\uac00\\ub2a5\\ud55c\\ub2e8\\uccb4 \\ub85c\\uc11c 5\\uba85\\uc774\\uc0c1\\uc758\\uad6c\\uc131\\uc6d0\\uc774\\uc788\\ub294\\ub2e8\\uccb4 \\u2461 \\uacc4\\uc57d\\uc790\\ub294\\ub2e8\\uccb4\\ub610\\ub294\\ub2e8\\uccb4\\uc758\\ub300\\ud45c\\uc790\\ub0b4\\uc9c0\\ub2e8\\uccb4\\uc758\\uc18c\\uc18d \\uc6d0\\uc73c\\ub85c\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uacc4\\uc57d\\uc790\\uac00\\uc544\\ub2cc\\ub2e8\\uccb4\\uc758\\uc18c\\uc18d\\uc6d0 \\uc774 \\ubcf4\\ud5d8\\ub8cc \\uc804\\ubd80 \\ub610\\ub294 \\uc77c\\ubd80\\ub97c \\ubd80\\ub2f4\\ud558\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294 \\uadf8 \\uc18c\\uc18d\\uc6d0\\uc774\\uacc4\\uc57d\\uc790\\ub85c\\uc11c\\uc758\\uad8c\\ub9ac\\ub97c\\ud589\\uc0ac\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2462 \\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\uc801\\uc6a9\\uc744\\ubc1b\\uae30\\uc704\\ud574\\uc11c\\ub294\\ub2e8\\uccb4\\uc5d0\\uc18c\\uc18d\\ub41c \\ud53c\\ubcf4\\ud5d8\\uc790\\uc218\\uac00 \\ucd5c\\ucd08 \\uacc4\\uc57d\\uc2dc 5\\uba85 \\uc774\\uc0c1(\\uc774\\ud558\\u300c\\ud53c\\ubcf4\\ud5d8 \\uc790\\ub2e8\\uccb4\\u300d\\ub77c\\ud569\\ub2c8\\ub2e4)\\uc774\\uac70\\ub098\\ub2e8\\uccb4\\uc5d0\\uc18c\\uc18d\\ub41c\\uacc4\\uc57d\\uc790\\uc218 \\uac00\\ucd5c\\ucd08\\uacc4\\uc57d\\uc2dc 5\\uba85 \\uc774\\uc0c1(\\uc774\\ud558\\u300c\\uacc4\\uc57d\\uc790\\ub2e8\\uccb4\\u300d\\ub77c\\ud569 \\ub2c8\\ub2e4)\\uc774\\uc5b4\\uc57c\\ud569\\ub2c8\\ub2e4. \\ub610\\ud55c, \\ub2e8\\uccb4\\uc18c\\uc18d\\uc6d0\\uc758\\ubc30\\uc6b0\\uc790, \\uc790\\ub140\\ub610\\ub294\\ubd80\\ubaa8(\\ubc30\\uc6b0\\uc790\\uc758\\ubd80\\ubaa8\\ud3ec\\ud568)\\ub97c \\ud53c\\ubcf4\\ud5d8\\uc790\\ub85c \\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c2\\uc870 (\\ub300\\ud45c\\uc790\\uc758 \\uc120\\uc815)\", \"Head\": \"\", \"text\": \"\\ub2e8\\uccb4\\uc758\\ub300\\ud45c\\uc790\\ub610\\ub294\\uc9c1\\ucc45\\uc0c1\\ub300\\ud45c\\uc790\\ub97c\\ub300\\ub9ac\\ud560\\uc218\\uc788\\ub294\\uc790 \\ub610\\ub294\\uc81c1\\uc870(\\uc801\\uc6a9\\ubc94\\uc704) \\uc81c2\\ud56d\\uc5d0\\uc11c \\uc815\\ud55c\\uacc4\\uc57d\\uc790\\uc911\\uc5d0\\uc11c \\ub300\\ud45c\\uc790\\ub97c\\uc120\\uc815\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c3\\uc870 (\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc99d\\uac00, \\uac10\\uc18c \\ub610\\ub294 \\uad50\\uccb4)\", \"Head\": \"\", \"text\": \"\\u2460 \\ub2e8\\uccb4\\ucde8\\uae09\\uacc4\\uc57d\\uc744\\ub9fa\\uc740 \\ud6c4\\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c\\uc99d\\uac00, \\uac10\\uc18c \\ub610 \\ub294\\uad50\\uccb4\\ud558\\uace0\\uc790\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uacc4\\uc57d\\uc790\\ub098\\ud53c\\ubcf4\\ud5d8\\uc790\\ub610 \\ub294\\uc81c2\\uc870(\\ub300\\ud45c\\uc790\\uc758\\uc120\\uc815)\\uc5d0\\uc11c\\uc815\\ud55c\\ub300\\ud45c\\uc790\\ub294\\uc9c0\\uccb4\\uc5c6 \\uc774\\uc11c\\uba74\\uc73c\\ub85c\\uadf8\\uc0ac\\uc2e4\\uc744\\ubcf4\\ud5d8\\ud68c\\uc0ac(\\uc774\\ud558\\u300c\\ud68c\\uc0ac\\u300d\\ub77c\\ud569 \\ub2c8\\ub2e4)\\uc5d0\\uc54c\\ub9ac\\uace0\\ud68c\\uc0ac\\uc758\\uc2b9\\uc778\\uc744\\ubc1b\\uc544\\uc57c\\ud569\\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\uc758\\ubcf4\\uc7a5\\uc740\\ud68c\\uc0ac\\uac00\\uc2b9\\uc778\\ud55c\\uc774\\ud6c4\\ubd80\\ud130\\uc2dc\\uc791\\ub418\\uba70\\ud68c \\uc0ac\\uac00\\uc2b9\\uc778\\uc744\\uac70\\uc808\\ud560\\uc0ac\\uc720\\uac00\\uc5c6\\ub294\\ud55c\\uc81c1\\ud56d\\uc758\\uc11c\\uba74\\uc774 \\ud68c\\uc0ac\\uc5d0\\uc811\\uc218\\ub41c\\ub54c\\ub97c\\uc2b9\\uc778\\ud55c\\ub54c\\ub85c\\ubd05\\ub2c8\\ub2e4. 1.\\ud53c\\ubcf4\\ud5d8\\uc790\\ub2e8\\uccb4\\uc5d0 \\ub300\\ud55c \\ub2e8\\uccb4\\ucde8\\uae09\\uacc4\\uc57d\\uc740 \\ubcf4\\ud5d8\\uae30\\uac04 \\uc911\\ud53c\\ubcf4\\ud5d8\\uc790\\uac10\\uc18c\\uc2dc\\uc5d0\\ud574\\ub2f9\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uacc4\\uc57d\\uc744\\ud574 \\uc9c0\\ub41c\\uac83\\uc73c\\ub85c\\ud558\\uba70, \\uc0c8\\ub85c\\uc774\\uc99d\\uac00\\ub610\\ub294\\uad50\\uccb4\\ub418\\ub294\\ud53c \\ubcf4\\ud5d8\\uc790\\uc758\\ubcf4\\ud5d8\\uae30\\uac04\\uc740\\uc774\\uacc4\\uc57d\\uc758\\ub0a8\\uc740\\ubcf4\\ud5d8\\uae30\\uac04\\uc73c \\ub85c\\ud558\\uace0, \\uc774\\ub85c\\uc778\\ud558\\uc5ec\\ubc1c\\uc0dd\\ub418\\ub294\\ubcc0\\uacbd\\ub41c\\ubcf4\\ud5d8\\ub8cc\\ub97c \\ubc1b\\uace0, \\ucd94\\uac00 \\ub610\\ub294 \\ud658\\uae09\\ub418\\ub294 \\ucc45\\uc784\\uc900\\ube44\\uae08\\uc740 \\ubc1b\\uac70\\ub098 \\ub3cc\\ub824\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ud53c\\ubcf4\\ud5d8\\uc790\\uc99d\\uac00\\ub098\\uad50\\uccb4\\uc2dc\\uc5d0 \\ud68c\\uc0ac\\uac00\\ubc1b\\uc544\\uc57c\\ud560\\ucc45\\uc784\\uc900\\ube44\\uae08\\ucc28\\uc561\\uc774\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0 \\ud68c\\uc0ac\\uc758\\ubcf4\\uc7a5\\uc740\\ucc45\\uc784\\uc900\\ube44\\uae08\\uc744\\uc815\\uc0b0\\ud55c\\ud6c4\\ubcc0\\uacbd\\ub41c\\ubcf4 \\ud5d8\\ub8cc\\ub97c\\ub0a9\\uc785\\ud558\\ub294\\ub0a0\\ubd80\\ud130\\uc2dc\\uc791\\ud569\\ub2c8\\ub2e4. 2.\\ud53c\\ubcf4\\ud5d8\\uc790\\ub2e8\\uccb4\\uc5d0 \\ub300\\ud55c \\ub2e8\\uccb4\\ucde8\\uae09\\uacc4\\uc57d\\uc5d0\\uc11c \\ud53c\\ubcf4\\ud5d8 \\uc790\\uac00 \\uc99d\\uac00 \\ub610\\ub294 \\uad50\\uccb4\\ub420 \\uacbd\\uc6b0\\uc5d0 \\uc554\\uacfc \\uac19\\uc774 \\ud68c\\uc0ac\\uac00 \\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\ub294\\uae30\\uac04\\uc774\\uc788\\ub294\\ubcf4\\uc7a5\\uc5d0\\uc788\\uc5b4 \\uc11c\\ub294,\\ud53c\\ubcf4\\ud5d8\\uc790\\uc99d\\uac00\\uc2dc\\uc5d0\\ub294\\ucc45\\uc784\\uc900\\ube44\\uae08\\uc744\\uc815\\uc0b0\\ud55c \\ud6c4\\ubcc0\\uacbd\\ub41c\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub0a9\\uc785\\ud55c\\ub0a0\\ubd80\\ud130\\ud68c\\uc0ac\\uac00\\ubcf4\\ud5d8\\uae08 \\uc744\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\ub294\\uae30\\uac04\\uc774\\uc801\\uc6a9\\ub418\\uba70, \\ud53c\\ubcf4\\ud5d8\\uc790\\uad50 \\uccb4\\uc2dc\\uc5d0\\ub294\\ud68c\\uc0ac\\uc758\\uc2b9\\uc778\\uc77c\\ubd80\\ud130\\ud68c\\uc0ac\\uac00\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0 \\uae09\\ud558\\uc9c0\\uc54a\\ub294\\uae30\\uac04\\uc774\\uc801\\uc6a9\\ub429\\ub2c8\\ub2e4. 3.\\uacc4\\uc57d\\uc790\\ub2e8\\uccb4\\uc5d0\\ub300\\ud55c\\ub2e8\\uccb4\\ucde8\\uae09\\uacc4\\uc57d\\uc740\\ubcf4\\ud5d8\\uae30\\uac04\\uc911 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc218\\uc758\\uac10\\uc18c\\uc2dc\\uc5d0\\ud574\\ub2f9\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uacc4\\uc57d\\uc744 \\uac1c\\ubcc4\\uacc4\\uc57d\\uc73c\\ub85c\\uc804\\ud658\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\u2462 \\uc81c1\\ud56d\\uc744 \\uc704\\ubc18\\ud558\\uc600\\uc744 \\uacbd\\uc6b0\\uc5d0\\ub294 \\ud68c\\uc0ac\\ub294 \\uc0c8\\ub85c\\uc774 \\uc99d\\uac00 \\ub610\\ub294\\uad50\\uccb4\\ub418\\ub294\\ud574\\ub2f9\\ud53c\\ubcf4\\ud5d8\\uc790\\uc5d0\\ub300\\ud558\\uc5ec\\ub294\\ubcf4\\uc0c1\\ud558\\uc9c0 \\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2463 \\uc81c1\\ud56d\\uc5d0\\ub530\\ub77c\\uc0c8\\ub85c\\uc774\\uad50\\uccb4\\ub418\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790 \\ubcc0\\uacbd\\uc804\\uacc4\\uc57d\\uacfc\\ub3d9\\uc77c\\ud55c\\ubcf4\\uc7a5\\uc870\\uac74\\ubc0f\\uc778\\uc218\\uae30\\uc900\\uc5d0\\ub530\\ub77c \\uac00\\uc785\\ub420\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c4\\uc870 (\\uc801\\uc6a9\\ubcf4\\ud5d8\\ub8cc)\", \"Head\": \"\", \"text\": \"\\u2460 \\uacc4\\uc57d\\uc790\\uc218 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc218\\uac00 5\\uba85 \\uc774\\uc0c1\\uc778 \\uacbd\\uc6b0\\uc5d0\\ub294 \\ub2e8\\uccb4\\ucde8\\uae09\\ubcf4\\ud5d8\\ub8cc\\ub97c\\uc801\\uc6a9\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2461 \\ubcf4\\ud5d8\\uae30\\uac04\\uc911\\ud53c\\ubcf4\\ud5d8\\uc790\\uc218\\uac00\\uac10\\uc18c\\ud558\\uc5ec 5\\uba85 \\ubbf8\\ub9cc\\uc774\\ub41c \\ub54c\\uc5d0\\ub294\\uc81c1\\ud56d\\uc744\\uc801\\uc6a9\\ud558\\uc9c0\\uc54a\\uc73c\\uba70, \\uc774\\ud6c4\\ud53c\\ubcf4\\ud5d8\\uc790\\uc218 \\uac00\\uc99d\\uac00\\ud558\\uc5ec 5\\uba85 \\uc774\\uc0c1\\uc774\\ub41c\\ub54c\\uc5d0\\ub294\\ub2e4\\uc2dc\\uc81c1\\ud56d\\uc744 \\uc801 \\uc6a9\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c5\\uc870 (\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785)\", \"Head\": \"\", \"text\": \"\\u2460 \\ubcf4\\ud5d8\\ub8cc\\ub294\\ub2e8\\uccb4\\ub610\\ub294\\ub2e8\\uccb4\\uc758\\ub300\\ud45c\\uc790\\uc640\\ud68c\\uc0ac\\uac00\\uc815\\ud55c\\ub0a0 \\uc5d0\\ub300\\ud45c\\uc790\\uac00\\uacc4\\uc57d\\uc790\\ub97c\\ub300\\ub9ac\\ud558\\uc5ec\\ubcf4\\ud5d8\\ub8cc\\ub97c\\uc77c\\uad04\\ub0a9\\uc785 \\ud558\\uc5ec\\uc57c\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uae09\\uc5ec\\uc774\\uccb4\\ubc0f\\uc790\\ub3d9\\uc774\\uccb4\\ub85c\\ubcf4\\ud5d8 \\ub8cc\\ub97c\\ub0a9\\uc785\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc77c\\uad04\\ub0a9\\uc785\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\ub294\\ub0a9\\uc785\\ubcf4\\ud5d8\\ub8cc\\uc5d0\\ub300\\ud55c\\uc601\\uc218\\uc99d\\uc744\\ub300\\ud45c\\uc790\\uc5d0\\uac8c\\ub4dc \\ub9bd\\ub2c8\\ub2e4.\\ub2e4\\ub9cc, \\ub2e8\\uccb4\\ub610\\ub294\\ub2e8\\uccb4\\uc758\\ub300\\ud45c\\uc790\\uc758\\uc694\\uad6c\\uac00\\uc788 \\uc744\\uacbd\\uc6b0\\uc774\\uac70\\ub098\\uacc4\\uc57d\\uc790\\ub2e8\\uccb4\\uc778\\uacbd\\uc6b0\\uc5d0\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\ubcc4 \\ub85c\\ub0a9\\uc785\\uc99d\\uba85\\uc11c\\ub97c\\ubc1c\\ud589\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c6\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uc18c\\uba78)\", \"Head\": \"\", \"text\": \"\\u2460 \\ub2e4\\uc74c\\uc911 \\ud55c\\uac00\\uc9c0\\uc758\\uacbd\\uc6b0\\uc5d0\\ud574\\ub2f9\\ub418\\ub294 \\ub54c\\uc5d0\\ub294 \\uc774\\ud2b9\\ubcc4 \\uc57d\\uad00\\uc740\\ud574\\ub2f9\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uc5d0\\ub300\\ud558\\uc5ec\\ub354\\uc774\\uc0c1 \\ud6a8\\ub825\\uc774\\uc5c6\\uc2b5\\ub2c8\\ub2e4. 1.\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uc18c\\uc18d\\ub2e8\\uccb4\\ub97c\\uc774\\ud0c8\\ud558\\uc600\\uc744 \\ub54c 2.\\ubcf4\\ud5d8\\ub8cc\\ub97c\\uc77c\\uad04\\ud558\\uc5ec\\ub0a9\\uc785\\ud558\\uc9c0\\uc54a\\uc558\\uc744\\ub54c \\u2461 \\uc81c1\\ud56d\\uc758\\uaddc\\uc815\\uc5d0\\uc758\\ud558\\uc5ec\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\ub354\\uc774\\uc0c1\\ud6a8\\ub825 \\uc774\\uc5c6\\uac8c\\ub41c\\uacbd\\uc6b0\\uc5d0\\ub294\\ucc28\\ud68c\\uc774\\ud6c4\\uc758\\ubcf4\\ud5d8\\ub8cc\\ub294\\ub2e8\\uccb4\\ucde8\\uae09 \\ubcf4\\ud5d8\\ub8cc\\ub97c\\uc801\\uc6a9\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c7\\uc870 (\\uc801\\uc6a9\\ud2b9\\uce59)\", \"Head\": \"\", \"text\": \"\\u2460 \\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc11c\\ub2e8\\uccb4\\uac00\\uaddc\\uc57d\\uc5d0\\ub530\\ub77c\\uad6c\\uc131\\uc6d0\\uc758\\uc804\\ubd80 \\ub610\\ub294\\uc77c\\ubd80\\ub97c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub85c\\ud558\\ub294\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc744\\uccb4\\uacb0\\ud558\\ub294 \\uacbd\\uc6b0\\ud0c0\\uc778\\uc758\\uc0ac\\ub9dd\\uc744\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ub85c\\ud558\\ub294\\ubcf4\\ud5d8\\uacc4 \\uc57d\\uc5d0\\uc11c\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc744\\uccb4\\uacb0\\ud560\\ub54c\\uae4c\\uc9c0\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uc11c\\uba74 \\uc5d0 \\uc758\\ud55c \\ub3d9\\uc758\\ub97c \\uc5bb\\uc9c0 \\uc54a\\uc740 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc744 \\ubb34\\ud6a8\\ub85c \\ud558\\uc9c0 \\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\ud56d\\uc758\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc774\\uccb4\\uacb0\\ub41c\\ub54c\\uc5d0\\ub294\\ud68c\\uc0ac\\ub294\\uacc4\\uc57d\\uc790\\uc5d0 \\uac8c\\ub9cc\\ubcf4\\ud5d8\\uc99d\\uad8c\\uc744\\ubc1c\\ud589\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uacc4\\uc57d\\uc790 \\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uc694\\uccad\\uc774\\uc788\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\ubcc4 \\ub85c\\ubcf4\\ud5d8\\uc99d\\uad8c\\uc744\\ubc1c\\ud589\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\u2462 \\uc81c1\\ud56d\\uc758 \\ubcf4\\ud5d8\\uacc4\\uc57d\\uc5d0\\uc11c \\uacc4\\uc57d\\uc790\\uac00 \\ud53c\\ubcf4\\ud5d8\\uc790 \\ub610\\ub294 \\uadf8 \\uc0c1\\uc18d\\uc778\\uc774\\uc544\\ub2cc\\uc790\\ub97c\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\ub85c\\uc9c0\\uc815\\ud560\\ub54c\\uc5d0\\ub294\\ub2e8 \\uccb4\\uc758\\uaddc\\uc57d\\uc5d0\\uc11c\\uba85\\uc2dc\\uc801\\uc73c\\ub85c\\uc815\\ud558\\ub294\\uacbd\\uc6b0\\uac00\\uc544\\ub2c8\\uba74\\uadf8 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uc11c\\uba74\\ub3d9\\uc758\\ub97c\\ubc1b\\uc544\\uc57c\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"3. \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"\", \"subsubtitle\": \"\\uc81c8\\uc870 (\\uc900\\uc6a9\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc11c\\uc815\\ud558\\uc9c0\\uc54a\\uc740\\uc0ac\\ud56d\\uc740\\ubcf4\\ud5d8\\uacc4\\uc57d\\uc744\\ub530\\ub985 \\ub2c8\\ub2e4.\"}\n"