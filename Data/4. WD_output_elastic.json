bcd1\\uc6d0\\uacfc\\ubcd1\\uc6d0\\uc5d0 \\uc11c\\uc9c1\\uc7a5\\ub610\\ub294\\uac1c\\uc778\\uc774\\uc2e4\\uc2dc\\ud55c\\uac74\\uac15\\uc9c4\\ub2e8\\uc11c\\uc0ac\\ubcf8\\ub4f1\\uac74\\uac15\\uc0c1 \\ud0dc\\ub97c\\ud310\\ub2e8\\ud560\\uc218\\uc788\\ub294\\uc790\\ub8cc\\ub85c\\uac74\\uac15\\uc9c4\\ub2e8\\uc744\\ub300\\uc2e0\\ud560\\uc218\\uc788\\uc2b5 \\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c12\\uc870 (\\uc0c1\\ud574\\ubcf4\\ud5d8\\uacc4\\uc57d \\ud6c4 \\uc54c\\ub9b4 \\uc758\\ubb34)\", \"Head\": \"\", \"text\": \"\\u2460 \\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\ub294\\ubcf4\\ud5d8\\uae30\\uac04\\uc911\\uc5d0\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\uadf8\\uc9c1\\uc5c5 \\ub610\\ub294 \\uc9c1\\ubb34\\ub97c\\ubcc0\\uacbd(\\uc790\\uac00\\uc6a9 \\uc6b4\\uc804\\uc790\\uac00\\uc601\\uc5c5\\uc6a9 \\uc6b4\\uc804\\uc790\\ub85c\\uc9c1\\uc5c5\\ub610\\ub294\\uc9c1\\ubb34\\ub97c\\ubcc0\\uacbd\\ud558\\ub294\\ub4f1\\uc758\\uacbd\\uc6b0\\ub97c\\ud3ec \\ud568\\ud569\\ub2c8\\ub2e4)\\ud558\\uac70\\ub098\\uc774\\ub95c\\uc790\\ub3d9\\ucc28\\ub610\\ub294\\uc6d0\\ub3d9\\uae30\\uc7a5\\uce58\\uc790\\uc804 \\uac70\\ub97c\\uacc4\\uc18d\\uc801\\uc73c\\ub85c\\uc0ac\\uc6a9\\ud558\\uac8c\\ub41c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc9c0\\uccb4\\uc5c6\\uc774\\ud68c \\uc0ac\\uc5d0\\uc54c\\ub824\\uc57c\\ud569\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\ud56d\\uc758\\ud1b5\\uc9c0\\uc5d0\\ub530\\ub77c\\uc704\\ud5d8\\uc774\\uc99d\\uac00\\ub610\\ub294\\uac10\\uc18c\\ud558\\ub294\\uacbd \\uc6b0\\ubcf4\\ud5d8\\ub8cc\\uac00\\ubcc0\\uacbd\\ub420\\uc218\\uc788\\uc73c\\uba70, \\uc794\\uc5ec\\uae30\\uac04\\ubcf4\\uc7a5\\uc744\\uc704 \\ud55c\\uc7ac\\uc6d0\\uc778\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc758\\uc815\\uc0b0\\uc73c\\ub85c\\uacc4\\uc57d\\uc790\\uac00\\ucd94\\uac00\\ub85c \\ub0a9\\uc785\\ud558\\uac70\\ub098\\ub3cc\\ub824\\ubc1b\\uc744\\uae08\\uc561\\uc774\\ubc1c\\uc0dd\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2462 \\ud68c\\uc0ac\\ub294\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uace0\\uc758\\ub610\\ub294\\uc911\\ub300\\ud55c\\uacfc \\uc2e4\\ub85c\\uc704\\ud5d8\\uc774\\uc99d\\uac00\\ub41c\\uacbd\\uc6b0\\uc5d0\\ub294\\ud1b5\\uc9c0\\ub97c\\ubc1b\\uc740\\ub0a0\\ubd80\\ud130 1 \\uac1c\\uc6d4\\uc774\\ub0b4\\uc5d0\\ubcf4\\ud5d8\\ub8cc\\uc758\\uc99d\\uc561\\uc744\\uccad\\uad6c\\ud558\\uac70\\ub098\\uacc4\\uc57d\\uc744\\ud574 \\uc9c0\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2463 \\uc81c2\\ud56d\\uc758\\ubcf4\\ud5d8\\ub8cc\\ubcc0\\uacbd\\uc73c\\ub85c\\uc778\\ud574\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub354\\ub0b4\\uc57c\\ud560 \\uacbd\\uc6b0\\ud68c\\uc0ac\\uc758\\uccad\\uad6c\\uc5d0\\ub300\\ud574\\uacc4\\uc57d\\uc790\\uac00\\uadf8\\ub0a9\\uc785\\uc744\\uac8c\\uc744\\ub9ac \\ud588\\uc744\\ub54c, \\ud68c\\uc0ac\\ub294\\uc9c1\\uc5c5\\ub610\\ub294\\uc9c1\\ubb34\\uac00\\ubcc0\\uacbd\\ub418\\uae30\\uc804\\uc5d0\\uc801 \\uc6a9\\ub41c\\ubcf4\\ud5d8\\uc694\\uc728(\\uc774\\ud558\\u300c\\ubcc0\\uacbd\\uc804\\uc694\\uc728\\u300d\\uc774\\ub77c\\ud569\\ub2c8\\ub2e4)\\uc758 \\uc9c1\\uc5c5\\ub610\\ub294\\uc9c1\\ubb34\\uac00\\ubcc0\\uacbd\\ub41c\\ud6c4\\uc5d0\\uc801\\uc6a9\\ud574\\uc57c\\ud560\\ubcf4\\ud5d8\\uc694\\uc728 (\\uc774\\ud558\\u300c\\ubcc0\\uacbd\\ud6c4 \\uc694\\uc728\\u300d\\uc774\\ub77c \\ud569\\ub2c8\\ub2e4)\\uc5d0 \\ub300\\ud55c \\ube44\\uc728\\uc5d0 \\ub530\\ub77c\\ubcf4\\ud5d8\\uae08\\uc744\\uc0ad\\uac10\\ud558\\uc5ec\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ubcc0\\uacbd\\ub41c \\uc9c1\\uc5c5\\ub610\\ub294\\uc9c1\\ubb34\\uc640\\uad00\\uacc4\\uc5c6\\uc774\\ubc1c\\uc0dd\\ud55c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720 \\uc5d0\\uad00\\ud574\\uc11c\\ub294\\uc6d0\\ub798\\ub300\\ub85c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2464 \\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uace0\\uc758\\ub610\\ub294\\uc911\\ub300\\ud55c\\uacfc\\uc2e4\\ub85c\\uc9c1 \\uc5c5\\ub610\\ub294\\uc9c1\\ubb34\\uc758\\ubcc0\\uacbd\\uc0ac\\uc2e4\\uc744\\ud68c\\uc0ac\\uc5d0\\uc54c\\ub9ac\\uc9c0\\uc54a\\uc558\\uc744\\uacbd \\uc6b0\\ubcc0\\uacbd\\ud6c4\\uc694\\uc728\\uc774\\ubcc0\\uacbd\\uc804\\uc694\\uc728\\ubcf4\\ub2e4\\ub192\\uc744\\ub54c\\uc5d0\\ub294\\ud68c\\uc0ac \\ub294\\ub3d9\\uc0ac\\uc2e4\\uc744\\uc548\\ub0a0\\ubd80\\ud130 1\\uac1c\\uc6d4\\uc774\\ub0b4\\uc5d0\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c \\ubcf4\\ud5d8\\uc790\\uc5d0\\uac8c \\uc81c4\\ud56d\\uc5d0 \\uc758\\ud574 \\ubcf4\\uc7a5\\ub428\\uc744 \\ud1b5\\ubcf4\\ud558\\uace0 \\uc774\\uc5d0 \\ub530\\ub77c\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c13\\uc870 (\\uc54c\\ub9b4 \\uc758\\ubb34 \\uc704\\ubc18\\uc758 \\ud6a8\\uacfc)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294\\uc544\\ub798\\uc640\\uac19\\uc740\\uc0ac\\uc2e4\\uc774\\uc788\\uc744\\uacbd\\uc6b0\\uc5d0\\ub294\\uc190\\ud574\\uc758\\ubc1c \\uc0dd\\uc5ec\\ubd80\\uc5d0\\uad00\\uacc4\\uc5c6\\uc774\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud560\\uc218\\uc788\\uc2b5\\ub2c8 \\ub2e4. 1.\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uace0\\uc758\\ub610\\ub294\\uc911\\ub300\\ud55c\\uacfc\\uc2e4\\ub85c \\uc81c11\\uc870(\\uacc4\\uc57d\\uc804\\uc54c\\ub9b4\\uc758\\ubb34)\\ub97c\\uc704\\ubc18\\ud558\\uace0\\uadf8\\uc758\\ubb34\\uac00 \\uc911\\uc694\\ud55c\\uc0ac\\ud56d\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uacbd\\uc6b0 2.\\ub69c\\ub837\\ud55c\\uc704\\ud5d8\\uc758\\uc99d\\uac00\\uc640\\uad00\\ub828\\ub41c\\uc81c12\\uc870(\\uc0c1\\ud574\\ubcf4\\ud5d8\\uacc4 \\uc57d\\ud6c4\\uc54c\\ub9b4\\uc758\\ubb34) \\uc81c1\\ud56d\\uc5d0\\uc11c\\uc815\\ud55c\\uacc4\\uc57d\\ud6c4\\uc54c\\ub9b4\\uc758 \\ubb34\\ub97c \\uacc4\\uc57d\\uc790 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uace0\\uc758 \\ub610\\ub294 \\uc911\\ub300\\ud55c \\uacfc\\uc2e4\\ub85c\\uc774\\ud589\\ud558\\uc9c0\\uc54a\\uc558\\uc744\\ub54c \\u2461 \\uc81c1\\ud56d\\uc81c1\\ud638\\uc758\\uacbd\\uc6b0\\uc5d0\\ub3c4\\ubd88\\uad6c\\ud558\\uace0\\ub2e4\\uc74c\\uc911\\ud558\\ub098\\uc5d0\\ud574 \\ub2f9\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ud68c\\uc0ac\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud560\\uc218\\uc5c6 \\uc2b5\\ub2c8\\ub2e4. 1.\\ud68c\\uc0ac\\uac00\\ucd5c\\ucd08\\uacc4\\uc57d\\uccb4\\uacb0\\ub2f9\\uc2dc\\uc5d0\\uadf8\\uc0ac\\uc2e4\\uc744\\uc54c\\uc558\\uac70\\ub098 \\uacfc\\uc2e4\\ub85c\\uc778\\ud558\\uc5ec\\uc54c\\uc9c0\\ubabb\\ud558\\uc600\\uc744\\ub54c 2.\\ud68c\\uc0ac\\uac00\\uadf8\\uc0ac\\uc2e4\\uc744\\uc548\\ub0a0\\ubd80\\ud130 1\\uac1c\\uc6d4\\uc774\\uc0c1\\uc9c0\\ub0ac\\uac70\\ub098 \\ub610\\ub294\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\ub54c\\ubd80\\ud130\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac \\uc720\\uac00\\ubc1c\\uc0dd\\ud558\\uc9c0\\uc54a\\uace0 2\\ub144(\\uc9c4\\ub2e8\\uacc4\\uc57d\\uc758\\uacbd\\uc6b0\\uc9c8\\ubcd1\\uc5d0 \\ub300\\ud558\\uc5ec\\ub294 1\\ub144)\\uc774\\uc9c0\\ub0ac\\uc744\\ub54c 3.\\ucd5c\\ucd08\\uacc4\\uc57d\\uc744 \\uccb4\\uacb0\\ud55c \\ub0a0(\\uac31\\uc2e0\\ud615 \\uacc4\\uc57d\\uc758 \\uacbd\\uc6b0 \\ucd5c\\ucd08 \\uacc4\\uc57d\\ud574\\ub2f9\\uc77c\\uc744\\ub9d0\\ud569\\ub2c8\\ub2e4)\\ubd80\\ud130 3\\ub144\\uc774\\uc9c0\\ub0ac\\uc744\\ub54c 4.\\ud68c\\uc0ac\\uac00\\uc774\\uacc4\\uc57d\\uc744\\uccad\\uc57d\\ud560\\ub54c\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\uac74\\uac15\\uc0c1\\ud0dc \\ub97c\\ud310\\ub2e8\\ud560\\uc218\\uc788\\ub294\\uae30\\ucd08\\uc790\\ub8cc(\\uac74\\uac15\\uc9c4\\ub2e8\\uc11c\\uc0ac\\ubcf8\\ub4f1) \\uc5d0 \\ub530\\ub77c \\uc2b9\\ub099\\ud55c \\uacbd\\uc6b0\\uc5d0 \\uac74\\uac15\\uc9c4\\ub2e8\\uc11c \\uc0ac\\ubcf8 \\ub4f1\\uc5d0 \\uba85 \\uae30\\ub418\\uc5b4\\uc788\\ub294\\uc0ac\\ud56d\\uc73c\\ub85c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud558 \\uc600\\uc744 \\ub54c(\\uacc4\\uc57d\\uc790 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\ud68c\\uc0ac\\uc5d0 \\uc81c\\ucd9c\\ud55c \\uae30\\ucd08\\uc790\\ub8cc\\uc758\\ub0b4\\uc6a9\\uc911\\uc911\\uc694\\uc0ac\\ud56d\\uc744\\uace0\\uc758\\ub85c\\uc0ac\\uc2e4\\uacfc\\ub2e4 \\ub974\\uac8c \\uc791\\uc131\\ud55c \\ub54c\\uc5d0\\ub294 \\uc774 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc744 \\ud574\\uc9c0\\ud560 \\uc218 \\uc788 \\uc2b5\\ub2c8\\ub2e4) 5.\\ubcf4\\ud5d8\\uc124\\uacc4\\uc0ac\\ub4f1\\uc774\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uc5d0\\uac8c\\uc54c\\ub9b4 \\uae30\\ud68c\\ub97c\\uc8fc\\uc9c0\\uc54a\\uc558\\uac70\\ub098\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uc0ac \\uc2e4\\ub300\\ub85c\\uc54c\\ub9ac\\ub294\\uac83\\uc744\\ubc29\\ud574\\ud55c\\uacbd\\uc6b0, \\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c \\ubcf4\\ud5d8\\uc790\\uc5d0\\uac8c\\uc0ac\\uc2e4\\ub300\\ub85c\\uc54c\\ub9ac\\uc9c0\\uc54a\\uac8c\\ud558\\uc600\\uac70\\ub098\\ubd80\\uc2e4 \\ud55c \\uc0ac\\ud56d\\uc744 \\uc54c\\ub9b4 \\uac83\\uc744 \\uad8c\\uc720\\ud588\\uc744 \\ub54c. \\ub2e4\\ub9cc, \\ubcf4\\ud5d8\\uc124 \\uacc4\\uc0ac \\ub4f1\\uc758 \\ud589\\uc704\\uac00 \\uc5c6\\uc5c8\\ub2e4 \\ud558\\ub354\\ub77c\\ub3c4 \\uacc4\\uc57d\\uc790 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uc0ac\\uc2e4\\ub300\\ub85c\\uc54c\\ub9ac\\uc9c0\\uc54a\\uac70\\ub098\\ubd80\\uc2e4\\ud55c\\uc0ac\\ud56d \\uc744\\uc54c\\ub838\\ub2e4\\uace0\\uc778\\uc815\\ub418\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574 \\uc9c0\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2462 \\uc81c1\\ud56d\\uc5d0 \\ub530\\ub77c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud558\\uc600\\uc744\\ub54c\\uc5d0\\ub294\\uc81c 25\\uc870(\\ud574\\uc9c0\\ud658\\uae09\\uae08) \\uc81c1\\ud56d\\uc5d0\\ub530\\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d \\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2463 \\uc81c1\\ud56d\\uc81c1\\ud638\\uc5d0\\uc758\\ud55c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ud574\\uc9c0\\uac00\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09 \\uc0ac\\uc720\\ubc1c\\uc0dd\\ud6c4\\uc5d0\\uc774\\ub8e8\\uc5b4\\uc9c4\\uacbd\\uc6b0\\uc5d0\\ud68c\\uc0ac\\ub294\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0 \\uae09\\ud558\\uc9c0\\uc54a\\uc73c\\uba70, \\uacc4\\uc57d\\uc804\\uc54c\\ub9b4\\uc758\\ubb34\\uc704\\ubc18\\uc0ac\\uc2e4\\ubfd0\\ub9cc\\uc544 \\ub2c8\\ub77c\\uacc4\\uc57d\\uc804\\uc54c\\ub9b4\\uc758\\ubb34\\uc0ac\\ud56d\\uc774\\uc911\\uc694\\ud55c\\uc0ac\\ud56d\\uc5d0\\ud574\\ub2f9\\ub418 \\ub294\\uc0ac\\uc720\\ub97c \\\"\\ubc18\\ub300\\uc99d\\uac70\\uac00\\uc788\\ub294\\uacbd\\uc6b0\\uc774\\uc758\\ub97c\\uc81c\\uae30\\ud560\\uc218 \\uc788\\uc2b5\\ub2c8\\ub2e4\\\"\\ub77c\\ub294\\ubb38\\uad6c\\uc640\\ud568\\uaed8\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\uc11c\\uba74\\ub4f1\\uc73c\\ub85c \\uc54c\\ub824\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\u2464 \\uc81c1\\ud56d\\uc81c2\\ud638\\uc5d0\\uc758\\ud55c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ud574\\uc9c0\\uac00\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09 \\uc0ac\\uc720\\ubc1c\\uc0dd\\ud6c4\\uc5d0\\uc774\\ub8e8\\uc5b4\\uc9c4\\uacbd\\uc6b0\\uc5d0\\ub294\\uc81c12\\uc870(\\uc0c1\\ud574\\ubcf4\\ud5d8 \\uacc4\\uc57d\\ud6c4\\uc54c\\ub9b4\\uc758\\ubb34) \\uc81c4\\ud56d\\ub610\\ub294\\uc81c5\\ud56d\\uc5d0\\ub530\\ub77c\\ubcf4\\ud5d8\\uae08 \\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2465 \\uc81c1\\ud56d\\uc5d0\\ub3c4 \\ubd88\\uad6c\\ud558\\uace0\\uc54c\\ub9b4\\uc758\\ubb34\\ub97c\\uc704\\ubc18\\ud55c\\uc0ac\\uc2e4\\uc774\\ubcf4 \\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ubc1c\\uc0dd\\uc5d0\\uc601\\ud5a5\\uc744\\ubbf8\\uce58\\uc9c0\\uc54a\\uc558\\uc74c\\uc744\\uacc4\\uc57d \\uc790, \\ud53c\\ubcf4\\ud5d8\\uc790 \\ub610\\ub294 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\uc99d\\uba85\\ud55c \\uacbd\\uc6b0\\uc5d0\\ub294 \\uc81c4\\ud56d\\ubc0f\\uc81c5\\ud56d\\uc5d0\\uad00\\uacc4\\uc5c6\\uc774\\uc57d\\uc815\\ud55c\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud569 \\ub2c8\\ub2e4. \\u2466 \\ud68c\\uc0ac\\ub294\\ub2e4\\ub978\\ubcf4\\ud5d8\\uac00\\uc785\\ub0b4\\uc5ed\\uc5d0\\ub300\\ud55c\\uacc4\\uc57d\\uc804\\uc54c\\ub9b4\\uc758\\ubb34 \\uc704\\ubc18\\uc744\\uc774\\uc720\\ub85c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud558\\uac70\\ub098\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09 \\uc744\\uac70\\uc808\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2467 \\uc81c22\\uc870(\\ubcf4\\ud5d8\\ub8cc\\uc758\\ub0a9\\uc785\\uc5f0\\uccb4\\ub85c\\uc778\\ud55c\\ud574\\uc9c0\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5))\\uc5d0 \\ub530\\ub77c \\uc774 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc774 \\ubd80\\ud65c(\\ud6a8\\ub825 \\ud68c\\ubcf5)\\ub41c \\uacbd\\uc6b0\\uc5d0\\ub294 \\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uacc4\\uc57d\\uc744 \\uc81c2\\ud56d\\uc758 \\ucd5c\\ucd08\\uacc4\\uc57d\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4. \\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uc774 \\uc5ec\\ub7ec\\ucc28\\ub840 \\ubc1c\\uc0dd\\ub41c \\uacbd\\uc6b0\\uc5d0\\ub294\\uac01\\uac01\\uc758\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uacc4\\uc57d\\uc744 \\ucd5c \\ucd08\\uacc4\\uc57d\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c14\\uc870 (\\uc0ac\\uae30\\uc5d0 \\uc758\\ud55c \\uacc4\\uc57d)\", \"Head\": \"\", \"text\": \"\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ub300\\ub9ac\\uc9c4\\ub2e8, \\uc57d\\ubb3c\\uc0ac\\uc6a9\\uc744\\uc218\\ub2e8\\uc73c \\ub85c\\uc9c4\\ub2e8\\uc808\\ucc28\\ub97c\\ud1b5\\uacfc\\ud558\\uac70\\ub098\\uc9c4\\ub2e8\\uc11c\\uc704\\u00b7\\ubcc0\\uc870\\ub610\\ub294\\uccad\\uc57d \\uc77c\\uc774\\uc804\\uc5d0\\uc554\\ub610\\ub294\\uc778\\uac04\\uba74\\uc5ed\\uacb0\\ud54d\\ubc14\\uc774\\ub7ec\\uc2a4(HIV) \\uac10\\uc5fc\\uc758 \\uc9c4\\ub2e8\\ud655\\uc815\\uc744\\ubc1b\\uc740\\ud6c4\\uc774\\ub97c\\uc228\\uae30\\uace0\\uac00\\uc785\\ud558\\ub294\\ub4f1\\uc0ac\\uae30\\uc5d0\\uc758 \\ud558\\uc5ec\\uacc4\\uc57d\\uc774\\uc131\\ub9bd\\ub418\\uc5c8\\uc74c\\uc744\\ud68c\\uc0ac\\uac00\\uc99d\\uba85\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294 \\uacc4\\uc57d\\uc77c\\ubd80\\ud130 5\\ub144 \\uc774\\ub0b4(\\uc0ac\\uae30\\uc0ac\\uc2e4\\uc744 \\uc548 \\ub0a0\\ubd80\\ud130 1\\uac1c\\uc6d4 \\uc774 \\ub0b4)\\uc5d0\\uacc4\\uc57d\\uc744\\ucde8\\uc18c\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c15\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uccb4\\uacb0)\", \"Head\": \"\", \"text\": \"\\u2460 \\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc740\\uae30\\ubcf8\\uacc4\\uc57d\\uc744\\uccb4\\uacb0\\ud560\\ub54c\\uacc4\\uc57d\\uc790\\uc758\\uccad\\uc57d \\uacfc\\ud68c\\uc0ac\\uc758\\uc2b9\\ub099\\uc73c\\ub85c\\uae30\\ubcf8\\uacc4\\uc57d\\uc5d0\\ubd80\\uac00\\ud558\\uc5ec\\uc774\\ub8e8\\uc5b4\\uc9d1 \\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc801\\ud569\\ud558\\uc9c0\\uc54a\\uc740\\uacbd \\uc6b0\\uc5d0\\ub294\\uc2b9\\ub099\\uc744\\uac70\\uc808\\ud558\\uac70\\ub098\\ubcc4\\ub3c4\\uc758\\uc870\\uac74(\\ubcf4\\ud5d8\\uac00\\uc785\\uae08 \\uc561\\uc81c\\ud55c, \\uc77c\\ubd80\\ubcf4\\uc7a5\\uc81c\\uc678, \\ubcf4\\ud5d8\\uae08\\uc0ad\\uac10, \\ubcf4\\ud5d8\\ub8cc\\ud560\\uc99d \\ub4f1)\\uc744\\ubd99\\uc5ec\\uc2b9\\ub099\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\u2462 \\ud68c\\uc0ac\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\uccad\\uc57d\\uc744\\ubc1b\\uace0\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ubc1b\\uc740\\uacbd\\uc6b0\\uc5d0\\uac74\\uac15\\uc9c4\\ub2e8\\uc744\\ubc1b\\uc9c0\\uc54a\\ub294\\ud2b9\\ubcc4\\uc57d\\uad00\\uc740\\uccad\\uc57d \\uc77c, \\uc9c4\\ub2e8\\uacc4\\uc57d\\uc740\\uc9c4\\ub2e8\\uc77c(\\uc7ac\\uc9c4\\ub2e8\\uc758\\uacbd\\uc6b0\\uc5d0\\ub294\\ucd5c\\uc885\\uc9c4 \\ub2e8\\uc77c)\\ubd80\\ud130 30\\uc77c \\uc774\\ub0b4\\uc5d0\\uc2b9\\ub099\\ub610\\ub294\\uac70\\uc808\\ud558\\uc5ec\\uc57c\\ud558\\uba70, \\uc2b9\\ub099\\ud55c \\ub54c\\uc5d0\\ub294 \\ubcf4\\ud5d8\\uc99d\\uad8c\\uc744 \\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\uadf8\\ub7ec\\ub098 30\\uc77c \\uc774\\ub0b4\\uc5d0\\uc2b9\\ub099\\ub610\\ub294\\uac70\\uc808\\uc758\\ud1b5\\uc9c0\\uac00\\uc5c6\\uc73c\\uba74\\uc2b9\\ub099\\ub41c\\uac83\\uc73c \\ub85c\\ubd05\\ub2c8\\ub2e4. \\u2463 \\ud68c\\uc0ac\\uac00\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uace0\\uc2b9\\ub099\\uc744\\uac70\\uc808\\ud55c\\uacbd\\uc6b0\\uc5d0 \\ub294\\uac70\\uc808\\ud1b5\\uc9c0\\uc640\\ud568\\uaed8\\ubc1b\\uc740\\uae08\\uc561\\uc744\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\ub3cc\\ub824\\ub4dc \\ub9ac\\uba70, \\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\uae30\\uac04\\uc5d0\\ub300\\ud558\\uc5ec\\ud45c\\uc900\\uc774\\uc728+1%\\ub97c \\uc5f0\\ub2e8\\uc704 \\ubcf5\\ub9ac\\ub85c \\uacc4\\uc0b0\\ud55c \\uae08\\uc561\\uc744 \\ub354\\ud558\\uc5ec \\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ud68c\\uc0ac\\ub294\\uacc4\\uc57d\\uc790\\uac00\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c\\uc2e0\\uc6a9\\uce74\\ub4dc\\ub85c \\ub0a9\\uc785\\ud55c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\uc2b9\\ub099\\uc744\\uac70\\uc808\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc2e0\\uc6a9 \\uce74\\ub4dc\\uc758\\ub9e4\\ucd9c\\uc744\\ucde8\\uc18c\\ud558\\uba70\\uc774\\uc790\\ub97c\\ub354\\ud558\\uc5ec\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a \\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c16\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubb34\\ud6a8)\", \"Head\": \"\", \"text\": \"\\ub2e4\\uc74c\\uc911\\ud55c\\uac00\\uc9c0\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ubb34 \\ud6a8\\ub85c\\ud558\\uba70\\uc774\\ubbf8\\ub0a9\\uc785\\ud55c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub3cc\\ub824\\ub4dc \\ub9bd\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ud68c\\uc0ac\\uc758\\uace0\\uc758\\ub610\\ub294\\uacfc\\uc2e4\\ub85c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\ubb34 \\ud6a8\\ub85c\\ub41c\\uacbd\\uc6b0\\uc640\\ud68c\\uc0ac\\uac00\\uc2b9\\ub099\\uc804\\uc5d0\\ubb34\\ud6a8\\uc784\\uc744\\uc54c\\uc558\\uac70\\ub098\\uc54c \\uc218\\uc788\\uc5c8\\uc74c\\uc5d0\\ub3c4\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc18\\ud658\\ud558\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8 \\ub8cc\\ub97c\\ub0a9\\uc785\\ud55c\\ub0a0\\uc758\\ub2e4\\uc74c\\ub0a0\\ubd80\\ud130\\ubc18\\ud658\\uc77c\\uae4c\\uc9c0\\uc758\\uae30\\uac04\\uc5d0\\ub300 \\ud558\\uc5ec\\ud68c\\uc0ac\\ub294\\uc774 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubcf4\\ud5d8\\uacc4\\uc57d\\ub300\\ucd9c\\uc774\\uc728\\uc744 \\uc5f0\\ub2e8 \\uc704\\ubcf5\\ub9ac\\ub85c\\uacc4\\uc0b0\\ud55c\\uae08\\uc561\\uc744\\ub354\\ud558\\uc5ec\\ub3cc\\ub824\\ub4dc\\ub9bd\\ub2c8\\ub2e4. 1.\\ud0c0\\uc778\\uc758\\uc0ac\\ub9dd\\uc744\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ub85c\\ud558\\ub294\\uacc4\\uc57d\\uc5d0\\uc11c \\uacc4\\uc57d\\uc744 \\uccb4\\uacb0\\ud560 \\ub54c\\uae4c\\uc9c0 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc11c\\uba74\\uc5d0 \\uc758\\ud55c \\ub3d9\\uc758\\ub97c\\uc5bb\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0. \\ub2e4\\ub9cc, \\ub2e8\\uccb4\\uac00\\uaddc\\uc57d\\uc5d0\\ub530 \\ub77c\\uad6c\\uc131\\uc6d0\\uc758\\uc804\\ubd80 \\ub610\\ub294\\uc77c\\ubd80\\ub97c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub85c\\ud558\\ub294 \\uacc4\\uc57d\\uc744\\uccb4\\uacb0\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc774\\ub97c\\uc801\\uc6a9\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8 \\ub2e4. \\uc774\\ub54c\\ub2e8\\uccb4\\ubcf4\\ud5d8\\uc758\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\ub97c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub610 \\ub294\\uadf8\\uc0c1\\uc18d\\uc778\\uc774\\uc544\\ub2cc\\uc790\\ub85c\\uc9c0\\uc815\\ud560\\ub54c\\uc5d0\\ub294\\ub2e8\\uccb4\\uc758 \\uaddc\\uc57d\\uc5d0\\uc11c \\uba85\\uc2dc\\uc801\\uc73c\\ub85c \\uc815\\ud55c \\uacbd\\uc6b0\\uac00 \\uc544\\ub2c8\\uba74 \\uc774\\ub97c \\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4. 2.\\ub9cc 15\\uc138 \\ubbf8\\ub9cc\\uc790, \\uc2ec\\uc2e0\\uc0c1\\uc2e4\\uc790\\ub610\\ub294\\uc2ec\\uc2e0\\ubc15\\uc57d\\uc790\\ub97c \\ud53c\\ubcf4\\ud5d8\\uc790\\ub85c \\ud558\\uc5ec \\uc0ac\\ub9dd\\uc744 \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\ub85c \\ud55c \\uacbd\\uc6b0.\\ub2e4\\ub9cc,\\uc2ec\\uc2e0\\ubc15\\uc57d\\uc790\\uac00\\uacc4\\uc57d\\uc744\\uccb4\\uacb0\\ud558\\uac70\\ub098\\uc18c \\uc18d \\ub2e8\\uccb4\\uc758 \\uaddc\\uc57d\\uc5d0 \\ub530\\ub77c \\ub2e8\\uccb4\\ubcf4\\ud5d8\\uc758 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\ub420\\ub54c\\uc5d0 \\uc758\\uc0ac\\ub2a5\\ub825\\uc774 \\uc788\\ub294 \\uacbd\\uc6b0\\uacc4\\uc57d\\uc774 \\uc720\\ud6a8\\ud569\\ub2c8 \\ub2e4. <\\uc6a9\\uc5b4\\ud480\\uc774> \\u300c\\uc2ec\\uc2e0\\uc0c1\\uc2e4\\uc790(\\u5fc3\\u795e\\u55aa\\u5931\\u8005) \\ub610\\ub294\\uc2ec\\uc2e0\\ubc15\\uc57d\\uc790(\\u5fc3\\u795e\\u8584\\u5f31\\u8005)\\u300d\\ub77c\\ud568\\uc740\\uc815\\uc2e0 \\ubcd1, \\uc815\\uc2e0\\ubc15\\uc57d,\\uc2ec\\ud55c\\uc758\\uc2dd\\uc7a5\\uc560\\ub4f1\\uc758\\uc2ec\\uc2e0\\uc7a5\\uc560\\ub85c\\uc778\\ud558\\uc5ec\\uc0ac\\ubb3c\\ubcc0\\ubcc4\\ub2a5\\ub825\\ub610\\ub294 \\uc758\\uc0ac\\uacb0\\uc815\\ub2a5\\ub825\\uc774\\uc5c6\\uac70\\ub098\\ubd80\\uc871\\ud55c\\uc790\\ub97c\\ub9d0\\ud569\\ub2c8\\ub2e4. 3.\\uacc4\\uc57d\\uc744 \\uccb4\\uacb0\\ud560\\ub54c \\uacc4\\uc57d\\uc5d0\\uc11c \\uc815\\ud55c\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\ub098 \\uc774\\uc5d0\\ubbf8\\ub2ec\\ub418\\uc5c8\\uac70\\ub098\\ucd08\\uacfc\\ub418\\uc5c8\\uc744\\uacbd\\uc6b0.\\ub2e4\\ub9cc, \\ud68c\\uc0ac \\uac00 \\ub098\\uc774\\uc758\\ucc29\\uc624\\ub97c \\ubc1c\\uacac\\ud558\\uc600\\uc744 \\ub54c\\uc774\\ubbf8 \\uacc4\\uc57d\\ub098\\uc774 \\uc5d0\\ub3c4\\ub2ec\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc720\\ud6a8\\ud55c\\uacc4\\uc57d\\uc73c\\ub85c\\ubcf4\\ub098\\uc81c2\\ud638 \\uc758\\ub9cc 15\\uc138 \\ubbf8\\ub9cc\\uc790\\uc5d0\\uad00\\ud55c\\uc608\\uc678\\uac00\\uc778\\uc815\\ub418\\ub294\\uac83\\uc740 \\uc544\\ub2d9\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c17\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00 \\ub0b4\\uc6a9\\uc758 \\ubcc0\\uacbd \\ub4f1)\", \"Head\": \"\", \"text\": \"\\u2460 \\uacc4\\uc57d\\uc790\\ub294\\ud68c\\uc0ac\\uc758\\uc2b9\\ub099\\uc744\\uc5bb\\uc5b4\\ub2e4\\uc74c\\uc758\\uc0ac\\ud56d\\uc744\\ubcc0\\uacbd\\ud560 \\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\uc774 \\uacbd\\uc6b0\\uc2b9\\ub099\\uc744\\uc11c\\uba74\\ub4f1\\uc73c\\ub85c\\uc54c\\ub9ac\\uac70\\ub098 \\ubcf4\\ud5d8\\uc99d\\uad8c\\uc758\\ub4b7\\uba74\\uc5d0\\uae30\\uc7ac\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4. 1.\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uc8fc\\uae30,\\ub0a9\\uc785\\ubc29\\ubc95\\ubc0f\\ub0a9\\uc785\\uae30\\uac04 2.\\ubcf4\\ud5d8\\uac00\\uc785\\uae08\\uc561\\ub4f1\\uae30\\ud0c0\\uacc4\\uc57d\\uc758\\ub0b4\\uc6a9 \\u2461 \\ud68c\\uc0ac\\ub294\\uacc4\\uc57d\\uc790\\uac00\\uc81c1\\ud56d\\uc81c2\\ud638\\uc5d0\\ub530\\ub77c\\ubcf4\\ud5d8\\uac00\\uc785\\uae08\\uc561 \\uc744 \\uac10\\uc561\\ud558\\uace0\\uc790 \\ud560 \\ub54c\\uc5d0\\ub294 \\uadf8 \\uac10\\uc561\\ub41c \\ubd80\\ubd84\\uc740 \\ud574\\uc9c0\\ub41c \\uac83\\uc73c\\ub85c \\ubcf4\\uba70, \\uc774\\ub85c\\uc368 \\ud68c\\uc0ac\\uac00\\uc9c0\\uae09\\ud558\\uc5ec\\uc57c \\ud560\\ud574\\uc9c0\\ud658 \\uae09\\uae08\\uc774\\uc788\\uc744\\ub54c\\uc5d0\\ub294\\uc81c25\\uc870(\\ud574\\uc9c0\\ud658\\uae09\\uae08)\\uc81c1\\ud56d\\uc5d0\\ub530 \\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c18\\uc870 (\\ubcf4\\ud5d8\\ub098\\uc774 \\ub4f1)\", \"Head\": \"\", \"text\": \"\\u2460 \\uc774\\uc57d\\uad00\\uc5d0\\uc11c\\uc758\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\ub098\\uc774\\ub294\\ubcf4\\ud5d8\\ub098\\uc774\\ub97c\\uae30\\uc900 \\uc73c\\ub85c\\ud569\\ub2c8\\ub2e4.\\ub2e4\\ub9cc,\\uc81c16\\uc870(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubb34\\ud6a8)\\uc81c2\\ud638 \\uc758\\uacbd\\uc6b0\\uc5d0\\ub294\\uc2e4\\uc81c\\ub9cc\\ub098\\uc774\\ub97c\\uc801\\uc6a9\\ud569\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\ud56d\\uc758 \\ubcf4\\ud5d8\\ub098\\uc774\\ub294 \\uacc4\\uc57d\\uc77c \\ud604\\uc7ac \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc2e4\\uc81c \\ub9cc\\ub098\\uc774\\ub97c\\uae30\\uc900\\uc73c\\ub85c 6\\uac1c\\uc6d4\\ubbf8\\ub9cc\\uc758\\ub05d\\uc218\\ub294\\ubc84\\ub9ac\\uace0 6\\uac1c \\uc6d4\\uc774\\uc0c1\\uc758\\ub05d\\uc218\\ub294 1\\ub144\\uc73c\\ub85c\\ud558\\uc5ec\\uacc4\\uc0b0\\ud558\\uba70,\\uc774\\ud6c4\\ub9e4\\ub144 \\uacc4\\uc57d\\ud574\\ub2f9\\uc77c\\uc5d0\\ub098\\uc774\\uac00\\uc99d\\uac00\\ud558\\ub294\\uac83\\uc73c\\ub85c\\ud569\\ub2c8\\ub2e4. \\u2462 \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758\\ub098\\uc774\\ub610\\ub294\\uc131\\ubcc4\\uc5d0\\uad00\\ud55c\\uae30\\uc7ac\\uc0ac\\ud56d\\uc774\\uc0ac\\uc2e4 \\uacfc\\ub2e4\\ub978\\uacbd\\uc6b0\\uc5d0\\ub294\\uc815\\uc815\\ub41c\\ub098\\uc774\\ub610\\ub294\\uc131\\ubcc4\\uc5d0\\ud574\\ub2f9\\ud558\\ub294 \\ubcf4\\ud5d8\\uae08\\ubc0f\\ubcf4\\ud5d8\\ub8cc\\ub85c\\ubcc0\\uacbd\\ud569\\ub2c8\\ub2e4. <\\ubcf4\\ud5d8\\ub098\\uc774\\uacc4\\uc0b0\\uc608\\uc2dc> \\uc0dd\\ub144\\uc6d4\\uc77c : 1988\\ub144 10\\uc6d4 2\\uc77c \\ud604\\uc7ac(\\uacc4\\uc57d\\uc77c) : 2014\\ub144 4\\uc6d4 13\\uc77c \\u21d2 2014\\ub144 4\\uc6d4 13\\uc77c - 1988\\ub144 10\\uc6d4 2\\uc77c = 25\\ub144 6\\uac1c\\uc6d4 11\\uc77c = 26\\uc138\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c19\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uc18c\\uba78)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\uac00\\uc81c3\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uc9c0\\uae09\\uc0ac\\uc720)\\uc5d0\\uc11c\\uc815\\ud55c\\uc0c1\\ud574\\uace0 \\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc744\\uc9c0\\uae09\\ud55c\\ub54c\\uc5d0\\ub294\\uadf8\\uc190\\ud574 \\ubcf4\\uc0c1\\uc758\\uc6d0\\uc778\\uc774\\uc0dd\\uae34\\ub54c\\ubd80\\ud130\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc740\\uc18c\\uba78\\ub418\\uba70 \\uadf8\\ub54c\\ubd80\\ud130\\ud6a8\\ub825\\uc774\\uc5c6\\uc2b5\\ub2c8\\ub2e4. \\uc774\\uacbd\\uc6b0\\ud68c\\uc0ac\\ub294\\uc774\\ud2b9\\ubcc4 \\uc57d\\uad00\\uc758\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2461 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ubcf4\\ud5d8\\uae30\\uac04\\uc911\\uc5d0\\uc0ac\\ub9dd\\ud558\\uc600\\uc744\\uacbd\\uc6b0\\uc5d0\\ub294 \\\"\\ubcf4 \\ud5d8\\ub8cc \\ubc0f \\ucc45\\uc784\\uc900\\ube44\\uae08 \\uc0b0\\ucd9c\\ubc29\\ubc95\\uc11c\\\"\\uc5d0\\uc11c \\uc815\\ud558\\ub294 \\ubc14\\uc5d0 \\ub530\\ub77c\\ud68c\\uc0ac\\uac00\\uc801\\ub9bd\\ud55c\\uc0ac\\ub9dd\\ub2f9\\uc2dc\\uc758\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ucc45\\uc784 \\uc900\\ube44\\uae08\\uc744 \\uacc4\\uc57d\\uc790\\uc5d0\\uac8c \\uc9c0\\uae09\\ud558\\uace0, \\uc774 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc740 \\ub354 \\uc774\\uc0c1\\ud6a8\\ub825\\uc774\\uc5c6\\uc2b5\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8\\ubb34 \\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc758 \\ubcf4\\ud1b5\\uc57d\\uad00(\\uc774\\ud558\\u300c\\ubcf4\\ud1b5\\uc57d\\uad00\\u300d\\uc774\\ub77c \\ud569\\ub2c8\\ub2e4) \\uc81c10\\uc870 (\\uacc4\\uc57d\\ub0b4\\uc6a9\\uc758 \\ubcc0\\uacbd\\ub4f1) \\uc81c1\\ud56d\\uc5d0 \\ub530\\ub77c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c\\ubcc0 \\uacbd\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc774\\ub97c\\uc801\\uc6a9\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c20\\uc870 (\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc \\ubc0f \\ud68c\\uc0ac\\uc758 \\ubcf4\\uc7a5\\uac1c\\uc2dc)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\uccad\\uc57d\\uc744\\uc2b9\\ub099\\ud558\\uace0\\uc81c1\\ud68c \\ubcf4\\ud5d8 \\ub8cc\\ub97c\\ubc1b\\uc740\\ub54c\\ubd80\\ud130 \\uc774\\uc57d\\uad00\\uc774\\uc815\\ud55c \\ubc14\\uc5d0 \\ub530\\ub77c\\ubcf4\\uc7a5\\uc744 \\ud569\\ub2c8\\ub2e4. \\ub610\\ud55c, \\ud68c\\uc0ac\\uac00 \\uccad\\uc57d\\uacfc\\ud568\\uaed8 \\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ubc1b\\uc740\\ud6c4\\uc2b9\\ub099\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub3c4\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\ub54c\\ubd80 \\ud130 \\ubcf4\\uc7a5\\uc774 \\uac1c\\uc2dc\\ub429\\ub2c8\\ub2e4. \\uc790\\ub3d9\\uc774\\uccb4 \\ub610\\ub294 \\uc2e0\\uc6a9\\uce74\\ub4dc\\ub85c \\ub0a9\\uc785\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uc790\\ub3d9\\uc774\\uccb4\\uc2e0\\uccad\\ub610\\ub294\\uc2e0\\uc6a9\\uce74\\ub4dc\\ub9e4 \\ucd9c\\uc2b9\\uc778\\uc5d0\\ud544\\uc694\\ud55c\\uc815\\ubcf4\\ub97c\\uc81c\\uacf5\\ud55c\\ub54c\\ub97c\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc \\ub97c\\ubc1b\\uc740\\ub54c\\ub85c\\ud558\\uba70, \\uacc4\\uc57d\\uc790\\uc758\\ucc45\\uc784\\uc788\\ub294\\uc0ac\\uc720\\ub85c\\uc790\\ub3d9 \\uc774\\uccb4\\ub610\\ub294\\ub9e4\\ucd9c\\uc2b9\\uc778\\uc774\\ubd88\\uac00\\ub2a5\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\ub8cc\\uac00 \\ub0a9\\uc785\\ub418\\uc9c0\\uc54a\\uc740\\uac83\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\uac00\\uccad\\uc57d\\uacfc\\ud568\\uaed8\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uace0\\uccad\\uc57d\\uc744\\uc2b9 \\ub099\\ud558\\uae30\\uc804\\uc5d0\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud558\\uc600\\uc744\\ub54c\\uc5d0\\ub3c4 \\ubcf4\\uc7a5\\uac1c\\uc2dc\\uc77c\\ubd80\\ud130\\uc774\\uc57d\\uad00\\uc774\\uc815\\ud558\\ub294\\ubc14\\uc5d0\\ub530\\ub77c\\ubcf4\\uc7a5\\uc744 \\ud569\\ub2c8\\ub2e4. <\\ubcf4\\uc7a5\\uac1c\\uc2dc\\uc77c> \\ud68c\\uc0ac\\uac00\\ubcf4\\uc7a5\\uc744\\uac1c\\uc2dc\\ud558\\ub294\\ub0a0\\ub85c\\uc11c\\uacc4\\uc57d\\uc774\\uc131\\ub9bd\\ub418\\uace0\\uc81c1\\ud68c\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\ub0a0 \\uc744\\ub9d0\\ud558\\ub098,\\ud68c\\uc0ac\\uac00\\uc2b9\\ub099\\ud558\\uae30\\uc804\\uc774\\ub77c\\ub3c4\\uccad\\uc57d\\uacfc\\ud568\\uaed8\\uc81c1\\ud68c\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\uacbd \\uc6b0\\uc5d0\\ub294\\uc81c1\\ud68c\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ubc1b\\uc740\\ub0a0\\uc744\\ub9d0\\ud569\\ub2c8\\ub2e4. \\ub610\\ud55c, \\ubcf4\\uc7a5\\uac1c\\uc2dc\\uc77c\\uc744\\uacc4\\uc57d\\uc77c\\ub85c \\ubd05\\ub2c8\\ub2e4. \\u2462 \\ud68c\\uc0ac\\ub294\\uc81c2\\ud56d\\uc5d0\\ub3c4\\ubd88\\uad6c\\ud558\\uace0\\ub2e4\\uc74c\\uc911\\ud55c\\uac00\\uc9c0\\uc5d0\\ud574\\ub2f9 \\ub418\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\uc7a5\\uc744\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. 1.\\uc81c11\\uc870(\\uacc4\\uc57d \\uc804 \\uc54c\\ub9b4 \\uc758\\ubb34)\\uc5d0 \\ub530\\ub77c \\uacc4\\uc57d\\uc790 \\ub610\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ud68c\\uc0ac\\uc5d0\\uc54c\\ub9b0\\ub0b4\\uc6a9\\uc774\\ub098\\uac74\\uac15\\uc9c4\\ub2e8\\ub0b4\\uc6a9 \\uc774\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc758\\ubc1c\\uc0dd\\uc5d0\\uc601\\ud5a5\\uc744\\ubbf8\\ucce4\\uc74c\\uc744\\ud68c \\uc0ac\\uac00\\uc99d\\uba85\\ud558\\ub294\\uacbd\\uc6b0 2.\\uc81c13\\uc870(\\uc54c\\ub9b4\\uc758\\ubb34\\uc704\\ubc18\\uc758\\ud6a8\\uacfc)\\ub97c\\uc900\\uc6a9\\ud558\\uc5ec\\ud68c\\uc0ac \\uac00\\ubcf4\\uc7a5\\uc744\\ud558\\uc9c0\\uc54a\\uc744\\uc218\\uc788\\ub294\\uacbd\\uc6b0 3.\\uc9c4\\ub2e8\\uacc4\\uc57d\\uc5d0\\uc11c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud560\\ub54c\\uae4c\\uc9c0 \\uc9c4\\ub2e8\\uc744\\ubc1b\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0.\\ub2e4\\ub9cc, \\uc9c4\\ub2e8\\uacc4\\uc57d\\uc5d0\\uc11c\\uc9c4\\ub2e8 \\uc744\\ubc1b\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0\\ub77c\\ub3c4\\uc0c1\\ud574\\ub85c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00 \\ubc1c\\uc0dd\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\uc7a5\\uc744\\ud574\\ub4dc\\ub9bd\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c21\\uc870 (\\ubcf4\\ud5d8\\ub8cc\\uc758 \\ub0a9\\uc785\\uc774 \\uc5f0\\uccb4\\ub418\\ub294 \\uacbd\\uc6b0 \\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09)\\uc640 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ud574\\uc9c0)\", \"Head\": \"\", \"text\": \"\\u2460 \\uacc4\\uc57d\\uc790\\uac00\\uc81c2\\ud68c \\uc774\\ud6c4\\uc758\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub0a9 \\uc785\\uae30\\uc77c\\uae4c\\uc9c0\\ub0a9\\uc785\\ud558\\uc9c0\\uc54a\\uc544\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uc774\\uc5f0\\uccb4\\uc911\\uc778 \\uacbd\\uc6b0\\uc5d0\\ud68c\\uc0ac\\ub294 14\\uc77c(\\ubcf4\\ud5d8\\uae30\\uac04\\uc774 1\\ub144 \\ubbf8\\ub9cc\\uc778\\uacbd\\uc6b0\\uc5d0 \\ub294 7\\uc77c) \\uc774\\uc0c1\\uc758\\uae30\\uac04\\uc744\\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09)\\uae30\\uac04(\\ub0a9\\uc785\\ucd5c \\uace0(\\ub3c5\\ucd09)\\uae30\\uac04\\uc758\\ub9c8\\uc9c0\\ub9c9\\ub0a0\\uc774\\uc601\\uc5c5\\uc77c\\uc774\\uc544\\ub2cc\\ub54c\\uc5d0\\ub294 \\ucd5c\\uace0(\\ub3c5\\ucd09)\\uae30\\uac04\\uc740\\uadf8\\ub2e4\\uc74c\\ub0a0\\uae4c\\uc9c0\\ub85c\\ud569\\ub2c8\\ub2e4)\\uc73c\\ub85c\\uc815 \\ud558\\uc5ec \\uc544\\ub798 \\uc0ac\\ud56d\\uc5d0 \\ub300\\ud558\\uc5ec \\uc11c\\uba74(\\ub4f1\\uae30\\uc6b0\\ud3b8 \\ub4f1), \\uc804\\ud654 (\\uc74c\\uc131\\ub179\\uc74c) \\ub610\\ub294 \\uc804\\uc790\\ubb38\\uc11c \\ub4f1\\uc73c\\ub85c \\uc54c\\ub824 \\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ud574\\uc9c0 \\uc804\\uc5d0\\ubc1c\\uc0dd\\ud55c\\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\uc5d0\\ub300\\ud558\\uc5ec \\ud68c\\uc0ac\\ub294\\ubcf4\\uc0c1\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4. 1.\\uacc4\\uc57d\\uc790(\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc640\\uacc4\\uc57d\\uc790\\uac00\\ub2e4\\ub978\\uacbd\\uc6b0\\ubcf4\\ud5d8\\uc218 \\uc775\\uc790\\ub97c\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4)\\uc5d0\\uac8c\\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09)\\uae30\\uac04\\ub0b4 \\uc5d0\\uc5f0\\uccb4\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub0a9\\uc785\\ud558\\uc5ec\\uc57c\\ud55c\\ub2e4\\ub294\\ub0b4\\uc6a9 2.\\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09)\\uae30\\uac04\\uc774 \\ub05d\\ub098\\ub294 \\ub0a0\\uae4c\\uc9c0 \\ubcf4\\ud5d8\\ub8cc\\ub97c \\ub0a9\\uc785\\ud558\\uc9c0\\uc54a\\uc744\\uacbd\\uc6b0\\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09)\\uae30\\uac04\\uc774\\ub05d\\ub098 \\ub294 \\ub0a0\\uc758 \\ub2e4\\uc74c\\ub0a0\\uc5d0 \\ud2b9\\ubcc4\\uc57d\\uad00\\uc774 \\ud574\\uc9c0\\ub41c\\ub2e4\\ub294 \\ub0b4\\uc6a9 (\\uc774 \\uacbd\\uc6b0\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\ud574\\uc9c0\\ub418\\ub294\\ub54c\\uc5d0\\ub294\\uc989\\uc2dc\\ud574\\uc9c0 \\ud658\\uae09\\uae08\\uc5d0\\uc11c \\ubcf4\\ud5d8\\uacc4\\uc57d\\ub300\\ucd9c\\uc6d0\\uae08\\uacfc \\uc774\\uc790\\uac00 \\ucc28\\uac10\\ub41c \\ub2e4\\ub294\\ub0b4\\uc6a9\\uc744\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4) \\u2461 \\ud68c\\uc0ac\\uac00\\uc81c1\\ud56d\\uc5d0\\ub530\\ub978\\ub0a9\\uc785\\ucd5c\\uace0(\\ub3c5\\ucd09) \\ub4f1\\uc744\\uc804\\uc790\\ubb38\\uc11c \\ub85c\\uc548\\ub0b4\\ud558\\uace0\\uc790\\ud560 \\uacbd\\uc6b0\\uc5d0\\ub294 \\uacc4\\uc57d\\uc790\\uc5d0\\uac8c \\uc11c\\uba74, \\uc804\\uc790 \\uc11c\\uba85\\ubc95\\uc81c2\\uc870 \\uc81c2\\ud638\\uc5d0\\ub530\\ub978\\uc804\\uc790\\uc11c\\uba85\\ub610\\ub294\\ub3d9\\ubc95\\uc81c2 \\uc870 \\uc81c3\\ud638\\uc5d0 \\ub530\\ub978\\uacf5\\uc778\\uc804\\uc790\\uc11c\\uba85\\uc73c\\ub85c\\ub3d9\\uc758\\ub97c \\uc5bb\\uc5b4\\uc218 \\uc2e0\\ud655\\uc778\\uc744\\uc870\\uac74\\uc73c\\ub85c\\uc804\\uc790\\ubb38\\uc11c\\ub97c\\uc1a1\\uc2e0\\ud558\\uc5ec\\uc57c\\ud558\\uba70,\\uacc4 \\uc57d\\uc790\\uac00\\uc804\\uc790\\ubb38\\uc11c\\uc5d0\\ub300\\ud558\\uc5ec\\uc218\\uc2e0\\uc744\\ud655\\uc778\\ud558\\uae30\\uc804\\uae4c\\uc9c0 \\ub294\\uadf8\\uc804\\uc790\\ubb38\\uc11c\\ub294\\uc1a1\\uc2e0\\ub418\\uc9c0\\uc54a\\uc740\\uac83\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4. \\ud68c \\uc0ac\\ub294\\uc804\\uc790\\ubb38\\uc11c\\uac00\\uc218\\uc2e0\\ub418\\uc9c0\\uc54a\\uc740\\uac83\\uc744\\ud655\\uc778\\ud55c\\uacbd\\uc6b0\\uc5d0 \\ub294\\uc81c1\\ud56d\\uc5d0\\uc11c \\uc815\\ud55c\\ub0b4\\uc6a9\\uc744 \\uc11c\\uba74(\\ub4f1\\uae30\\uc6b0\\ud3b8 \\ub4f1) \\ub610\\ub294 \\uc804\\ud654(\\uc74c\\uc131\\ub179\\uc74c)\\ub85c\\ub2e4\\uc2dc\\uc54c\\ub824\\ub4dc\\ub9bd\\ub2c8\\ub2e4. \\u2462 \\uc81c1\\ud56d\\uc5d0 \\ub530\\ub77c \\ud2b9\\ubcc4\\uc57d\\uad00\\uc774 \\ud574\\uc9c0\\ub41c \\uacbd\\uc6b0\\uc5d0\\ub294 \\uc81c25\\uc870 (\\ud574\\uc9c0\\ud658\\uae09\\uae08) \\uc81c1\\ud56d\\uc5d0\\ub530\\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d\\uc790\\uc5d0 \\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c22\\uc870 (\\ubcf4\\ud5d8\\ub8cc\\uc758 \\ub0a9\\uc785\\uc5f0\\uccb4\\ub85c \\uc778\\ud55c \\ud574\\uc9c0\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5))\", \"Head\": \"\", \"text\": \"\\u2460 \\uc81c21\\uc870(\\ubcf4\\ud5d8\\ub8cc\\uc758 \\ub0a9\\uc785\\uc774 \\uc5f0\\uccb4\\ub418\\ub294 \\uacbd\\uc6b0 \\ub0a9\\uc785\\ucd5c\\uace0 (\\ub3c5\\ucd09)\\uc640\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ud574\\uc9c0)\\uc5d0\\ub530\\ub77c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\ud574\\uc9c0 \\ub418\\uc5c8\\uc73c\\ub098\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\ubc1b\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0(\\ubcf4\\ud5d8\\uacc4\\uc57d\\ub300 \\ucd9c\\ub4f1\\uc5d0\\ub530\\ub77c\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc774\\ucc28\\uac10\\ub418\\uc5c8\\uc73c\\ub098\\ubc1b\\uc9c0\\uc54a\\uc740 \\uacbd\\uc6b0 \\ub610\\ub294 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc774 \\uc5c6\\ub294 \\uacbd\\uc6b0\\ub97c \\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4) \\uacc4\\uc57d\\uc790\\ub294\\ud574\\uc9c0\\ub41c\\ub0a0\\ubd80\\ud130 2\\ub144\\uc774\\ub0b4\\uc5d0\\ud68c\\uc0ac\\uac00\\uc815\\ud55c\\uc808 \\ucc28\\uc5d0\\ub530\\ub77c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uc744 \\uccad\\uc57d\\ud560\\uc218 \\uc788\\uc2b5\\ub2c8\\ub2e4.\\ud68c\\uc0ac\\uac00\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uc744\\uc2b9\\ub099\\ud55c\\ub54c\\uc5d0\\uacc4 \\uc57d\\uc790\\ub294 \\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uc744 \\uccad\\uc57d\\ud55c\\ub0a0\\uae4c\\uc9c0\\uc758\\uc5f0\\uccb4\\ub41c \\ubcf4\\ud5d8\\ub8cc\\uc5d0 \\ud45c\\uc900\\uc774\\uc728+1% \\ubc94\\uc704 \\ub0b4\\uc5d0\\uc11c \\uac01\\uc0c1\\ud488\\ubcc4\\ub85c\\ud68c \\uc0ac\\uac00\\uc815\\ud558\\ub294\\uc774\\uc728\\ub85c\\uacc4\\uc0b0\\ud55c\\uae08\\uc561\\uc744\\ub354\\ud558\\uc5ec\\ub0a9\\uc785\\ud558\\uc5ec \\uc57c\\ud569\\ub2c8\\ub2e4.\\ub2e4\\ub9cc, \\uae08\\ub9ac\\uc5f0\\ub3d9\\ud615\\ubcf4\\ud5d8\\uc740\\uac01\\uc0c1\\ud488\\ubcc4\\uc0ac\\uc5c5 \\ubc29\\ubc95\\uc11c\\uc5d0\\uc11c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uc774\\uc728\\ub85c\\uacc4\\uc0b0\\ud569\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\ud56d\\uc5d0\\ub530\\ub77c\\ud574\\uc9c0\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\ud558\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294\\uc81c11\\uc870(\\uacc4\\uc57d \\uc804\\uc54c\\ub9b4\\uc758\\ubb34), \\uc81c13\\uc870(\\uc54c\\ub9b4 \\uc758\\ubb34 \\uc704\\ubc18\\uc758 \\ud6a8\\uacfc), \\uc81c14\\uc870(\\uc0ac\\uae30\\uc5d0 \\uc758\\ud55c \\uacc4\\uc57d), \\uc81c 15\\uc870(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\uccb4\\uacb0) \\ubc0f \\uc81c20\\uc870(\\uc81c1\\ud68c \\ubcf4\\ud5d8\\ub8cc\\ubc0f \\ud68c\\uc0ac\\uc758\\ubcf4\\uc7a5\\uac1c\\uc2dc)\\ub97c\\uc900\\uc6a9\\ud569\\ub2c8\\ub2e4. \\u2462 \\uc81c1\\ud56d\\uc5d0\\uc11c\\uc815\\ud55c\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)\\uc774\\uc774\\ub8e8 \\uc5b4\\uc9c4\\uacbd\\uc6b0\\ub77c\\ub3c4\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ucd5c\\ucd08\\uacc4\\uc57d\\uccad \\uc57d\\uc2dc\\uc81c11\\uc870(\\uacc4\\uc57d\\uc804\\uc54c\\ub9b4\\uc758\\ubb34)\\ub97c\\uc704\\ubc18\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294 \\uc81c13\\uc870(\\uc54c\\ub9b4\\uc758\\ubb34\\uc704\\ubc18\\uc758\\ud6a8\\uacfc)\\uac00\\uc801\\uc6a9\\ub429\\ub2c8\\ub2e4. \\u2463 \\ubcf4\\ud1b5\\uc57d\\uad00\\uc81c20\\uc870(\\ubcf4\\ud5d8\\ub8cc\\uc758\\ub0a9\\uc785\\uc5f0\\uccb4\\ub85c\\uc778\\ud55c\\ud574\\uc9c0\\uacc4 \\uc57d\\uc758\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)) \\uc81c3\\ud56d\\uc5d0\\ub530\\ub77c\\ubcf4\\ud1b5\\uc57d\\uad00\\uae30\\ubcf8 \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uae30\\uc77c\\ubc0f\\ub0a9\\uc785\\uc644\\ub8cc\\uc2dc\\uc810\\uc774\\uc5f0\\uc7a5\\ub41c\\uacbd\\uc6b0\\uc5d0 \\ub3c4\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ub0a9\\uc785\\uae30\\uc77c\\ubc0f\\ub0a9\\uc785\\uc644\\ub8cc\\uc2dc\\uc810\\uc740\\uc5f0\\uc7a5 \\ub418\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ub0a9\\uc785\\uae30\\uac04\\uc744\\uc5f0 \\uae08\\uac1c\\uc2dc\\uc804 \\ubcf4\\ud5d8\\uae30\\uac04\\uc73c\\ub85c \\uc815\\ud55c\\uacbd\\uc6b0, \\ub0a9\\uc785\\uae30\\uc77c \\ubc0f\\ub0a9 \\uc785\\uc644\\ub8cc\\uc2dc\\uc810\\uc740\\uc5f0\\uae08\\uac1c\\uc2dc\\uc2dc\\uc810\\uc758\\uc5f0\\uc7a5\\uc5d0\\ub530\\ub77c\\uc5f0\\ub2e8\\uc704 \\ub85c\\uc5f0\\uc7a5\\ub429\\ub2c8\\ub2e4. \\u2464 \\ubcf4\\ud1b5\\uc57d\\uad00\\uc81c20\\uc870(\\ubcf4\\ud5d8\\ub8cc\\uc758\\ub0a9\\uc785\\uc5f0\\uccb4\\ub85c\\uc778\\ud55c\\ud574\\uc9c0\\uacc4 \\uc57d\\uc758\\ubd80\\ud65c(\\ud6a8\\ub825\\ud68c\\ubcf5)) \\uc81c5\\ud56d\\uc5d0 \\ub530\\ub77c \\uc5f0\\uae08\\uac1c\\uc2dc\\uc2dc\\uc810 \\uc758\\uc5f0\\uc7a5\\uc73c\\ub85c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\uae30\\uac04\\ub610\\ub294\\ub0a9\\uc785\\uae30\\uac04 \\uc774\\ubcc0\\uacbd\\ub418\\ub294\\uacbd\\uc6b0\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\uc7a5\\ubcf4\\ud5d8\\ub8cc\\uac00\\ubcc0\\uacbd \\ub420\\uc218\\uc788\\uc73c\\uba70, \\uc794\\uc5ec\\uae30\\uac04\\ubcf4\\uc7a5\\uc744\\uc704\\ud55c\\uc7ac\\uc6d0\\uc778\\ud574\\uc9c0\\ud658 \\uae09\\uae08\\uc758\\uc815\\uc0b0\\uc73c\\ub85c\\uacc4\\uc57d\\uc790\\uac00\\ucd94\\uac00\\ub85c\\ub0a9\\uc785\\ud558\\uac70\\ub098\\ub3cc\\ub824 \\ubc1b\\uc744\\uae08\\uc561\\uc774\\ubc1c\\uc0dd\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c23\\uc870 (\\uacc4\\uc57d\\uc790\\uc758 \\uc784\\uc758\\ud574\\uc9c0 \\ubc0f \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc11c\\uba74\\ub3d9\\uc758 \\ucca0\\ud68c)\", \"Head\": \"\", \"text\": \"\\u2460 \\uacc4\\uc57d\\uc790\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc774\\uc18c\\uba78\\ud558\\uae30\\uc804\\uc5d0\\ub294\\uc5b8\\uc81c\\ub4e0\\uc9c0 \\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud560\\uc218\\uc788\\uc73c\\uba70, \\uc774\\uacbd\\uc6b0\\ud68c\\uc0ac\\ub294\\uc81c 25\\uc870(\\ud574\\uc9c0\\ud658\\uae09\\uae08) \\uc81c1\\ud56d\\uc5d0\\ub530\\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d \\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2461 \\uc81c16\\uc870(\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubb34\\ud6a8)\\uc5d0 \\ub530\\ub77c\\uc0ac\\ub9dd\\uc744\\ubcf4\\ud5d8\\uae08\\uc9c0 \\uae09\\uc0ac\\uc720\\ub85c\\ud558\\ub294\\uacc4\\uc57d\\uc5d0\\uc11c\\uc11c\\uba74\\uc73c\\ub85c\\ub3d9\\uc758\\ub97c\\ud55c\\ud53c\\ubcf4\\ud5d8 \\uc790\\ub294\\uacc4\\uc57d\\uc758\\ud6a8\\ub825\\uc774\\uc720\\uc9c0\\ub418\\ub294\\uae30\\uac04\\uc5d0\\ub294\\uc5b8\\uc81c\\ub4e0\\uc9c0\\uc11c \\uba74\\ub3d9\\uc758\\ub97c \\uc7a5\\ub798\\ub97c\\ud5a5\\ud558\\uc5ec\\ucca0\\ud68c\\ud560 \\uc218\\uc788\\uc73c\\uba70, \\uc11c\\uba74\\ub3d9 \\uc758\\ucca0\\ud68c\\ub85c\\uacc4\\uc57d\\uc774\\ud574\\uc9c0\\ub418\\uc5b4\\ud68c\\uc0ac\\uac00\\uc9c0\\uae09\\ud558\\uc5ec\\uc57c\\ud560\\ud574 \\uc9c0\\ud658\\uae09\\uae08\\uc774\\uc788\\uc744\\ub54c\\uc5d0\\ub294\\uc81c25\\uc870(\\ud574\\uc9c0\\ud658\\uae09\\uae08) \\uc81c1\\ud56d \\uc5d0\\ub530\\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c24\\uc870 (\\uc911\\ub300\\uc0ac\\uc720\\ub85c \\uc778\\ud55c \\ud574\\uc9c0)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294\\uc544\\ub798\\uc640\\uac19\\uc740\\uc0ac\\uc2e4\\uc774\\uc788\\uc744\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uc0ac\\uc2e4\\uc744 \\uc548\\ub0a0\\ubd80\\ud130 1\\uac1c\\uc6d4\\uc774\\ub0b4\\uc5d0\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud560\\uc218\\uc788 \\uc2b5\\ub2c8\\ub2e4. 1.\\uacc4\\uc57d\\uc790, \\ud53c\\ubcf4\\ud5d8\\uc790 \\ub610\\ub294 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\uace0\\uc758\\ub85c \\ubcf4 \\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ub97c\\ubc1c\\uc0dd\\uc2dc\\ud0a8\\uacbd\\uc6b0 2.\\uacc4\\uc57d\\uc790, \\ud53c\\ubcf4\\ud5d8\\uc790 \\ub610\\ub294 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\ubcf4\\ud5d8\\uae08 \\uccad \\uad6c\\uc5d0 \\uad00\\ud55c \\uc11c\\ub958\\uc5d0 \\uace0\\uc758\\ub85c \\uc0ac\\uc2e4\\uacfc \\ub2e4\\ub978 \\uac83\\uc744 \\uae30\\uc7ac \\ud558\\uc600\\uac70\\ub098 \\uadf8 \\uc11c\\ub958 \\ub610\\ub294 \\uc99d\\uac70\\ub97c \\uc704\\uc870 \\ub610\\ub294 \\ubcc0\\uc870\\ud55c \\uacbd\\uc6b0.\\ub2e4\\ub9cc, \\uc774\\ubbf8\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0 \\uc5d0\\ub294\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc5d0\\uc601\\ud5a5\\uc744\\ubbf8\\uce58\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. <\\uc774\\ubbf8\\ubc1c\\uc0dd\\ud55c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc5d0\\ub300\\ud55c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09> \\uacc4\\uc57d\\uc790,\\ud53c\\ubcf4\\ud5d8\\uc790\\ub610\\ub294\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00\\ubcf4\\ud5d8\\uae08\\uccad\\uad6c\\uc5d0\\uad00\\ud55c\\uc11c\\ub958\\ub97c\\ubcc0\\uc870\\ud558\\uc5ec \\ubcf4\\ud5d8\\uae08\\uc744\\uccad\\uad6c\\ud55c\\uacbd\\uc6b0, \\ud68c\\uc0ac\\ub294\\uadf8\\uc0ac\\uc2e4\\uc744\\uc548\\ub0a0\\ubd80\\ud130 1\\uac1c\\uc6d4 \\uc774\\ub0b4\\uc5d0\\uacc4\\uc57d\\uc744 \\ud574\\uc9c0\\ud560\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\\ub2e4\\ub9cc, \\uc774\\uacbd\\uc6b0\\uc5d0\\ub3c4\\ud68c\\uc0ac\\ub294\\uc2e4\\uc81c\\ubc1c\\uc0dd\\ud55c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac \\uc720\\uc5d0\\ub300\\ud574\\uc11c\\ub294\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\uac00\\uc81c1\\ud56d\\uc5d0\\ub530\\ub77c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ud574\\uc9c0\\ud55c\\uacbd\\uc6b0\\ud68c \\uc0ac\\ub294\\uadf8\\ucde8\\uc9c0\\ub97c\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\ud1b5\\uc9c0\\ud558\\uace0\\uc81c25\\uc870(\\ud574\\uc9c0\\ud658 \\uae09\\uae08) \\uc81c1\\ud56d\\uc5d0 \\ub530\\ub978 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744 \\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\ub2e4 \\ub9cc, \\uc81c1\\ud56d \\uc81c1\\ud638\\uc5d0\\uc11c \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\ubcf4\\ud5d8\\uae08\\uc758 \\uc77c\\ubd80 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc778\\uacbd\\uc6b0\\uc5d0\\ub294\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\uc740\\ubcf4\\ud5d8\\uae08\\uc5d0\\ud574 \\ub2f9\\ub418\\ub294\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c25\\uc870 (\\ud574\\uc9c0\\ud658\\uae09\\uae08)\", \"Head\": \"\", \"text\": \"\\u2460 \\uc774\\uc57d\\uad00\\uc5d0\\ub530\\ub978\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc740\\u201c\\ubcf4\\ud5d8\\ub8cc\\ubc0f\\ucc45\\uc784\\uc900\\ube44 \\uae08\\uc0b0\\ucd9c\\ubc29\\ubc95\\uc11c\\u201d\\uc5d0\\ub530\\ub77c\\uacc4\\uc0b0\\ud569\\ub2c8\\ub2e4. \\u2461 \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc758\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uacc4\\uc57d\\uc790\\ub294\\ud68c \\uc0ac\\uc5d0\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uccad\\uad6c\\ud558\\uc5ec\\uc57c\\ud558\\uba70,\\ud68c\\uc0ac\\ub294\\uccad\\uad6c\\ub97c \\uc811\\uc218\\ud55c\\ub0a0\\ubd80\\ud130 3\\uc601\\uc5c5\\uc77c\\uc774\\ub0b4\\uc5d0\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc744\\uc9c0\\uae09\\ud569 \\ub2c8\\ub2e4. \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc9c0\\uae09\\uc77c\\uae4c\\uc9c0\\uc758\\uae30\\uac04\\uc5d0\\ub300\\ud55c\\uc774\\uc790\\uc758 \\uacc4\\uc0b0\\uc740\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud560\\ub54c\\uc758\\uc801\\ub9bd\\uc774\\uc728\\uacc4\\uc0b0([\\ubcc4\\ud45c 1] \\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud560 \\ub54c\\uc758 \\uc801\\ub9bd\\uc774\\uc728\\uacc4\\uc0b0)\\uc5d0 \\ub530\\ub985\\ub2c8 \\ub2e4. \\u2462 \\ud68c\\uc0ac\\ub294\\uacbd\\uacfc\\uae30\\uac04\\ubcc4\\ud574\\uc9c0\\ud658\\uae09\\uae08\\uc5d0\\uad00\\ud55c\\ud45c\\ub97c\\uacc4\\uc57d\\uc790 \\uc5d0\\uac8c\\uc81c\\uacf5\\ud558\\uc5ec\\ub4dc\\ub9bd\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c26\\uc870 (\\uc18c\\uba78\\uc2dc\\ud6a8)\", \"Head\": \"\", \"text\": \"\\ubcf4\\ud5d8\\uae08\\uccad\\uad6c\\uad8c, \\ub9cc\\uae30\\ud658\\uae09\\uae08\\uccad\\uad6c\\uad8c, \\ubcf4\\ud5d8\\ub8cc \\ubc18\\ud658\\uccad\\uad6c \\uad8c, \\ud574\\uc9c0\\ud658\\uae09\\uae08\\uccad\\uad6c\\uad8c, \\ucc45\\uc784\\uc900\\ube44\\uae08\\ubc18\\ud658\\uccad\\uad6c\\uad8c\\uc740 3\\ub144 \\uac04\\ud589\\uc0ac\\ud558\\uc9c0\\uc54a\\uc73c\\uba74\\uc18c\\uba78\\uc2dc\\ud6a8\\uac00\\uc644\\uc131\\ub429\\ub2c8\\ub2e4. <\\uc18c\\uba78\\uc2dc\\ud6a8> \\uc18c\\uba78\\uc2dc\\ud6a8\\ub294\\ud574\\ub2f9\\uccad\\uad6c\\uad8c\\uc744\\ud589\\uc0ac\\ud560\\uc218\\uc788\\ub294\\ub54c\\ubd80\\ud130\\uc9c4\\ud589\\ud569\\ub2c8\\ub2e4. \\ubcf4\\ud5d8\\uae08\\uc9c0 \\uae09\\uc0ac\\uc720\\uac00 2015\\ub144 4\\uc6d4 1\\uc77c\\uc5d0\\ubc1c\\uc0dd\\ud558\\uc600\\uc74c\\uc5d0\\ub3c4 2018\\ub144 4\\uc6d4 1\\uc77c\\uae4c\\uc9c0\\ubcf4\\ud5d8\\uae08\\uc744 \\uccad\\uad6c\\ud558\\uc9c0\\uc54a\\ub294\\uacbd\\uc6b0\\uc18c\\uba78\\uc2dc\\ud6a8\\uac00\\uc644\\uc131\\ub418\\uc5b4\\ubcf4\\ud5d8\\uae08\\ub4f1\\uc744\\uc9c0\\uae09\\ubc1b\\uc9c0\\ubabb\\ud560\\uc218\\uc788 \\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c27\\uc870 (\\uc900\\uc6a9\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc815\\ud558\\uc9c0\\uc54a\\uc740\\uc0ac\\ud56d\\uc740\\ubcf4\\ud1b5\\uc57d\\uad00\\uc744\\ub530\\ub985\\ub2c8 \\ub2e4. \\ub2e4\\ub9cc, \\ubcf4\\ud1b5\\uc57d\\uad00\\uc81c3\\uc870(\\uc5f0\\uae08\\uc758\\uc9c0\\uae09), \\uc81c11\\uc870(\\uacc4\\uc57d \\uc758\\uc138\\uc81c\\ud61c\\ud0dd\\ub4f1)\\uc740\\uc81c\\uc678\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"2-1. \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c1\\uc870 (\\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\uc0ac\\uc720)\", \"Head\": \"\", \"text\": \"\\ud68c\\uc0ac\\ub294\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\uae30\\uac04\\uc911\\uc5d0\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uc0c1\\ud574 \\ub85c\\uc7a5\\ud574\\ubd84\\ub958\\ud45c([\\ubcc4\\ud45c2] \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\ucc38\\uc870. \\uc774\\ud558\\uac19\\uc2b5\\ub2c8 \\ub2e4)\\uc5d0\\uc11c\\uc815\\ud55c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774 80% \\uc774\\uc0c1\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uc7a5\\ud574 \\uc0c1\\ud0dc\\uac00 \\ub418\\uc5c8\\uc744 \\ub54c\\uc5d0\\ub294\\ucc28\\ud68c\\uc774\\ud6c4\\uc758\\u300c\\ubcf4\\ud1b5\\uc57d\\uad00 \\ubcf4\\ud5d8\\uacc4 \\uc57d\\u300d(\\uc774\\ud558 \\u300c\\ubcf4\\ud1b5\\uc57d\\uad00 \\ubcf4\\ud5d8\\uacc4\\uc57d\\u300d\\uc740\\u300c\\ubcf4\\ud1b5\\uc57d\\uad00\\u300d, \\u300c\\ubcf4\\ud1b5\\uacc4\\uc57d\\uc790\\u300d\\ub294 \\u300c\\uacc4\\uc57d\\uc790\\u300d, \\u300c\\ubcf4\\ud5d8\\ud68c\\uc0ac\\u300d\\ub294\\u300c\\ud68c \\uc0ac\\u300d\\ub77c \\ud569\\ub2c8\\ub2e4)\\uc758 \\uae30\\ubcf8\\uacc4\\uc57d \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uc744 \\uba74\\uc81c\\ud569\\ub2c8 \\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"2-1. \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c2\\uc870 (\\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c\\uc5d0 \\uad00\\ud55c \\uc138\\ubd80\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\u2460 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uc774\\uba74\\uc81c\\ub41c\\uc774\\ud6c4\\uc5d0\\ub294 \\uc815\\uc0c1\\uc801\\uc73c\\ub85c\\u300c\\ubcf4\\ud1b5 \\uc57d\\uad00\\uc758\\uae30\\ubcf8\\uacc4\\uc57d\\ubcf4\\ud5d8\\ub8cc\\u300d\\uac00\\ub0a9\\uc785\\ub41c\\uac83\\uc73c\\ub85c\\ubd05\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\uc870(\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\uc0ac\\uc720)\\uc5d0\\uc11c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774\\uc0c1 \\ud574\\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 180\\uc77c \\uc774\\ub0b4\\uc5d0 \\ud655\\uc815\\ub418\\uc9c0\\uc54a\\ub294\\uacbd\\uc6b0\\uc5d0 \\ub294\\uc0c1\\ud574 \\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 180\\uc77c\\uc774 \\ub418\\ub294\\ub0a0\\uc758 \\uc758\\uc0ac\\uc9c4\\ub2e8\\uc5d0 \\uae30\\ucd08\\ud558\\uc5ec\\uace0\\uc815\\ub420\\uac83\\uc73c\\ub85c\\uc778\\uc815\\ub418\\ub294\\uc0c1\\ud0dc\\ub97c\\uc7a5\\ud574\\uc9c0\\uae09 \\ub960\\ub85c\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc5d0 \\uc7a5\\ud574\\ud310\\uc815\\uc2dc \\uae30\\ub97c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. \\u2462 \\uc81c2\\ud56d\\uc5d0 \\ub530\\ub77c \\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774 \\uacb0\\uc815\\ub418\\uc5c8\\uc73c\\ub098 \\uadf8 \\uc774\\ud6c4 \\ubcf4\\uc7a5\\uc744\\ubc1b\\uc744\\uc218\\uc788\\ub294\\uae30\\uac04(\\uacc4\\uc57d\\uc758\\ud6a8\\ub825\\uc774\\uc5c6\\uc5b4\\uc9c4\\uacbd \\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae30\\uac04\\uc774 10\\ub144 \\uc774\\uc0c1\\uc778\\uacc4\\uc57d\\uc740\\uc0c1\\ud574\\ubc1c\\uc0dd\\uc77c \\ubd80\\ud130 2\\ub144\\uc774\\ub0b4\\ub85c\\ud558\\uace0,\\ubcf4\\ud5d8\\uae30\\uac04\\uc774 10\\ub144\\ubbf8\\ub9cc\\uc778\\uacc4\\uc57d \\uc740\\uc0c1\\ud574\\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 1\\ub144\\uc774\\ub0b4)\\uc5d0\\uc7a5\\ud574\\uc0c1\\ud0dc\\uac00\\ub354\\uc545\\ud654 \\ub418\\ub294\\ub54c\\uc5d0\\ub294\\uadf8\\uc545\\ud654\\ub41c\\uc7a5\\ud574\\uc0c1\\ud0dc\\ub97c\\uae30\\uc900\\uc73c\\ub85c\\uc7a5\\ud574\\uc9c0 \\uae09\\ub960\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\u2463 \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc5d0\\ud574\\ub2f9\\ub418\\uc9c0 \\uc54a\\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790 \\uc758 \\uc9c1\\uc5c5, \\uc5f0\\ub839, \\uc2e0\\ubd84 \\ub610\\ub294 \\uc131\\ubcc4 \\ub4f1\\uc5d0 \\uad00\\uacc4\\uc5c6\\uc774 \\uc2e0\\uccb4 \\uc758\\uc7a5\\ud574\\uc815\\ub3c4\\uc5d0\\ub530\\ub77c\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uad6c\\ubd84\\uc5d0\\uc900\\ud558\\uc5ec\\uc9c0 \\uae09\\uc561\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc7a5\\ud574\\ubd84 \\ub958\\ubcc4\\ucd5c\\uc800\\uc9c0\\uae09\\ub960\\uc7a5\\ud574\\uc815\\ub3c4\\uc5d0\\uc774\\ub974\\uc9c0\\uc54a\\ub294\\ud6c4\\uc720\\uc7a5\\ud574 \\uc5d0\\ub300\\ud558\\uc5ec\\ub294\\ubcf4\\ud5d8\\ub8cc\\uc758\\ub0a9\\uc785\\uc744\\uba74\\uc81c\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2464 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc640\\ud68c\\uc0ac\\uac00\\uc81c1\\uc870(\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\uc0ac\\uc720) \\uc758\\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\uc0ac\\uc720\\uc5d0\\ub300\\ud574\\ud569\\uc758\\ud558\\uc9c0\\ubabb\\ud560\\ub54c\\uc5d0 \\ub294\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc640\\ud68c\\uc0ac\\uac00\\ud568\\uaed8\\uc81c3\\uc790\\ub97c\\uc815\\ud558\\uace0\\uadf8\\uc81c3 \\uc790\\uc758\\uc758\\uacac\\uc5d0\\ub530\\ub97c\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\uc81c3\\uc790\\ub294\\uc758\\ub8cc\\ubc95\\uc81c3 \\uc870(\\uc758\\ub8cc\\uae30\\uad00)\\uc5d0 \\uaddc\\uc815\\ud55c\\uc885\\ud569\\ubcd1\\uc6d0\\uc18c\\uc18d\\uc804\\ubb38\\uc758\\uc911\\uc5d0 \\uc815\\ud558\\uba70, \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\uc0ac\\uc720 \\ud310\\uc815\\uc5d0\\ub4dc\\ub294\\uc758\\ub8cc\\ube44 \\uc6a9\\uc740\\ud68c\\uc0ac\\uac00\\uc804\\uc561\\ubd80\\ub2f4\\ud569\\ub2c8\\ub2e4. \\u2465 \\uac19\\uc740\\uc0c1\\ud574\\ub85c\\ub450\\uac00\\uc9c0\\uc774\\uc0c1\\uc758\\ud6c4\\uc720\\uc7a5\\ud574\\uac00\\uc0dd\\uae34\\uacbd\\uc6b0\\uc5d0 \\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc744\\ud569\\uc0b0\\ud558\\uc5ec\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc2e0\\uccb4\\ubd80\\uc704\\ubcc4\\ud310\\uc815\\uae30\\uc900\\uc5d0\\ubcc4\\ub3c4\\ub85c\\uc815 \\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uae30\\uc900\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. \\u2466 \\ub2e4\\ub978 \\uc0c1\\ud574\\ub85c\\uc778\\ud558\\uc5ec\\ud6c4\\uc720\\uc7a5\\ud574\\uac00 2\\ud68c \\uc774\\uc0c1 \\ubc1c\\uc0dd\\ud558\\uc600 \\uc744\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\ub54c\\ub9c8\\ub2e4\\uc774\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09 \\ub960\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc2e0\\uccb4\\ubd80\\uc704 \\ubcc4\\ud310\\uc815\\uae30\\uc900\\uc5d0\\uc11c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uae30\\uc900\\uc5d0\\ub530 \\ub985\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"2-1. \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c3\\uc870 (\\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uc744 \\uba74\\uc81c\\ud558\\uc9c0 \\uc54a\\ub294 \\uc0ac\\uc720)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294 \\ub2e4\\uc74c\\uc911 \\uc5b4\\ub290\\ud55c \\uac00\\uc9c0\\uc758 \\uacbd\\uc6b0\\uc5d0\\uc758\\ud558\\uc5ec\\ubcf4\\ud5d8 \\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\ub54c\\uc5d0\\ub294\\ubcf4\\ud5d8\\ub8cc\\uc758\\ub0a9\\uc785\\uc744 \\uba74\\uc81c\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. 1.\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uace0\\uc758\\ub85c\\uc790\\uc2e0\\uc744\\ud574\\uce5c\\uacbd\\uc6b0.\\ub2e4\\ub9cc, \\ud53c\\ubcf4 \\ud5d8\\uc790\\uac00\\uc2ec\\uc2e0\\uc0c1\\uc2e4\\ub4f1\\uc73c\\ub85c\\uc790\\uc720\\ub85c\\uc6b4\\uc758\\uc0ac\\uacb0\\uc815\\uc744\\ud560 \\uc218 \\uc5c6\\ub294 \\uc0c1\\ud0dc\\uc5d0\\uc11c \\uc790\\uc2e0\\uc744 \\ud574\\uce5c \\uacbd\\uc6b0\\uc5d0\\ub294 \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uc744\\uba74\\uc81c\\ud569\\ub2c8\\ub2e4. 2.\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00\\uace0\\uc758\\ub85c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c\\ud574\\uce5c\\uacbd\\uc6b0. 3.\\uacc4\\uc57d\\uc790\\uac00\\uace0\\uc758\\ub85c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c\\ud574\\uce5c\\uacbd\\uc6b0 4.\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc784\\uc2e0, \\ucd9c\\uc0b0(\\uc81c\\uc655\\uc808\\uac1c\\ub97c \\ud3ec\\ud568\\ud569\\ub2c8 \\ub2e4), \\uc0b0\\ud6c4\\uae30. \\uadf8\\ub7ec\\ub098 \\ud68c\\uc0ac\\uac00\\ubcf4\\uc7a5\\ud558\\ub294\\ubcf4\\ud5d8\\ub8cc\\ub0a9 \\uc785\\uba74\\uc81c\\uc0ac\\uc720\\ub85c\\uc778\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\ub0a9\\uc785\\uc744\\uba74\\uc81c\\ud569 \\ub2c8\\ub2e4. 5.\\uc804\\uc7c1,\\uc678\\uad6d\\uc758\\ubb34\\ub825\\ud589\\uc0ac,\\ud601\\uba85,\\ub0b4\\ub780,\\uc0ac\\ubcc0,\\ud3ed\\ub3d9 \\u2461 \\ud68c\\uc0ac\\ub294 \\ub2e4\\ub978\\uc57d\\uc815\\uc774 \\uc5c6\\uc73c\\uba74 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\uc9c1\\uc5c5, \\uc9c1\\ubb34 \\ub610\\ub294\\ub3d9\\ud638\\ud68c\\ud65c\\ub3d9\\ubaa9\\uc801\\uc73c\\ub85c\\uc544\\ub798\\uc5d0\\uc5f4\\uac70\\ub41c\\ud589\\uc704\\ub85c\\uc778 \\ud558\\uc5ec\\uc81c1\\uc870(\\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c\\uc0ac\\uc720)\\uc758 \\uc0c1\\ud574 \\uad00\\ub828\\ubcf4 \\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\ub54c\\uc5d0\\ub294\\ud574\\ub2f9\\ubcf4\\ud5d8 \\ub8cc\\ub0a9\\uc785\\uc744\\uba74\\uc81c\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. 1.\\uc804\\ubb38\\ub4f1\\ubc18(\\uc804\\ubb38\\uc801\\uc778\\ub4f1\\uc0b0\\uc6a9\\uad6c\\ub97c\\uc0ac\\uc6a9\\ud558\\uc5ec\\uc554\\ubcbd\\ub610 \\ub294\\ube59\\ubcbd\\uc744\\uc624\\ub974\\ub0b4\\ub9ac\\uac70\\ub098\\ud2b9\\uc218\\ud55c\\uae30\\uc220,\\uacbd\\ud5d8, \\uc0ac\\uc804 \\ud6c8\\ub828\\uc744\\ud544\\uc694\\ub85c\\ud558\\ub294\\ub4f1\\ubc18\\uc744\\ub9d0\\ud569\\ub2c8\\ub2e4), \\uae00\\ub77c\\uc774\\ub354 \\uc870\\uc885, \\uc2a4\\uce74\\uc774\\ub2e4\\uc774\\ube59, \\uc2a4\\ucfe0\\ubc84\\ub2e4\\uc774\\ube59, \\ud589\\uae00\\ub77c\\uc774 \\ub529,\\uc218\\uc0c1\\ubcf4\\ud2b8,\\ud328\\ub7ec\\uae00\\ub77c\\uc774\\ub529 2.\\ubaa8\\ud130\\ubcf4\\ud2b8, \\uc790\\ub3d9\\ucc28\\ub610\\ub294\\uc624\\ud1a0\\ubc14\\uc774\\uc5d0\\uc758\\ud55c\\uacbd\\uae30,\\uc2dc \\ubc94, \\ud765\\ud589(\\uc774\\ub97c \\uc704\\ud55c\\uc5f0\\uc2b5\\uc744\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4) \\ub610\\ub294\\uc2dc \\uc6b4\\uc804(\\ub2e4\\ub9cc, \\uacf5\\uc6a9\\ub3c4\\ub85c\\uc0c1\\uc5d0\\uc11c\\uc2dc\\uc6b4\\uc804\\uc744\\ud558\\ub294\\ub3d9\\uc548 \\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\uc7a5\\ud569\\ub2c8\\ub2e4) 3.\\uc120\\ubc15\\uc2b9\\ubb34\\uc6d0, \\uc5b4\\ubd80, \\uc0ac\\uacf5, \\uadf8 \\ubc16\\uc5d0 \\uc120\\ubc15\\uc5d0\\ud0d1\\uc2b9\\ud558 \\ub294 \\uac83\\uc744 \\uc9c1\\ubb34\\ub85c \\ud558\\ub294 \\uc0ac\\ub78c\\uc774 \\uc9c1\\ubb34\\uc0c1 \\uc120\\ubc15\\uc5d0 \\ud0d1\\uc2b9 \\ud558\\uace0\\uc788\\ub294\\ub3d9\\uc548\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"2-1. \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c4\\uc870 (\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\uc18c\\uba78)\", \"Head\": \"\", \"text\": \"\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ubcf4\\ud5d8\\uc99d\\uad8c\\uc5d0\\uae30\\uc7ac\\ub41c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\uae30 \\uac04 \\uc911\\uc5d0 \\uc0ac\\ub9dd\\ud558\\uc600\\uc744 \\uacbd\\uc6b0\\uc5d0\\ub294 \\\"\\ubcf4\\ud5d8\\ub8cc \\ubc0f \\ucc45\\uc784\\uc900\\ube44\\uae08 \\uc0b0\\ucd9c\\ubc29\\ubc95\\uc11c\\\"\\uc5d0\\uc11c\\uc815\\ud558\\ub294\\ubc14\\uc5d0\\ub530\\ub77c\\ud68c\\uc0ac\\uac00\\uc801\\ub9bd\\ud55c\\uc0ac\\ub9dd \\ub2f9\\uc2dc\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ucc45\\uc784\\uc900\\ube44\\uae08\\uc744\\uacc4\\uc57d\\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud558 \\uace0,\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc740\\ub354\\uc774\\uc0c1\\ud6a8\\ub825\\uc774\\uc5c6\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"2. \\ubcf4\\ud5d8\\ub8cc\\ub0a9\\uc785\\uba74\\uc81c\\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"2-1. \\ubcf4\\ud5d8\\ub8cc \\ub0a9\\uc785\\uba74\\uc81c \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c5\\uc870 (\\uc900\\uc6a9\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc815\\ud558\\uc9c0\\uc54a\\uc740\\uc0ac\\ud56d\\uc740\\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80% \\uc774\\uc0c1)\\uc0dd\\ud65c\\uc790\\uae08\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ub530\\ub974\\uba70, \\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80% \\uc774\\uc0c1)\\uc0dd\\ud65c\\uc790\\uae08\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc11c\\uc815\\ud558\\uc9c0\\uc54a\\ub294\\uc0ac\\ud56d\\uc740\\ubcf4\\ud1b5 \\uc57d\\uad00\\uc744\\ub530\\ub985\\ub2c8\\ub2e4. [\\ubcc4\\ud45c1]\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud560\\ub54c\\uc758\\uc801\\ub9bd\\uc774\\uc728\\uacc4\\uc0b0\", \"subtitle\": \"\", \"subsubtitle\": \"\", \"Head\": \"\", \"text\": \"\\uc8fc) 1. \\ud68c\\uc0ac\\uac00\\uc5f0\\uae08\\uc758\\uc9c0\\uae09\\uc2dc\\uae30\\ub3c4\\ub798 7\\uc77c\\uc774\\uc804\\uc5d0\\uc9c0\\uae09\\ud560\\uc0ac\\uc720\\uc640\\uae08\\uc561\\uc744\\uc54c\\ub9ac\\uc9c0\\uc54a \\uc740\\uacbd\\uc6b0, \\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\ub0a0\\uc758\\ub2e4\\uc74c\\ub0a0\\ubd80\\ud130\\uccad\\uad6c\\uc77c\\uae4c\\uc9c0\\uc758\\uae30\\uac04\\uc740\\uacf5\\uc2dc \\uc774\\uc728\\uc744\\uc801\\uc6a9\\ud55c\\uc774\\uc790\\ub97c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. 2. \\uc5f0\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ubc1c\\uc0dd\\uc77c\\uc774\\uc804\\uc5d0\\uc5f0\\uae08\\uc744\\uccad\\uad6c\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc5f0\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ubc1c\\uc0dd \\uc77c\\uae4c\\uc9c0\\ub294\\uacf5\\uc2dc\\uc774\\uc728\\uc744\\uc801\\uc6a9\\ud55c\\uc774\\uc790\\ub97c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. 3. \\uc9c0\\uae09\\uc774\\uc790\\uc758\\uacc4\\uc0b0\\uc740\\uc5f0\\ub2e8\\uc704\\ubcf5\\ub9ac\\ub85c\\uacc4\\uc0b0\\ud558\\uba70,\\uae08\\ub9ac\\uc5f0\\ub3d9\\ud615\\ubcf4\\ud5d8\\uc740\\uc77c\\uc790\\uacc4\\uc0b0 \\ud569\\ub2c8\\ub2e4. 4. \\uacc4\\uc57d\\uc790\\ub4f1\\uc758\\ucc45\\uc784\\uc788\\ub294\\uc0ac\\uc720\\ub85c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc774\\uc9c0\\uc5f0\\ub41c\\ub54c\\uc5d0\\ub294\\uadf8\\ud574\\ub2f9\\uae30\\uac04 \\uc5d0\\ub300\\ud55c\\uc774\\uc790\\ub294\\uc9c0\\uae09\\ub418\\uc9c0\\uc54a\\uc744\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"\", \"subsubtitle\": \"<\\ucd1d\\uce59> 1. \\uc7a5\\ud574\\uc758\\uc815\\uc758\", \"Head\": \"\", \"text\": \"\\uac00.\\u201c\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740\\uc0c1\\ud574\\ub610\\ub294\\uc9c8\\ubcd1\\uc5d0\\ub300\\ud558\\uc5ec\\uce58\\uc720\\ub41c\\ud6c4\\uc2e0\\uccb4\\uc5d0\\ub0a8\\uc544\\uc788\\ub294\\uc601\\uad6c \\uc801\\uc778\\uc815\\uc2e0\\ub610\\ub294\\uc721\\uccb4\\uc758\\ud6fc\\uc190\\uc0c1\\ud0dc\\ub97c\\ub9d0\\ud55c\\ub2e4. \\ub2e4\\ub9cc, \\uc9c8\\ubcd1\\uacfc \\ubd80\\uc0c1\\uc758\\uc8fc\\uc99d\\uc0c1\\uacfc \\ud569\\ubcd1\\uc99d\\uc0c1\\ubc0f\\uc774\\uc5d0\\ub300\\ud55c\\uce58\\ub8cc\\ub97c\\ubc1b\\ub294\\uacfc\\uc815\\uc5d0\\uc11c\\uc77c\\uc2dc\\uc801\\uc73c\\ub85c\\ub098\\ud0c0\\ub098\\ub294\\uc99d\\uc0c1\\uc740 \\uc7a5\\ud574\\uc5d0\\ud3ec\\ud568\\ub418\\uc9c0\\uc54a\\ub294\\ub2e4. \\ub098.\\u201c\\uc601\\uad6c\\uc801\\u201d\\uc774\\ub77c\\ud568\\uc740\\uc6d0\\uce59\\uc801\\uc73c\\ub85c\\uce58\\uc720\\ud558\\ub294\\ub54c\\uc7a5\\ub798\\ud68c\\ubcf5\\ud560\\uac00\\ub9dd\\uc774\\uc5c6\\ub294\\uc0c1\\ud0dc \\ub85c\\uc11c \\uc815\\uc2e0\\uc801 \\ub610\\ub294 \\uc721\\uccb4\\uc801\\ud6fc\\uc190\\uc0c1\\ud0dc\\uc784\\uc774 \\uc758\\ud559\\uc801\\uc73c\\ub85c \\uc778\\uc815\\ub418\\ub294 \\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c \\ub2e4. \\ub2e4.\\u201c\\uce58\\uc720\\ub41c\\ud6c4\\u201d\\ub77c\\ud568\\uc740\\uc0c1\\ud574\\ub610\\ub294\\uc9c8\\ubcd1\\uc5d0\\ub300\\ud55c\\uce58\\ub8cc\\uc758\\ud6a8\\uacfc\\ub97c\\uae30\\ub300\\ud560\\uc218\\uc5c6\\uac8c \\ub418\\uace0\\ub610\\ud55c\\uadf8\\uc99d\\uc0c1\\uc774\\uace0\\uc815\\ub41c\\uc0c1\\ud0dc\\ub97c\\ub9d0\\ud55c\\ub2e4. \\ub77c.\\ub2e4\\ub9cc,\\uc601\\uad6c\\ud788\\uace0\\uc815\\ub41c\\uc99d\\uc0c1\\uc740\\uc544\\ub2c8\\uc9c0\\ub9cc\\uce58\\ub8cc\\uc885\\uacb0\\ud6c4\\ud55c\\uc2dc\\uc801\\uc73c\\ub85c\\ub098\\ud0c0\\ub098\\ub294\\uc7a5 \\ud574\\uc5d0\\ub300\\ud558\\uc5ec\\ub294\\uadf8\\uae30\\uac04\\uc774 5\\ub144 \\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\ud574\\ub2f9\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc758 20%\\ub97c \\ubcf4\\ud5d8\\uac00 \\uc785\\uae08\\uc561\\uc5d0\\uacf1\\ud558\\uc5ec\\uc0b0\\ucd9c\\ud55c\\uae08\\uc561\\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"\", \"subsubtitle\": \"2. \\uc2e0\\uccb4\\ubd80\\uc704\", \"Head\": \"\", \"text\": \"\\u201c\\uc2e0\\uccb4\\ubd80\\uc704\\u201d\\ub77c\\ud568\\uc740\\u2460\\ub208\\u2461\\uadc0\\u2462\\ucf54\\u2463\\uc539\\uc5b4\\uba39\\uac70\\ub098\\ub9d0\\ud558\\ub294\\uae30\\ub2a5\\u2464\\uc678\\ubaa8\\u2465 \\ucc99\\ucd94(\\ub4f1\\ubf08) \\u2466\\uccb4\\uac04\\uace8\\u2467\\ud314\\u2468\\ub2e4\\ub9ac\\u2469\\uc190\\uac00\\ub77d\\u246a\\ubc1c\\uac00\\ub77d\\u246b\\ud749\\u00b7\\ubcf5\\ubd80\\uc7a5\\uae30\\ubc0f \\ube44\\ub1e8\\uc0dd\\uc2dd\\uae30\\u246c\\uc2e0\\uacbd\\uacc4\\u00b7\\uc815\\uc2e0\\ud589\\ub3d9\\uc758 13\\uac1c\\ubd80\\uc704\\ub97c\\ub9d0\\ud558\\uba70,\\uc774\\ub97c\\uac01\\uac01\\ub3d9\\uc77c\\ud55c\\uc2e0 \\uccb4\\ubd80\\uc704\\ub77c\\ud55c\\ub2e4. \\ub2e4\\ub9cc, \\uc88c\\u00b7\\uc6b0\\uc758\\ub208, \\uadc0, \\ud314, \\ub2e4\\ub9ac\\ub294\\uac01\\uac01\\ub2e4\\ub978\\uc2e0\\uccb4\\ubd80\\uc704\\ub85c\\ubcf8 \\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"\", \"subsubtitle\": \"3. \\uae30\\ud0c0\", \"Head\": \"\", \"text\": \"\\uac00.\\ud558\\ub098\\uc758\\uc7a5\\ud574\\uac00\\uad00\\ucc30\\ubc29\\ubc95\\uc5d0\\ub530\\ub77c\\uc11c\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc0c1 2\\uac00\\uc9c0\\uc774\\uc0c1\\uc758\\uc2e0\\uccb4\\ubd80\\uc704\\ub610 \\ub294\\ub3d9\\uc77c\\ud55c\\uc2e0\\uccb4\\ubd80\\uc704\\uc5d0\\uc11c, \\ud558\\ub098\\uc758\\uc7a5\\ud574\\uc5d0\\ub2e4\\ub978\\uc7a5\\ud574\\uac00\\ud1b5\\uc0c1\\ud30c\\uc0dd\\ud558\\ub294\\uad00\\uacc4\\uc5d0 \\uc788\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uac01\\uac01\\uadf8\\uc911\\ub192\\uc740\\uc9c0\\uae09\\ub960\\ub9cc\\uc744\\uc801\\uc6a9\\ud55c\\ub2e4. \\ub098.\\ub3d9\\uc77c\\ud55c\\uc2e0\\uccb4\\ubd80\\uc704\\uc5d0 2\\uac00\\uc9c0\\uc774\\uc0c1\\uc758\\uc7a5\\ud574\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ub354\\ud558\\uc9c0\\uc54a\\uace0\\uadf8\\uc911 \\ub192\\uc740\\uc9c0\\uae09\\ub960\\uc744\\uc801\\uc6a9\\ud568\\uc744\\uc6d0\\uce59\\uc73c\\ub85c\\ud55c\\ub2e4. \\uadf8\\ub7ec\\ub098\\uac01\\uc2e0\\uccb4\\ubd80\\uc704\\ubcc4\\ud310\\uc815\\uae30\\uc900\\uc5d0 \\uc11c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uae30\\uc900\\uc5d0\\ub530\\ub978\\ub2e4. \\ub2e4.\\uc758\\ud559\\uc801\\uc73c\\ub85c\\ub1cc\\uc0ac\\ud310\\uc815\\uc744\\ubc1b\\uace0\\ud638\\ud761\\uae30\\ub2a5\\uacfc\\uc2ec\\uc7a5\\ubc15\\ub3d9\\uae30\\ub2a5\\uc744\\uc0c1\\uc2e4\\ud558\\uc5ec\\uc778\\uacf5\\uc2ec \\ubc15\\ub3d9\\uae30\\ub4f1\\uc7a5\\uce58\\uc5d0\\uc758\\uc874\\ud558\\uc5ec\\uc0dd\\uba85\\uc744\\uc5f0\\uc7a5\\ud558\\uace0\\uc788\\ub294\\ub1cc\\uc0ac\\uc0c1\\ud0dc\\ub294\\uc7a5\\ud574\\uc758\\ud310\\uc815\\ub300 \\uc0c1\\uc5d0\\ud3ec\\ud568\\ub418\\uc9c0\\uc54a\\ub294\\ub2e4. \\ub77c.\\uc7a5\\ud574\\uc9c4\\ub2e8\\uc11c\\uc5d0\\ub294\\u2460\\uc7a5\\ud574\\uc9c4\\ub2e8\\uba85\\ubc0f\\ubc1c\\uc0dd\\uc2dc\\uae30\\u2461\\uc7a5\\ud574\\uc758\\ub0b4\\uc6a9\\uacfc\\uadf8\\uc815\\ub3c4\\u2462\\uc0ac \\uace0\\uc640\\uc758\\uc778\\uacfc\\uad00\\uacc4\\ubc0f\\uc0ac\\uace0\\uc758\\uad00\\uc5ec\\ub3c4\\u2463\\ud5a5\\ud6c4\\uce58\\ub8cc\\uc758\\ubb38\\uc81c\\ubc0f\\ud638\\uc804\\ub3c4\\ub97c\\ud544\\uc218\\uc801 \\uc73c\\ub85c\\uae30\\uc7ac\\ud574\\uc57c\\ud55c\\ub2e4. \\ub2e4\\ub9cc, \\uc2e0\\uacbd\\uacc4\\u318d\\uc815\\uc2e0\\ud589\\ub3d9\\uc7a5\\ud574\\uc758\\uacbd\\uc6b0\\u2460\\uac1c\\ud638(\\uc7a5\\ud574\\ub85c \\ud63c\\uc790\\uc11c\\ud65c\\ub3d9\\uc774\\uc5b4\\ub824\\uc6b4\\uc0ac\\ub78c\\uc744\\uacc1\\uc5d0\\uc11c\\ub3cc\\ubcf4\\ub294\\uac83)\\uc5ec\\ubd80\\u2461\\uac1d\\uad00\\uc801\\uc774\\uc720\\ubc0f\\uac1c\\ud638 \\uc758\\ub0b4\\uc6a9\\uc744\\ucd94\\uac00\\ub85c\\uae30\\uc7ac\\ud558\\uc5ec\\uc57c\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"1. \\ub208\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\uc2dc\\ub825\\uc7a5\\ud574\\uc758\\uacbd\\uc6b0\\uacf5\\uc778\\ub41c\\uc2dc\\ub825\\uac80\\uc0ac\\ud45c\\uc5d0\\ub530\\ub77c\\uce21\\uc815\\ud55c\\ub2e4. 2) \\u201c\\uad50\\uc815\\uc2dc\\ub825\\u201d\\uc774\\ub77c\\ud568\\uc740\\uc548\\uacbd(\\ucf58\\ud0dd\\ud2b8\\ub80c\\uc988\\ub97c\\ud3ec\\ud568\\ud55c\\ubaa8\\ub4e0\\uc885\\ub958\\uc758\\uc2dc\\ub825\\uad50\\uc815 \\uc218\\ub2e8)\\uc73c\\ub85c\\uad50\\uc815\\ud55c\\uc2dc\\ub825\\uc744\\ub9d0\\ud55c\\ub2e4. 3) \\u201c\\ud55c \\ub208\\uc774\\uba40\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ub208\\ub3d9\\uc790\\uc758\\uc801\\ucd9c\\uc740\\ubb3c\\ub860\\uba85\\uc554\\uc744\\uac00\\ub9ac\\uc9c0\\ubabb\\ud558 \\uac70\\ub098(\\u201c\\uad11\\uac01\\ubb34\\u201d)\\uaca8\\uc6b0\\uac00\\ub9b4\\uc218\\uc788\\ub294\\uacbd\\uc6b0(\\u201c\\uad11\\uac01\\u201d)\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\uc548\\uad6c\\uc6b4\\ub3d9\\uc7a5\\ud574\\uc758\\ud310\\uc815\\uc740\\uc678\\uc0c1\\ud6c4 1\\ub144\\uc774\\uc0c1\\uc774\\uc9c0\\ub09c\\ub4a4\\uc5d0\\uadf8\\uc7a5\\ud574\\uc815\\ub3c4\\ub97c\\ud3c9\\uac00 \\ud55c\\ub2e4. 5) \\u201c\\uc548\\uad6c\\uc758\\ub69c\\ub837\\ud55c\\uc6b4\\ub3d9\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740\\uc548\\uad6c\\uc758\\uc8fc\\uc2dc\\uc57c(\\uba38\\ub9ac\\ub97c\\uc6c0\\uc9c1\\uc774\\uc9c0\\uc54a\\uace0 \\ub208\\ub9cc\\uc744\\uc6c0\\uc9c1\\uc5ec\\uc11c\\ubcfc\\uc218\\uc788\\ub294\\ubc94\\uc704)\\uc758 \\uc6b4\\ub3d9\\ubc94\\uc704\\uac00\\uc815\\uc0c1\\uc758 1/2 \\uc774\\ud558\\ub85c\\uac10\\uc18c \\ub41c\\uacbd\\uc6b0\\ub098\\uc815\\uba74\\uc591\\uc548\\uc2dc(\\ub450\\ub208\\uc73c\\ub85c\\ud558\\ub098\\uc758\\uc0ac\\ubb3c\\uc744\\ubcf4\\ub294\\uac83)\\uc5d0\\uc11c\\ubcf5\\uc2dc(\\ubb3c\\uccb4 \\uac00\\ub458\\ub85c\\ubcf4\\uc774\\uac70\\ub098\\uacb9\\uccd0\\ubcf4\\uc784)\\ub97c\\ub0a8\\uae34\\ub54c\\ub97c\\ub9d0\\ud55c\\ub2e4. 6) \\u201c\\uc548\\uad6c\\uc758\\ub69c\\ub837\\ud55c\\uc870\\uc808\\uae30\\ub2a5\\uc7a5\\ud574\\u201c\\ub77c\\ud568\\uc740\\uc870\\uc808\\ub825\\uc774\\uc815\\uc0c1\\uc758 1/2 \\uc774\\ud558\\ub85c\\uac10 \\uc18c\\ub41c\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. \\ub2e4\\ub9cc, \\uc870\\uc808\\ub825\\uc758\\uac10\\uc18c\\ub97c\\ubb34\\uc2dc\\ud560\\uc218\\uc788\\ub294 45\\uc138 \\uc774\\uc0c1\\uc758 \\uacbd\\uc6b0\\uc5d0\\ub294\\uc81c\\uc678\\ud55c\\ub2e4. 7) \\u201c\\uc2dc\\uc57c\\uac00 \\uc881\\uc544\\uc9c4\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc2dc\\uc57c\\uac01\\ub3c4\\uc758 \\ud569\\uacc4\\uac00 \\uc815\\uc0c1\\uc2dc\\uc57c\\uc758 60%\\uc774\\ud558\\ub85c \\uc81c\\ud55c\\ub41c\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 8) \\u201c\\ub208\\uaebc\\ud480\\uc5d0\\ub69c\\ub837\\ud55c\\uacb0\\uc190\\uc744\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ub208\\uaebc\\ud480\\uc758\\uacb0\\uc190\\uc73c\\ub85c\\uc778\\ud574\\ub208\\uc744 \\uac10\\uc558\\uc744\\ub54c\\uac01\\ub9c9(\\uac80\\uc740\\uc790\\uc704)\\uc774\\uc644\\uc804\\ud788\\ub36e\\uc774\\uc9c0\\uc54a\\ub294\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 9) \\u201c\\ub208\\uaebc\\ud480\\uc5d0\\ub69c\\ub837\\ud55c\\uc6b4\\ub3d9\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201c\\ub77c\\ud568\\uc740\\ub208\\uc744\\ub5b4\\uc744\\ub54c\\ub3d9\\uacf5\\uc744 1/2 \\uc774\\uc0c1 \\ub36e\\uac70\\ub098\\ub610\\ub294\\ub208\\uc744\\uac10\\uc558\\uc744\\ub54c\\uac01\\ub9c9\\uc744\\uc644\\uc804\\ud788\\ub36e\\uc744\\uc218\\uc5c6\\ub294\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c \\ub2e4. 10)\\uc678\\uc0c1\\uc774\\ub098\\ud654\\uc0c1\\ub4f1\\uc73c\\ub85c\\ub208\\ub3d9\\uc790\\uc758\\uc801\\ucd9c\\uc774\\ubd88\\uac00\\ud53c\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc678\\ubaa8\\uc758\\ucd94\\uc0c1 (\\ucd94\\ud55c\\ubaa8\\uc2b5)\\uc774 \\uac00\\uc0b0\\ub41c\\ub2e4. \\uc774 \\uacbd\\uc6b0\\ub208\\ub3d9\\uc790\\uac00\\uc801\\ucd9c\\ub418\\uc5b4\\ub208\\uc790\\uc704\\uc758\\uc870\\uc9c1\\uc694\\ubab0 (\\u51f9\\u6c92) \\ub4f1\\uc73c\\ub85c\\uc758\\uc548\\ub9c8\\uc800\\ub07c\\uc6cc\\ub123\\uc744\\uc218\\uc5c6\\ub294\\uc0c1\\ud0dc\\uc774\\uba74\\u201c\\ub69c\\ub837\\ud55c\\ucd94\\uc0c1(\\ucd94\\ud55c \\ubaa8\\uc2b5)\\u201d\\uc73c\\ub85c, \\uc758\\uc548\\uc744\\ub07c\\uc6cc\\ub123\\uc744\\uc218\\uc788\\ub294\\uc0c1\\ud0dc\\uc774\\uba74\\u201c\\uc57d\\uac04\\uc758\\ucd94\\uc0c1(\\ucd94\\ud55c\\ubaa8 \\uc2b5)\\u201d\\uc73c\\ub85c\\uc9c0\\uae09\\ub960\\uc744\\uac00\\uc0b0\\ud55c\\ub2e4. 11)\\u201c\\ub208\\uaebc\\ud480\\uc5d0\\ub69c\\ub837\\ud55c\\uacb0\\uc190\\uc744\\ub0a8\\uae34\\ub54c\\u201d\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ucd94\\uc0c1(\\ucd94\\ud55c\\ubaa8 \\uc2b5)\\uc7a5\\ud574\\ub97c\\ud3ec\\ud568\\ud558\\uc5ec\\uc7a5\\ud574\\ub97c\\ud3c9\\uac00\\ud55c\\uac83\\uc73c\\ub85c\\ubcf4\\uace0\\ucd94\\uc0c1(\\ucd94\\ud55c \\ubaa8\\uc2b5)\\uc7a5\\ud574\\ub97c \\uac00\\uc0b0\\ud558\\uc9c0\\uc54a\\ub294\\ub2e4.\\ub2e4\\ub9cc, \\uc548\\uba74\\ubd80\\uc758\\ucd94\\uc0c1(\\ucd94\\ud55c\\ubaa8\\uc2b5)\\uc740\\ub450\\uac00\\uc9c0\\uc7a5\\ud574\\ud3c9\\uac00\\ubc29 \\ubc95\\uc911\\ud53c\\ubcf4\\ud5d8\\uc790\\uc5d0\\uac8c\\uc720\\ub9ac\\ud55c\\uac83\\uc744\\uc801\\uc6a9\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"2. \\uadc0\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\uccad\\ub825\\uc7a5\\ud574\\ub294\\uc21c\\uc74c\\uccad\\ub825\\uac80\\uc0ac\\uacb0\\uacfc\\uc5d0\\ub530\\ub77c\\ub370\\uc2dc\\ubca8(dB : decibel)\\ub85c\\uc11c\\ud45c\\uc2dc\\ud558 \\uace0 3\\ud68c\\uc774\\uc0c1\\uccad\\ub825\\uac80\\uc0ac\\ub97c\\uc2e4\\uc2dc\\ud55c\\ud6c4\\uc21c\\uc74c\\ud3c9\\uade0\\uc5ed\\uce58\\uc5d0\\ub530\\ub77c\\uc801\\uc6a9\\ud55c\\ub2e4. 2) \\u201c\\ud55c\\uadc0\\uc758\\uccad\\ub825\\uc744\\uc644\\uc804\\ud788\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc21c\\uc74c\\uccad\\ub825\\uac80\\uc0ac\\uacb0\\uacfc\\ud3c9\\uade0\\uc21c\\uc74c \\uc5ed\\uce58\\uac00 90dB\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 3) \\u201c\\uc2ec\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc21c\\uc74c\\uccad\\ub825\\uac80\\uc0ac\\uacb0\\uacfc\\ud3c9\\uade0\\uc21c\\uc74c\\uc5ed\\uce58\\uac00 80dB \\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\uc5d0\\ud574\\ub2f9\\ub418\\uc5b4, \\uadc0\\uc5d0\\ub2e4\\ub300\\uace0\\ub9d0\\ud558\\uc9c0\\uc54a\\uace0\\ub294\\ud070\\uc18c\\ub9ac\\ub97c\\uc54c\\uc544\\ub4e3\\uc9c0 \\ubabb\\ud558\\ub294\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\u201c\\uc57d\\uac04\\uc758 \\uc7a5\\ud574\\ub97c \\ub0a8\\uae34 \\ub54c\\u201d\\ub77c \\ud568\\uc740 \\uc21c\\uc74c\\uccad\\ub825\\uac80\\uc0ac \\uacb0\\uacfc \\ud3c9\\uade0\\uc21c\\uc74c\\uc5ed\\uce58\\uac00 70dB\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\uc5d0\\ud574\\ub2f9\\ub418\\uc5b4, 50cm\\uc774\\uc0c1\\uc758\\uac70\\ub9ac\\uc5d0\\uc11c\\ub294\\ubcf4\\ud1b5\\uc758\\ub9d0\\uc18c\\ub9ac\\ub97c\\uc54c \\uc544\\ub4e3\\uc9c0\\ubabb\\ud558\\ub294\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 5) \\uc21c\\uc74c\\uccad\\ub825\\uac80\\uc0ac\\ub97c\\uc2e4\\uc2dc\\ud558\\uae30\\uace4\\ub780\\ud558\\uac70\\ub098\\uac80\\uc0ac\\uacb0\\uacfc\\uc5d0\\ub300\\ud55c\\uac80\\uc99d\\uc774\\ud544\\uc694\\ud55c\\uacbd \\uc6b0\\uc5d0\\ub294 \\u201c\\uc5b8\\uc5b4\\uccad\\ub825\\uac80\\uc0ac, \\uc784\\ud53c\\ub358\\uc2a4 \\uccad\\ub825\\uac80\\uc0ac, \\ub1cc\\uac04\\uc720\\ubc1c\\ubc18\\uc751\\uccad\\ub825\\uac80\\uc0ac (ABR), \\uc790\\uae30\\uccad\\ub825\\uacc4\\uae30\\uac80\\uc0ac, \\uc774\\uc74c\\ud5a5\\ubc29\\uc0ac\\uac80\\uc0ac\\u201d\\ub4f1\\uc744\\ucd94\\uac00\\uc2e4\\uc2dc\\ud6c4\\uc7a5\\ud574\\ub97c\\ud3c9 \\uac00\\ud55c\\ub2e4. \\ub2e4.\\uadd3\\ubc14\\ud034\\uc758\\uacb0\\uc190 1) \\u201c\\uadd3\\ubc14\\ud034\\uc758\\ub300\\ubd80\\ubd84\\uc774\\uacb0\\uc190\\ub41c\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uadd3\\ubc14\\ud034\\uc758\\uc5f0\\uace8\\ubd80\\uac00 1/2\\uc774\\uc0c1 \\uacb0 \\uc190\\ub41c\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud558\\uba70, \\uadd3\\ubc14\\ud034\\uc758\\uacb0\\uc190\\uc774 1/2\\ubbf8\\ub9cc\\uc774\\uace0\\uae30\\ub2a5\\uc5d0\\ubb38\\uc81c\\uac00\\uc5c6\\uc73c\\uba74 \\uc678\\ubaa8\\uc758\\ucd94\\uc0c1(\\ucd94\\ud55c\\ubaa8\\uc2b5)\\uc7a5\\ud574\\ub85c\\ud3c9\\uac00\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"3. \\ucf54\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\\"\\ucf54\\uc758\\uae30\\ub2a5\\uc744\\uc644\\uc804\\ud788\\uc783\\uc5c8\\uc744\\ub54c\\\"\\ub77c\\ud568\\uc740\\uc591\\ucabd\\ucf54\\uc758\\ud638\\ud761\\uace4\\ub780\\ub610\\ub294\\uc591\\ucabd\\ucf54 \\uc758\\ud6c4\\uac01\\uae30\\ub2a5\\uc744\\uc644\\uc804\\ud788\\uc783\\uc740\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud558\\uba70, \\ud6c4\\uac01\\uac10\\ud1f4\\ub294\\uc7a5\\ud574\\uc758\\ub300\\uc0c1\\uc73c\\ub85c \\ud558\\uc9c0\\uc54a\\ub294\\ub2e4. 2) \\ucf54\\uc758\\ucd94\\uc0c1(\\ucd94\\ud55c\\ubaa8\\uc2b5) \\uc7a5\\ud574\\ub97c\\uc218\\ubc18\\ud55c\\ub54c\\uc5d0\\ub294\\uae30\\ub2a5\\uc7a5\\ud574\\uc640\\uac01\\uac01\\ub354\\ud558\\uc5ec\\uc9c0\\uae09 \\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc90