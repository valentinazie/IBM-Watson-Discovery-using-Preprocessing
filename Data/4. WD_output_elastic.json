ub837\\ud558\\uace0\\ud2b9\\uc218\\ubcf4\\uc870\\uac80\\uc0ac\\uc5d0\\uc11c\\uc774\\uc0c1 \\uc774\\uc788\\uc73c\\uba70,\\ucc99\\ucd94\\uc2e0\\uacbd\\uadfc\\uc758\\ubd88\\uc644\\uc804\\ub9c8\\ube44\\uac00\\uc778\\uc815\\ub418\\ub294\\uacbd\\uc6b0 11)\\uc57d\\uac04\\uc758\\ucd94\\uac04\\ud310\\ud0c8\\ucd9c\\uc99d(\\uc18d\\uce6d\\ub514\\uc2a4\\ud06c) \\uac00)\\ud2b9\\uc218\\uac80\\uc0ac(\\uc804\\uc0b0\\ud654\\ub2e8\\uce35\\ucd2c\\uc601(CT), \\uc790\\uae30\\uacf5\\uba85\\uc601\\uc0c1\\ubc95(MRI) \\ub4f1)\\uc5d0\\uc11c\\ucd94\\uac04\\ud310 \\ubcd1\\ubcc0\\uc774\\ud655\\uc778\\ub418\\uace0\\uc758\\ud559\\uc801\\uc73c\\ub85c\\uc778\\uc815\\ud560\\ub9cc\\ud55c\\ud558\\uc9c0\\ubc29\\uc0ac\\ud1b5(\\uc8fc\\ubcc0\\ubd80\\uc704\\ub85c\\ubed7\\uce58 \\ub294\\uc99d\\uc0c1)\\ub610\\ub294\\uac10\\uac01\\uc774\\uc0c1\\uc774\\uc788\\ub294\\uacbd\\uc6b0 12)\\ucd94\\uac04\\ud310\\ud0c8\\ucd9c\\uc99d(\\uc18d\\uce6d\\ub514\\uc2a4\\ud06c)\\uc73c\\ub85c\\uc9c4\\ub2e8\\ub41c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc218\\uc220\\uc5ec\\ubd80\\uc5d0\\uad00\\uacc4\\uc5c6\\uc774 \\uc6b4\\ub3d9\\uc7a5\\ud574\\ubc0f\\uae30\\ud615\\uc7a5\\ud574\\ub85c\\ud3c9\\uac00\\ud558\\uc9c0\\uc54a\\ub294\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"7. \\uccb4\\uac04\\uace8\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\u201c\\uccb4\\uac04\\uace8\\u201d\\uc774\\ub77c\\ud568\\uc740\\uc5b4\\uae68\\ubf08,\\uace8\\ubc18\\ubf08,\\ube57\\uc7a5\\ubf08,\\uac00\\uc2b4\\ubf08,\\uac08\\ube44\\ubf08\\ub97c\\ub9d0\\ud558\\uba70, \\uc774\\ub97c\\ubaa8\\ub450\\ub3d9\\uc77c\\ud55c\\ubd80\\uc704\\ub85c\\ud55c\\ub2e4. 2) \\\"\\uace8\\ubc18\\ubf08\\uc758\\ub69c\\ub837\\ud55c\\uae30\\ud615\\u201c\\uc774\\ub77c\\ud568\\uc740\\uc544\\ub798\\uc640\\uac19\\ub2e4. \\uac00)\\ucc9c\\uc7a5\\uad00\\uc808\\ub610\\ub294\\uce58\\uace8\\ubb38\\ud569\\ubd80\\uac00\\ubd84\\ub9ac\\ub41c\\uc0c1\\ud0dc\\ub85c\\uce58\\uc720\\ub418\\uc5c8\\uac70\\ub098\\uc88c\\uace8\\uc774 2.5cm \\uc774\\uc0c1\\ubd84\\ub9ac\\ub41c\\ubd80\\uc815\\uc720\\ud569\\uc0c1\\ud0dc\\ub610\\ub294\\uc5ec\\uc790\\uc5d0\\uac8c\\uc815\\uc0c1\\ubd84\\ub9cc\\uc5d0\\uc9c0\\uc7a5\\uc744\\uc904\\uc815\\ub3c4\\ub85c \\uace8\\ubc18\\uc758\\ubcc0\\ud615\\uc774\\ub0a8\\uc740\\uc0c1\\ud0dc \\ub098)\\uc54c\\ubab8\\uc774\\ub418\\uc5c8\\uc744\\ub54c\\ubcc0\\ud615(\\uacb0\\uc190\\uc744\\ud3ec\\ud568)\\uc744\\uba85\\ubc31\\ud558\\uac8c\\uc54c\\uc218\\uc788\\uc744\\uc815\\ub3c4\\ub97c\\ub9d0 \\ud558\\uba70,\\ubc29\\uc0ac\\uc120\\uac80\\uc0ac\\ub85c\\uce21\\uc815\\ud55c\\uac01\\ubcc0\\ud615\\uc774 20\\u00b0\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0 3) \\u201c\\ube57\\uc7a5\\ubf08,\\uac00\\uc2b4\\ubf08,\\uac08\\ube44\\ubf08\\ub610\\ub294\\uc5b4\\uae68\\ubf08\\uc5d0\\ub69c\\ub837\\ud55c\\uae30\\ud615\\uc774\\ub0a8\\uc740\\ub54c\\u201d\\ub77c\\ud568\\uc740 \\uc54c\\ubab8\\uc774\\ub418\\uc5c8\\uc744\\ub54c\\ubcc0\\ud615(\\uacb0\\uc190\\uc744\\ud3ec\\ud568)\\uc744 \\uba85\\ubc31\\ud558\\uac8c\\uc54c\\uc218\\uc788\\uc744\\uc815\\ub3c4\\ub97c\\ub9d0\\ud558 \\uba70,\\ubc29\\uc0ac\\uc120\\uac80\\uc0ac\\ub85c\\uce21\\uc815\\ud55c\\uac01\\ubcc0\\ud615\\uc774 20\\u00b0\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\uac08\\ube44\\ubf08\\uc758\\uae30\\ud615\\uc740\\uadf8\\uac1c\\uc218\\uc640\\uc815\\ub3c4, \\ubd80\\uc704\\ub4f1\\uc5d0\\uad00\\uacc4\\uc5c6\\uc774\\uc804\\uccb4\\ub97c\\uc77c\\uad04\\ud558\\uc5ec\\ud558 \\ub098\\uc758\\uc7a5\\ud574\\ub85c\\ucde8\\uae09\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"8. \\ud314\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\uace8\\uc808\\ubd80\\uc5d0\\uae08\\uc18d\\ub0b4\\uace0\\uc815\\ubb3c\\ub4f1\\uc744\\uc0ac\\uc6a9\\ud558\\uc600\\uae30\\ub54c\\ubb38\\uc5d0\\uadf8\\uac83\\uc774\\uae30\\ub2a5\\uc7a5\\ud574\\uc758\\uc6d0\\uc778 \\uc774\\ub418\\ub294\\ub54c\\uc5d0\\ub294\\uadf8\\ub0b4\\uace0\\uc815\\ubb3c\\ub4f1\\uc774\\uc81c\\uac70\\ub41c\\ud6c4\\uc7a5\\ud574\\ub97c\\ud310\\uc815\\ud55c\\ub2e4. 2) \\uad00\\uc808\\uc744\\uc0ac\\uc6a9\\ud558\\uc9c0\\uc54a\\uc544\\ubc1c\\uc0dd\\ud55c\\uae30\\ub2a5\\uc7a5\\ud574(\\uc608\\ucee8\\ub300\\uc11d\\uace0\\ubd95\\ub300(cast)\\ub85c \\ud658\\ubd80\\ub97c \\uace0\\uc815\\uc2dc\\ucf30\\uae30\\ub54c\\ubb38\\uc5d0\\uce58\\uc720\\ud6c4\\uc758\\uad00\\uc808\\uc5d0\\uae30\\ub2a5\\uc7a5\\ud574\\uac00\\uc0dd\\uae34\\uacbd\\uc6b0)\\uc640 \\uc77c\\uc2dc\\uc801\\uc778 \\uc7a5\\ud574\\ub294\\uc7a5\\ud574\\ubcf4\\uc0c1\\uc744\\ud558\\uc9c0\\uc54a\\ub294\\ub2e4. 3) \\u201c\\ud314\\u201d\\uc774\\ub77c\\ud568\\uc740\\uc5b4\\uae68\\uad00\\uc808(\\u80a9\\u95dc\\u7bc0)\\ubd80\\ud130\\uc190\\ubaa9\\uad00\\uc808\\uae4c\\uc9c0\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\u201c\\ud314\\uc758 3\\ub300\\uad00\\uc808\\u201d\\uc774\\ub77c\\ud568\\uc740\\uc5b4\\uae68\\uad00\\uc808,\\ud314\\uafc8\\uce58\\uad00\\uc808\\ubc0f\\uc190\\ubaa9\\uad00\\uc808\\uc744\\ub9d0\\ud55c\\ub2e4. 5) \\u201c\\ud55c\\ud314\\uc758\\uc190\\ubaa9\\uc774\\uc0c1\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc190\\ubaa9\\uad00\\uc808\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uac00\\uae4c\\uc6b4\\ucabd \\uc5d0\\uc11c\\uc808\\ub2e8\\ub41c\\ub54c\\ub97c\\ub9d0\\ud558\\uba70,\\ud314\\uafc8\\uce58\\uad00\\uc808\\uc0c1\\ubd80\\uc5d0\\uc11c\\uc808\\ub2e8\\ub41c\\uacbd\\uc6b0\\ub3c4\\ud3ec\\ud568\\ub41c\\ub2e4. 6) \\ud314\\uc758\\uad00\\uc808\\uae30\\ub2a5\\uc7a5\\ud574\\ud3c9\\uac00\\ub294\\ud314\\uc758 3\\ub300\\uad00\\uc808\\uc758\\uad00\\uc808\\uc6b4\\ub3d9\\ubc94\\uc704\\uc81c\\ud55c\\ub4f1\\uc73c\\ub85c\\ud3c9 \\uac00\\ud55c\\ub2e4. \\uac01\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\uce21\\uc815\\uc740\\ubbf8\\uad6d\\uc758\\uc0ac\\ud611\\ud68c(A.M.A.)\\u201c\\uc601\\uad6c\\uc801\\uc2e0\\uccb4 \\uc7a5\\ud574\\ud3c9\\uac00\\uc9c0\\uce68\\u201d\\uc758\\uc815\\uc0c1\\uac01\\ub3c4\\ubc0f\\uce21\\uc815\\ubc29\\ubc95\\ub4f1\\uc744\\ub530\\ub974\\uba70, \\uad00\\uc808\\uae30\\ub2a5\\uc7a5\\ud574\\ub97c \\ud45c\\uc2dc\\ud560\\uacbd\\uc6b0\\uc5d0\\ub294\\uc7a5\\ud574\\ubd80\\uc704\\uc758\\uc7a5\\ud574\\uac01\\ub3c4\\uc640\\uc815\\uc0c1\\ubd80\\uc704\\uc758\\uce21\\uc815\\uce58\\ub97c\\ub3d9\\uc2dc\\uc5d0\\ud310 \\ub2e8\\ud558\\uc5ec\\uc7a5\\ud574\\uc0c1\\ud0dc\\ub97c\\uba85\\ud655\\ud788\\ud55c\\ub2e4. \\uac00)\\u201c\\uae30\\ub2a5\\uc744\\uc644\\uc804\\ud788\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\uc644\\uc804\\uac15\\uc9c1(\\uad00\\uc808\\uad73\\uc74c)\\ub610\\ub294\\uc778\\uacf5\\uad00\\uc808\\uc774\\ub098\\uc778\\uacf5\\uace8\\ub450\\ub97c\\uc0bd\\uc785\\ud55c\\uacbd\\uc6b0 \\u2461 \\uadfc\\uc804\\ub3c4 \\uac80\\uc0ac\\uc0c1 \\uc644\\uc804\\ub9c8\\ube44 \\uc18c\\uacac\\uc774 \\uc788\\uace0 \\uadfc\\ub825\\uac80\\uc0ac\\uc5d0\\uc11c \\uadfc\\ub825\\uc774\\u201c0\\ub4f1\\uae09 (Zero)\\u201d\\uc778\\uacbd\\uc6b0 \\ub098)\\u201c\\uc2ec\\ud55c\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 1/4\\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd\\uc6b0 \\u2461 \\uadfc\\uc804\\ub3c4\\uac80\\uc0ac\\uc0c1\\uc2ec\\ud55c\\ub9c8\\ube44\\uc18c\\uacac\\uc774\\uc788\\uace0\\uadfc\\ub825\\uac80\\uc0ac\\uc5d0\\uc11c\\uadfc\\ub825\\uc774\\u201c1 \\ub4f1\\uae09 (Trace)\\\"\\uc778\\uacbd\\uc6b0 \\ub2e4)\\u201c\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 1/2 \\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd \\uc6b0 \\ub77c)\\u201c\\uc57d\\uac04\\uc758\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 3/4 \\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd \\uc6b0 7) \\u201c\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc544\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc0c1\\uc644\\uace8\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740 \\uacbd\\uc6b0\\ub610\\ub294\\uc694\\uace8\\uacfc\\ucc99\\uace8\\uc758 2\\uac1c\\ubf08\\ubaa8\\ub450\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 8) \\u201c\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc544\\uc57d\\uac04\\uc758\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc694\\uace8\\uacfc\\ucc99\\uace8\\uc911\\uc5b4\\ub290\\ud55c \\ubf08\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 9) \\u201c\\ubf08\\uc5d0\\uae30\\ud615\\uc744\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc0c1\\uc644\\uace8\\ub610\\ub294\\uc694\\uace8\\uacfc\\ucc99\\uace8\\uc5d0\\ubcc0\\ud615\\uc774\\ub0a8\\uc544 \\uc815\\uc0c1\\uc5d0\\ube44\\ud574\\ubd80\\uc815\\uc720\\ud569\\ub41c\\uac01\\ubcc0\\ud615\\uc774 15\\u00b0\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. \\ub2e4.\\uc9c0\\uae09\\ub960\\uc758\\uacb0\\uc815 1) 1\\uc0c1\\uc9c0(\\ud314\\uacfc\\uc190\\uac00\\ub77d)\\uc758\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc740\\uc6d0\\uce59\\uc801\\uc73c\\ub85c\\uac01\\uac01\\ub354\\ud558\\ub418, \\uc9c0\\uae09 \\ub960\\uc740 60% \\ud55c\\ub3c4\\ub85c\\ud55c\\ub2e4. 2) \\ud55c\\ud314\\uc758 3\\ub300\\uad00\\uc808\\uc911\\uad00\\uc808\\ud558\\ub098\\uc5d0\\uae30\\ub2a5\\uc7a5\\ud574\\uac00\\uc0dd\\uae30\\uace0\\ub2e4\\ub978\\uad00\\uc808\\ud558\\ub098\\uc5d0\\uae30\\ub2a5 \\uc7a5\\ud574\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uc9c0\\uae09\\ub960\\uc740\\uac01\\uac01\\uc801\\uc6a9\\ud558\\uc5ec\\ub354\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"9. \\ub2e4\\ub9ac\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\uace8\\uc808\\ubd80\\uc5d0\\uae08\\uc18d\\ub0b4\\uace0\\uc815\\ubb3c\\ub4f1\\uc744\\uc0ac\\uc6a9\\ud558\\uc600\\uae30\\ub54c\\ubb38\\uc5d0\\uadf8\\uac83\\uc774\\uae30\\ub2a5\\uc7a5\\ud574\\uc758\\uc6d0\\uc778 \\uc774\\ub418\\ub294\\ub54c\\uc5d0\\ub294\\uadf8\\ub0b4\\uace0\\uc815\\ubb3c\\ub4f1\\uc774\\uc81c\\uac70\\ub41c\\ud6c4\\uc7a5\\ud574\\ub97c\\ud310\\uc815\\ud55c\\ub2e4. 2) \\uad00\\uc808\\uc744\\uc0ac\\uc6a9\\ud558\\uc9c0\\uc54a\\uc544\\ubc1c\\uc0dd\\ud55c\\uae30\\ub2a5\\uc7a5\\ud574(\\uc608\\ucee8\\ub300\\uc11d\\uace0\\ubd95\\ub300(cast)\\ub85c \\ud658\\ubd80\\ub97c \\uace0\\uc815\\uc2dc\\ucf30\\uae30\\ub54c\\ubb38\\uc5d0\\uce58\\uc720\\ud6c4\\uc758\\uad00\\uc808\\uc5d0\\uae30\\ub2a5\\uc7a5\\ud574\\uac00\\uc0dd\\uae34\\uacbd\\uc6b0)\\uc640 \\uc77c\\uc2dc\\uc801\\uc778 \\uc7a5\\ud574\\uc5d0\\ub300\\ud558\\uc5ec\\ub294\\uc7a5\\ud574\\ubcf4\\uc0c1\\uc744\\ud558\\uc9c0\\uc54a\\ub294\\ub2e4. 3) \\u201c\\ub2e4\\ub9ac\\u201d\\ub77c\\ud568\\uc740\\uc5c9\\ub369\\uc774\\uad00\\uc808(\\u80a1\\u95dc\\u7bc0)\\ubd80\\ud130\\ubc1c\\ubaa9\\uad00\\uc808\\uae4c\\uc9c0\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\u201c\\ub2e4\\ub9ac\\uc758 3\\ub300\\uad00\\uc808\\u201d\\uc774\\ub77c\\ud568\\uc740\\uace0\\uad00\\uc808,\\ubb34\\ub98e\\uad00\\uc808\\ubc0f\\ubc1c\\ubaa9\\uad00\\uc808\\uc744\\ub9d0\\ud55c\\ub2e4. 5) \\u201c\\ud55c\\ub2e4\\ub9ac\\uc758\\ubc1c\\ubaa9\\uc774\\uc0c1\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ubc1c\\ubaa9\\uad00\\uc808\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uac00\\uae4c\\uc6b4 \\ucabd\\uc5d0\\uc11c\\uc808\\ub2e8\\ub41c\\ub54c\\ub97c\\ub9d0\\ud558\\uba70, \\ubb34\\ub98e\\uad00\\uc808\\uc758\\uc0c1\\ubd80\\uc5d0\\uc11c\\uc808\\ub2e8\\ub41c\\uacbd\\uc6b0\\ub3c4\\ud3ec\\ud568\\ub41c \\ub2e4. 6) \\ub2e4\\ub9ac\\uc758\\uad00\\uc808\\uae30\\ub2a5\\uc7a5\\ud574\\ud3c9\\uac00\\ub294\\ud558\\uc9c0\\uc758 3\\ub300\\uad00\\uc808\\uc758\\uad00\\uc808\\uc6b4\\ub3d9\\ubc94\\uc704\\uc81c\\ud55c\\ubc0f\\ub3d9 \\uc694\\uc131\\uc720\\ubb34\\ub4f1\\uc73c\\ub85c\\ud3c9\\uac00\\ud55c\\ub2e4. \\uac01\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\uce21\\uc815\\uc740\\ubbf8\\uad6d\\uc758\\uc0ac\\ud611\\ud68c(A. M.A.) \\u201c\\uc601\\uad6c\\uc801\\uc2e0\\uccb4\\uc7a5\\ud574\\ud3c9\\uac00\\uc9c0\\uce68\\u201d\\uc758\\uc815\\uc0c1\\uac01\\ub3c4\\ubc0f\\uce21\\uc815\\ubc29\\ubc95\\ub4f1\\uc744\\ub530\\ub974 \\uba70,\\uad00\\uc808\\uae30\\ub2a5\\uc7a5\\ud574\\ub97c\\ud45c\\uc2dc\\ud560\\uacbd\\uc6b0\\uc5d0\\ub294\\uc7a5\\ud574\\ubd80\\uc704\\uc758\\uc7a5\\ud574\\uac01\\ub3c4\\uc640\\uc815\\uc0c1\\ubd80\\uc704\\uc758 \\uce21\\uc815\\uce58\\ub97c\\ub3d9\\uc2dc\\uc5d0\\ud310\\ub2e8\\ud558\\uc5ec\\uc7a5\\ud574\\uc0c1\\ud0dc\\ub97c\\uba85\\ud655\\ud788\\ud55c\\ub2e4. \\uac00)\\u201c\\uae30\\ub2a5\\uc744\\uc644\\uc804\\ud788\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\uc644\\uc804\\uac15\\uc9c1(\\uad00\\uc808\\uad73\\uc74c)\\ub610\\ub294\\uc778\\uacf5\\uad00\\uc808\\uc774\\ub098\\uc778\\uacf5\\uace8\\ub450\\ub97c\\uc0bd\\uc785\\ud55c\\uacbd\\uc6b0 \\u2461 \\uadfc\\uc804\\ub3c4\\uac80\\uc0ac\\uc0c1\\uc644\\uc804\\ub9c8\\ube44\\uc18c\\uacac\\uc774\\uc788\\uace0\\uadfc\\ub825\\uac80\\uc0ac\\uc5d0\\uc11c\\uadfc\\ub825\\uc774\\u201c0\\ub4f1\\uae09 (Zero)\\u201d\\uc778\\uacbd\\uc6b0 \\ub098)\\u201c\\uc2ec\\ud55c\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 1/4\\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd\\uc6b0 \\u2461 \\uac1d\\uad00\\uc801\\uac80\\uc0ac(\\uc2a4\\ud2b8\\ub808\\uc2a4\\uc5d1\\uc2a4\\uc120)\\uc0c1 15mm \\uc774\\uc0c1\\uc758\\ub3d9\\uc694\\uad00\\uc808(\\uad00\\uc808\\uc774\\ud754\\ub4e4 \\ub9ac\\uac70\\ub098\\uc6c0\\uc9c1\\uc774\\ub294\\uac83)\\uc774\\uc788\\ub294\\uacbd\\uc6b0 \\u2462 \\uadfc\\uc804\\ub3c4\\uac80\\uc0ac\\uc0c1\\uc2ec\\ud55c\\ub9c8\\ube44\\uc18c\\uacac\\uc774\\uc788\\uace0\\uadfc\\ub825\\uac80\\uc0ac\\uc5d0\\uc11c\\uadfc\\ub825\\uc774\\u201c1\\ub4f1\\uae09 (Trace)\\\"\\uc778\\uacbd\\uc6b0 \\ub2e4)\\u201c\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 1/2 \\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd \\uc6b0 \\u2461 \\uac1d\\uad00\\uc801\\uac80\\uc0ac(\\uc2a4\\ud2b8\\ub808\\uc2a4\\uc5d1\\uc2a4\\uc120)\\uc0c1 10mm \\uc774\\uc0c1\\uc758\\ub3d9\\uc694\\uad00\\uc808(\\uad00\\uc808\\uc774\\ud754\\ub4e4 \\ub9ac\\uac70\\ub098\\uc6c0\\uc9c1\\uc774\\ub294\\uac83)\\uc774\\uc788\\ub294\\uacbd\\uc6b0 \\ub77c)\\u201c\\uc57d\\uac04\\uc758\\uc7a5\\ud574\\u201d\\ub77c\\ud568\\uc740 \\u2460 \\ud574\\ub2f9\\uad00\\uc808\\uc758\\uc6b4\\ub3d9\\ubc94\\uc704\\ud569\\uacc4\\uac00\\uc815\\uc0c1\\uc6b4\\ub3d9\\ubc94\\uc704\\uc758 3/4 \\uc774\\ud558\\ub85c\\uc81c\\ud55c\\ub41c\\uacbd \\uc6b0 \\u2461 \\uac1d\\uad00\\uc801\\uac80\\uc0ac(\\uc2a4\\ud2b8\\ub808\\uc2a4\\uc5d1\\uc2a4\\uc120)\\uc0c1 5mm \\uc774\\uc0c1\\uc758\\ub3d9\\uc694\\uad00\\uc808(\\uad00\\uc808\\uc774\\ud754\\ub4e4 \\ub9ac\\uac70\\ub098\\uc6c0\\uc9c1\\uc774\\ub294\\uac83)\\uc774\\uc788\\ub294\\uacbd\\uc6b0 7) \\u201c\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc544\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ub300\\ud1f4\\uace8\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740 \\uacbd\\uc6b0\\ub610\\ub294\\uacbd\\uace8\\uacfc\\uc885\\uc544\\ub9ac\\ubf08\\uc758 2\\uac1c\\ubf08\\ubaa8\\ub450\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 8) \\u201c\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc544\\uc57d\\uac04\\uc758\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uacbd\\uace8\\uacfc\\uc885\\uc544\\ub9ac\\ubf08\\uc911\\uc5b4\\ub290 \\ud55c\\ubf08\\uc5d0\\uac00\\uad00\\uc808\\uc774\\ub0a8\\uc740\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 9) \\u201c\\ubf08\\uc5d0\\uae30\\ud615\\uc744\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ub300\\ud1f4\\uace8\\ub610\\ub294\\uacbd\\uace8\\uc5d0\\uae30\\ud615\\uc774\\ub0a8\\uc544\\uc815\\uc0c1\\uc5d0 \\ube44\\ud574\\ubd80\\uc815\\uc720\\ud569\\ub41c\\uac01\\ubcc0\\ud615\\uc774 15\\u00b0\\uc774\\uc0c1\\uc778\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 10)\\ub2e4\\ub9ac\\uc758\\ub2e8\\ucd95\\uc740\\uc0c1\\uc804\\uc7a5\\uace8\\uadf9\\uc5d0\\uc11c\\ubd80\\ud130\\uacbd\\uace8\\ub0b4\\uce21\\uacfc\\ud558\\ub2e8\\uae4c\\uc9c0\\uc758\\uae38\\uc774\\ub97c\\uce21\\uc815 \\ud558\\uc5ec\\uc815\\uc0c1\\uc778\\ucabd\\ub2e4\\ub9ac\\uc758\\uae38\\uc774\\uc640\\ube44\\uad50\\ud558\\uc5ec\\ub2e8\\ucd95\\ub41c\\uae38\\uc774\\ub97c\\uc0b0\\ucd9c\\ud55c\\ub2e4. \\ub2e4\\ub9ac\\uae38 \\uc774\\uc758\\uce21\\uc815\\uc5d0\\uc774\\uc6a9\\ud558\\ub294\\uace8\\ud45c\\uc801(bony landmark)\\uc774 \\uba85\\ud655\\ud558\\uc9c0\\uc54a\\uc740\\uacbd\\uc6b0\\ub098\\ub2e4 \\ub9ac\\uc758\\ub2e8\\ucd95\\uc7a5\\ud574\\ud310\\ub2e8\\uc774\\uc560\\ub9e4\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uc2a4\\uce90\\ub178\\uadf8\\ub7a8(scanogram)\\uc73c\\ub85c \\ub2e4\\ub9ac \\uc758\\ub2e8\\ucd95\\uc815\\ub3c4\\ub97c\\uce21\\uc815\\ud55c\\ub2e4. \\ub2e4.\\uc9c0\\uae09\\ub960\\uc758\\uacb0\\uc815 1) 1\\ud558\\uc9c0(\\ub2e4\\ub9ac\\uc640\\ubc1c\\uac00\\ub77d)\\uc758\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc740\\uc6d0\\uce59\\uc801\\uc73c\\ub85c\\uac01\\uac01\\ub354\\ud558\\ub418, \\uc9c0 \\uae09\\ub960\\uc740 60% \\ud55c\\ub3c4\\ub85c\\ud55c\\ub2e4. 2) \\ud55c \\ub2e4\\ub9ac\\uc758 3\\ub300 \\uad00\\uc808\\uc911\\uad00\\uc808\\ud558\\ub098\\uc5d0\\uae30\\ub2a5\\uc7a5\\ud574\\uac00\\uc0dd\\uae30\\uace0\\ub2e4\\ub978\\uad00\\uc808\\ud558\\ub098\\uc5d0 \\uae30\\ub2a5\\uc7a5\\ud574\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uc9c0\\uae09\\ub960\\uc740\\uac01\\uac01\\uc801\\uc6a9\\ud558\\uc5ec\\ub354\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"10.\\uc190\\uac00\\ub77d\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\uc190\\uac00\\ub77d\\uc5d0\\ub294\\uccab\\uc9f8\\uc190\\uac00\\ub77d\\uc5d0 2\\uac1c\\uc758\\uc190\\uac00\\ub77d\\uad00\\uc808\\uc774\\uc788\\ub2e4. \\uadf8\\uc911\\uc2ec\\uc7a5\\uc5d0\\uc11c\\uac00\\uae4c \\uc6b4\\ucabd\\ubd80\\ud130\\uc911\\uc218\\uc9c0\\uad00\\uc808,\\uc9c0\\uad00\\uc808\\uc774\\ub77c\\ud55c\\ub2e4. 2) \\ub2e4\\ub978 \\ub124\\uc190\\uac00\\ub77d\\uc5d0\\ub294 3\\uac1c\\uc758\\uc190\\uac00\\ub77d\\uad00\\uc808\\uc774\\uc788\\ub2e4. \\uadf8\\uc911\\uc2ec\\uc7a5\\uc5d0\\uc11c\\uac00\\uae4c\\uc6b4\\ucabd \\ubd80\\ud130\\uc911\\uc218\\uc9c0\\uad00\\uc808, \\uc81c1\\uc9c0\\uad00\\uc808(\\uadfc\\uc704\\uc9c0\\uad00\\uc808) \\ubc0f \\uc81c2\\uc9c0\\uad00\\uc808(\\uc6d0\\uc704\\uc9c0\\uad00\\uc808)\\uc774 \\ub77c\\ubd80\\ub978\\ub2e4. 3) \\u201c\\uc190\\uac00\\ub77d\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uccab\\uc9f8\\uc190\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc9c0\\uad00\\uc808\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uc11c \\uac00\\uae4c\\uc6b4\\ucabd\\uc5d0\\uc11c, \\ub2e4\\ub978\\ub124\\uc190\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc81c1\\uc9c0\\uad00\\uc808(\\uadfc\\uc704\\uc9c0\\uad00\\uc808)\\ubd80\\ud130\\uc2ec\\uc7a5 \\uc5d0\\uc11c\\uac00\\uae4c\\uc6b4\\ucabd\\uc73c\\ub85c\\uc190\\uac00\\ub77d\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\ub97c\\ub9d0\\ud55c\\ub2e4. 4) \\u201c\\uc190\\uac00\\ub77d\\ubf08\\uc77c\\ubd80\\ub97c\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uccab\\uc9f8\\uc190\\uac00\\ub77d\\uc758\\uc9c0\\uad00\\uc808, \\ub2e4\\ub978\\ub124\\uc190 \\uac00\\ub77d\\uc758\\uc81c1\\uc9c0\\uad00\\uc808(\\uadfc\\uc704\\uc9c0\\uad00\\uc808)\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uc11c\\uba3c\\ucabd\\uc73c\\ub85c\\uc190\\uac00\\ub77d\\ubf08\\ub97c\\uc783\\uc5c8 \\uac70\\ub098\\ubf1b\\uc870\\uac01\\uc774\\ub5a8\\uc5b4\\uc838\\uc788\\ub294\\uac83\\uc774\\uc5d1\\uc2a4\\uc120\\uc0ac\\uc9c4\\uc73c\\ub85c\\uba85\\ubc31\\ud55c\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 5) \\u201c\\uc190\\uac00\\ub77d\\uc5d0\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uc190\\uac00\\ub77d\\uc758\\uc0dd\\ub9ac\\uc801\\uc6b4\\ub3d9\\uc601\\uc5ed\\uc774 \\uc815\\uc0c1\\uc6b4\\ub3d9\\uac00\\ub2a5\\uc601\\uc5ed\\uc758 1/2 \\uc774\\ud558\\uac00\\ub418\\uc5c8\\uc744\\ub54c\\uc774\\uba70\\uc774\\uacbd\\uc6b0\\uc190\\uac00\\ub77d\\uad00\\uc808\\uc758\\uad74 \\uc2e0(\\uad7d\\ud788\\uace0\\ud3b4\\uae30)\\uc6b4\\ub3d9 \\uac00\\ub2a5\\uc601\\uc5ed\\uc73c\\ub85c\\uce21\\uc815\\ud55c\\ub2e4. \\uccab\\uc9f8 \\uc190\\uac00\\ub77d\\uc774\\uc678\\uc758\\ub2e4\\ub978 \\ub124\\uc190\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc81c1, \\uc81c2\\uc9c0\\uad00\\uc808\\uc758\\uad74\\uc2e0(\\uad7d\\ud788\\uace0\\ud3b4\\uae30)\\uc6b4\\ub3d9\\uc601\\uc5ed\\uc744\\ub354\\ud558\\uc5ec \\uc815\\uc0c1\\uc6b4\\ub3d9\\uc601\\uc5ed\\uc758 1/2 \\uc774\\ud558\\uc778\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 6) \\ud55c \\uc190\\uac00\\ub77d\\uc5d0\\uc7a5\\ud574\\uac00\\uc0dd\\uae30\\uace0\\ub2e4\\ub978\\uc190\\uac00\\ub77d\\uc5d0\\uc7a5\\ud574\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0, \\uc9c0\\uae09\\ub960\\uc740 \\uac01\\uac01\\uc801\\uc6a9\\ud558\\uc5ec\\ub354\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"11.\\ubc1c\\uac00\\ub77d\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\ud310\\uc815\\uae30\\uc900 1) \\u201c\\ubc1c\\uac00\\ub77d\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uccab\\uc9f8\\ubc1c\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc9c0\\uad00\\uc808\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uac00 \\uae4c\\uc6b4\\ucabd\\uc744, \\ub098\\uba38\\uc9c0\\ub124\\ubc1c\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc81c1\\uc9c0\\uad00\\uc808(\\uadfc\\uc704\\uc9c0\\uad00\\uc808)\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0 \\uc11c\\uac00\\uae4c\\uc6b4\\ucabd\\uc744\\uc783\\uc5c8\\uc744\\ub54c\\ub97c\\ub9d0\\ud55c\\ub2e4. 2) \\ub9ac\\uc2a4\\ud504\\ub791\\uad00\\uc808\\uc774\\uc0c1\\uc5d0\\uc11c\\uc783\\uc740\\ub54c\\ub77c\\ud568\\uc740\\uc871\\uadfc-\\uc911\\uc871\\uace8\\uac04\\uad00\\uc808\\uc774\\uc0c1\\uc5d0\\uc11c\\uc808 \\ub2e8\\ub41c\\uacbd\\uc6b0\\ub97c\\ub9d0\\ud55c\\ub2e4. 3) \\u201c\\ubc1c\\uac00\\ub77d\\ubf08\\uc77c\\ubd80\\ub97c\\uc783\\uc5c8\\uc744\\ub54c\\u201d\\ub77c\\ud568\\uc740\\uccab\\uc9f8\\ubc1c\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc9c0\\uad00\\uc808, \\ub2e4\\ub978 \\ub124\\ubc1c\\uac00\\ub77d\\uc5d0\\uc11c\\ub294\\uc81c1\\uc9c0\\uad00\\uc808(\\uadfc\\uc704\\uc9c0\\uad00\\uc808)\\ubd80\\ud130\\uc2ec\\uc7a5\\uc5d0\\uc11c\\uba3c\\ucabd\\uc5d0\\uc11c\\ubc1c\\uac00\\ub77d \\ubf08\\ub97c\\uc783\\uc5c8\\uc744\\ub54c\\ub97c\\ub9d0\\ud558\\uace0\\ub2e8\\uc21c\\ud788\\uc0b4\\uc810\\uc774\\ub5a8\\uc5b4\\uc9c4\\uac83\\ub9cc\\uc73c\\ub85c\\ub294\\ub300\\uc0c1\\uc774\\ub418\\uc9c0 \\uc54a\\ub294\\ub2e4. 4) \\u201c\\ubc1c\\uac00\\ub77d\\uc5d0\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740\\ubc1c\\uac00\\ub77d\\uc758\\uc0dd\\ub9ac\\uc801\\uc6b4\\ub3d9\\uc601\\uc5ed\\uc774 \\uc815\\uc0c1\\uc6b4\\ub3d9\\uac00\\ub2a5\\uc601\\uc5ed\\uc758 1/2 \\uc774\\ud558\\uac00 \\ub418\\uc5c8\\uc744\\ub54c\\ub97c\\ub9d0\\ud558\\uba70, \\uc774 \\uacbd\\uc6b0\\ubc1c\\uac00\\ub77d\\uc758 \\uc8fc\\ub41c\\uae30\\ub2a5\\uc778\\ubc1c\\uac00\\ub77d\\uad00\\uc808\\uc758\\uad74\\uc2e0(\\uad7d\\ud788\\uace0\\ud3b4\\uae30)\\uae30\\ub2a5\\uc744\\uce21\\uc815\\ud558\\uc5ec\\uacb0\\uc815\\ud55c\\ub2e4. 5) \\ud55c \\ubc1c\\uac00\\ub77d\\uc5d0\\uc7a5\\ud574\\uac00\\uc0dd\\uae30\\uace0\\ub2e4\\ub978\\ubc1c\\uac00\\ub77d\\uc5d0\\uc7a5\\ud574\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0, \\uc9c0\\uae09\\ub960\\uc740 \\uac01\\uac01\\uc801\\uc6a9\\ud558\\uc5ec\\ub354\\ud55c\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\", \"subtitle\": \"<\\uc7a5\\ud574\\ubd84\\ub958\\ubcc4\\ud310\\uc815\\uae30\\uc900>\", \"subsubtitle\": \"12.\\ud749\\u00b7\\ubcf5\\ubd80\\uc7a5\\uae30\\ubc0f\\ube44\\ub1e8\\uc0dd\\uc2dd\\uae30\\uc758\\uc7a5\\ud574\", \"Head\": \"\", \"text\": \"\\uac00.\\uc7a5\\ud574\\uc758\\ubd84\\ub958 \\ub098.\\uc7a5\\ud574\\uc758\\ud310\\uc815\\uae30\\uc900 1) \\u201c\\ud749\\ubcf5\\ubd80\\uc7a5\\uae30\\ub610\\ub294\\ube44\\ub1e8\\uc0dd\\uc2dd\\uae30\\uae30\\ub2a5\\uc5d0\\uc2ec\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740 \\uac00)\\uc2ec\\uc7a5,\\ud3d0, \\uc2e0\\uc7a5,\\ub610\\ub294\\uac04\\uc7a5\\uc758\\uc7a5\\uae30\\uc774\\uc2dd\\uc744\\ud55c\\uacbd\\uc6b0 \\ub098)\\uc7a5\\uae30\\uc774\\uc2dd\\uc744\\ud558\\uc9c0\\uc54a\\uace0\\uc11c\\ub294\\uc0dd\\uba85\\uc720\\uc9c0\\uac00\\ubd88\\uac00\\ub2a5\\ud558\\uc5ec\\ud608\\uc561\\ud22c\\uc11d\\ub4f1\\uc758\\ub8cc\\ucc98 \\uce58\\ub97c\\ud3c9\\uc0dd\\ud1a0\\ub85d\\ubc1b\\uc544\\uc57c\\ud560\\ub54c \\ub2e4)\\ubc29\\uad11\\uc758\\uae30\\ub2a5\\uc774\\uc644\\uc804\\ud788\\uc5c6\\uc5b4\\uc9c4\\ub54c 2) \\u201c\\ud749\\ubcf5\\ubd80\\uc7a5\\uae30\\ub610\\ub294\\ube44\\ub1e8\\uc0dd\\uc2dd\\uae30\\uae30\\ub2a5\\uc5d0\\ub69c\\ub837\\ud55c\\uc7a5\\ud574\\ub97c\\ub0a8\\uae34\\ub54c\\u201d\\ub77c\\ud568\\uc740 \\uac00)\\uc704,\\ub300\\uc7a5\\ub610\\ub294\\ucdcc\\uc7a5\\uc758\\uc804\\ubd80\\ub97c\\uc798\\ub77c\\ub0b4\\uc5c8\\uc744\\ub54c \\ub098)\\uc18c\\uc7a5\\ub6