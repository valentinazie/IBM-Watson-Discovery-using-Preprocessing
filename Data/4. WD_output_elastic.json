ub824\\uc8fc\\ub294\\uae08\\uc561\\uc744\\ub9d0\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c2\\uc870 (\\uc6a9\\uc5b4\\uc758 \\uc815\\uc758)\", \"Head\": \"\\u2463 \\uae30\\uac04\\uacfc\\ub0a0\\uc9dc\\uad00\\ub828\\uc6a9\\uc5b4\", \"text\": \"1.\\ubcf4\\ud5d8\\uae30\\uac04: \\uacc4\\uc57d\\uc5d0\\ub530\\ub77c\\ubcf4\\uc7a5\\uc744\\ubc1b\\ub294\\uae30\\uac04\\uc744\\ub9d0\\ud569 \\ub2c8\\ub2e4. 2.\\uc601\\uc5c5\\uc77c: \\ud68c\\uc0ac\\uac00\\uc601\\uc5c5\\uc810\\uc5d0\\uc11c\\uc815\\uc0c1\\uc801\\uc73c\\ub85c\\uc601\\uc5c5\\ud558\\ub294 \\ub0a0\\uc744\\ub9d0\\ud558\\uba70, \\ud1a0\\uc694\\uc77c, \\u2018\\uad00\\uacf5\\uc11c\\uc758\\uacf5\\ud734\\uc77c\\uc5d0\\uad00\\ud55c \\uaddc\\uc815\\u2019\\uc5d0\\ub530\\ub978\\uacf5\\ud734\\uc77c\\uacfc\\uadfc\\ub85c\\uc790\\uc758\\ub0a0\\uc744\\uc81c\\uc678\\ud569\\ub2c8 \\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c2\\uc870 (\\uc6a9\\uc5b4\\uc758 \\uc815\\uc758)\", \"Head\": \"\\u2464 \\ubcf4\\ud5d8\\ub8cc\\uad00\\ub828\\uc6a9\\uc5b4\", \"text\": \"1.\\ubcf4\\ud5d8\\ub8cc : \\uacc4\\uc57d\\uc790\\uac00\\ub9e4\\ub144, \\ub9e46\\uac1c\\uc6d4, \\ub9e43\\uac1c\\uc6d4 \\ub610\\ub294 \\ub9e4\\uc6d4\\uacc4\\uc18d\\ub0a9\\uc785\\ud558\\uae30\\ub85c\\ud55c\\ubcf4\\ud5d8\\ub8cc\\ub97c\\ub9d0\\ud569\\ub2c8\\ub2e4. \\uac00. \\ubcf4\\uc7a5\\ubcf4\\ud5d8\\ub8cc :\\uc190\\ud574\\ub97c\\ubcf4\\uc7a5\\ud558\\ub294\\ub370\\ud544\\uc694\\ud55c\\ubcf4\\ud5d8 \\ub8cc\\ub97c\\ub9d0\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c3\\uc870 (\\ubcf4\\ud5d8\\uae08\\uc758 \\uc9c0\\uae09\\uc0ac\\uc720)\", \"Head\": \"\", \"text\": \"\\ud68c\\uc0ac\\ub294 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\ubcf4\\ud5d8\\uc99d\\uad8c\\uc5d0\\uae30\\uc7ac\\ub41c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758 \\ubcf4\\ud5d8\\uae30\\uac04(\\uc774\\ud558\\u300c\\ubcf4\\ud5d8\\uae30\\uac04\\u300d\\uc774\\ub77c \\ud569\\ub2c8\\ub2e4) \\uc911\\uc5d0 \\uc0c1\\ud574 \\ub85c\\uc7a5\\ud574\\ubd84\\ub958\\ud45c([\\ubcc4\\ud45c2] \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\ucc38\\uc870. \\uc774\\ud558\\uac19\\uc2b5\\ub2c8 \\ub2e4)\\uc5d0\\uc11c\\uc815\\ud55c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774 80% \\uc774\\uc0c1\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uc7a5\\ud574 \\uc0c1\\ud0dc\\uac00\\ub418\\uc5c8\\uc744\\ub54c\\uc5d0\\ub294\\ucd5c\\ucd08 1\\ud68c\\uc5d0\\ud55c\\ud558\\uc5ec 5\\ub144\\uac04\\ub9e4\\ub144\\ubcf4 \\ud5d8\\uc99d\\uad8c\\uc5d0\\uae30\\uc7ac\\ub41c\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc758\\ubcf4\\ud5d8\\uac00\\uc785\\uae08\\uc561\\uc744 \\ubcf4\\ud5d8 \\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ubc1c\\uc0dd\\ud574\\ub2f9\\uc77c(\\ub2e8, \\ud574\\ub2f9\\ub144\\ub3c4\\uc5d0\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09 \\uc0ac\\uc720\\ubc1c\\uc0dd\\uc77c\\uc774\\uc5c6\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ubc1c\\uc0dd\\uc77c \\uc774 \\uc18d\\ud55c \\ub2ec\\uc758 \\ub9c8\\uc9c0\\ub9c9 \\ub0a0\\ub85c \\ud569\\ub2c8\\ub2e4)\\uc5d0 \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574 (80%\\uc774\\uc0c1)\\uc0dd\\ud65c\\uc790\\uae08\\uc73c\\ub85c\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc5d0\\uac8c\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c4\\uc870 (\\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc5d0 \\uad00\\ud55c \\uc138\\ubd80\\uaddc\\uc815)\", \"Head\": \"\", \"text\": \"\\u2460 \\uc81c3\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uc9c0\\uae09\\uc0ac\\uc720)\\uc5d0\\uc11c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774\\uc0c1\\ud574 \\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 180\\uc77c \\uc774\\ub0b4\\uc5d0 \\ud655\\uc815\\ub418\\uc9c0 \\uc54a\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294 \\uc0c1\\ud574\\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 180\\uc77c\\uc774 \\ub418\\ub294 \\ub0a0\\uc758\\uc758\\uc0ac\\uc9c4\\ub2e8\\uc5d0\\uae30 \\ucd08\\ud558\\uc5ec\\uace0\\uc815\\ub420\\uac83\\uc73c\\ub85c\\uc778\\uc815\\ub418\\ub294\\uc0c1\\ud0dc\\ub97c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960 \\ub85c\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc5d0 \\uc7a5\\ud574\\ud310\\uc815\\uc2dc\\uae30 \\ub97c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. \\u2461 \\uc81c1\\ud56d\\uc5d0 \\ub530\\ub77c \\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc774 \\uacb0\\uc815\\ub418\\uc5c8\\uc73c\\ub098 \\uadf8 \\uc774\\ud6c4 \\ubcf4\\uc7a5\\uc744\\ubc1b\\uc744\\uc218\\uc788\\ub294\\uae30\\uac04(\\uacc4\\uc57d\\uc758\\ud6a8\\ub825\\uc774\\uc5c6\\uc5b4\\uc9c4\\uacbd \\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae30\\uac04\\uc774 10\\ub144 \\uc774\\uc0c1\\uc778\\uacc4\\uc57d\\uc740\\uc0c1\\ud574\\ubc1c\\uc0dd\\uc77c \\ubd80\\ud130 2\\ub144\\uc774\\ub0b4\\ub85c\\ud558\\uace0,\\ubcf4\\ud5d8\\uae30\\uac04\\uc774 10\\ub144\\ubbf8\\ub9cc\\uc778\\uacc4\\uc57d \\uc740\\uc0c1\\ud574\\ubc1c\\uc0dd\\uc77c\\ubd80\\ud130 1\\ub144\\uc774\\ub0b4)\\uc5d0\\uc7a5\\ud574\\uc0c1\\ud0dc\\uac00\\ub354\\uc545\\ud654 \\ub418\\ub294\\ub54c\\uc5d0\\ub294\\uadf8\\uc545\\ud654\\ub41c\\uc7a5\\ud574\\uc0c1\\ud0dc\\ub97c\\uae30\\uc900\\uc73c\\ub85c\\uc7a5\\ud574\\uc9c0 \\uae09\\ub960\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\u2462 \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc5d0\\ud574\\ub2f9\\ub418\\uc9c0 \\uc54a\\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790 \\uc758 \\uc9c1\\uc5c5, \\ub098\\uc774, \\uc2e0\\ubd84 \\ub610\\ub294 \\uc131\\ubcc4 \\ub4f1\\uc5d0 \\uad00\\uacc4\\uc5c6\\uc774 \\uc2e0\\uccb4 \\uc758\\uc7a5\\ud574\\uc815\\ub3c4\\uc5d0\\ub530\\ub77c\\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uad6c\\ubd84\\uc5d0\\uc900\\ud558\\uc5ec\\uc9c0 \\uae09\\uc561\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc7a5\\ud574\\ubd84 \\ub958\\ubcc4\\ucd5c\\uc800\\uc9c0\\uae09\\ub960\\uc7a5\\ud574\\uc815\\ub3c4\\uc5d0\\uc774\\ub974\\uc9c0\\uc54a\\ub294\\ud6c4\\uc720\\uc7a5\\ud574 \\uc5d0\\ub300\\ud558\\uc5ec\\ub294\\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc744\\uc9c0 \\uae09\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. \\u2463 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc640\\ud68c\\uc0ac\\uac00\\uc81c3\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uc9c0\\uae09\\uc0ac\\uc720)\\uc758 \\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc5d0\\ub300\\ud574\\ud569\\uc758\\ud558\\uc9c0\\ubabb\\ud560\\ub54c\\uc5d0\\ub294\\ubcf4\\ud5d8 \\uc218\\uc775\\uc790\\uc640\\ud68c\\uc0ac\\uac00\\ud568\\uaed8\\uc81c3\\uc790\\ub97c\\uc815\\ud558\\uace0\\uadf8\\uc81c3\\uc790\\uc758\\uc758 \\uacac\\uc5d0\\ub530\\ub97c\\uc218\\uc788\\uc2b5\\ub2c8\\ub2e4. \\uc81c3\\uc790\\ub294\\uc758\\ub8cc\\ubc95\\uc81c3\\uc870(\\uc758\\ub8cc \\uae30\\uad00)\\uc5d0\\uaddc\\uc815\\ud55c\\uc885\\ud569\\ubcd1\\uc6d0\\uc18c\\uc18d\\uc804\\ubb38\\uc758\\uc911\\uc5d0\\uc815\\ud558\\uba70, \\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ud310\\uc815\\uc5d0\\ub4dc\\ub294\\uc758\\ub8cc\\ube44\\uc6a9\\uc740\\ud68c\\uc0ac\\uac00\\uc804 \\uc561\\ubd80\\ub2f4\\ud569\\ub2c8\\ub2e4. \\u2464 \\uac19\\uc740\\uc0c1\\ud574\\ub85c\\ub450\\uac00\\uc9c0\\uc774\\uc0c1\\uc758\\ud6c4\\uc720\\uc7a5\\ud574\\uac00\\uc0dd\\uae34\\uacbd\\uc6b0\\uc5d0 \\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc744\\ud569\\uc0b0\\ud558\\uc5ec\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc2e0\\uccb4\\ubd80\\uc704\\ubcc4\\ud310\\uc815\\uae30\\uc900\\uc5d0\\ubcc4\\ub3c4\\ub85c\\uc815 \\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uae30\\uc900\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. \\u2465 \\ub2e4\\ub978 \\uc0c1\\ud574\\ub85c\\uc778\\ud558\\uc5ec\\ud6c4\\uc720\\uc7a5\\ud574\\uac00 2\\ud68c \\uc774\\uc0c1 \\ubc1c\\uc0dd\\ud558\\uc600 \\uc744\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\ub54c\\ub9c8\\ub2e4\\uc774\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\uc9c0\\uae09 \\ub960\\uc744\\uacb0\\uc815\\ud569\\ub2c8\\ub2e4. \\uadf8\\ub7ec\\ub098 \\uadf8\\ud6c4\\uc720\\uc7a5\\ud574\\uac00 \\uc774\\ubbf8\\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc744\\uc9c0\\uae09\\ubc1b\\uc740\\ub3d9\\uc77c\\ud55c\\ubd80 \\uc704\\uc5d0\\uac00\\uc911\\ub41c\\ub54c\\uc5d0\\ub294\\ucd5c\\uc885\\uc7a5\\ud574\\uc0c1\\ud0dc\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc5d0\\uc11c\\uc774\\ubbf8\\uc9c0\\uae09\\ubc1b\\uc740\\uc0c1 \\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc744\\ucc28\\uac10\\ud558\\uc5ec\\uc9c0\\uae09\\ud569 \\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc7a5\\ud574\\ubd84\\ub958\\ud45c\\uc758\\uac01\\uc2e0\\uccb4\\ubd80\\uc704\\ubcc4\\ud310\\uc815\\uae30\\uc900 \\uc5d0\\uc11c\\ubcc4\\ub3c4\\ub85c\\uc815\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uae30\\uc900\\uc5d0\\ub530\\ub985\\ub2c8\\ub2e4. \\u2466 \\uc774\\ubbf8\\uc774\\ud2b9\\ubcc4\\uc57d\\uad00\\uc5d0\\uc11c\\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c \\uc790\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc5d0\\ud574\\ub2f9\\ub418\\uc9c0\\uc54a\\uc558\\uac70\\ub098(\\ubcf4\\uc7a5\\uac1c\\uc2dc\\uc774\\uc804 \\uc758\\uc6d0\\uc778\\uc5d0\\uc758\\ud558\\uac70\\ub098\\ub610\\ub294\\uadf8\\uc774\\uc804\\uc5d0\\ubc1c\\uc0dd\\ud55c\\ud6c4\\uc720\\uc7a5\\ud574 \\ub97c\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4), \\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\uc774\\uc9c0\\uae09\\ub418\\uc9c0\\uc54a\\uc558\\ub358\\ud53c\\ubcf4\\ud5d8\\uc790\\uc5d0\\uac8c\\uadf8\\uc2e0\\uccb4\\uc758\\ub3d9\\uc77c\\ubd80 \\uc704\\uc5d0\\ub610\\ub2e4\\uc2dc\\uc81c6\\ud56d\\uc5d0\\uaddc\\uc815\\ud558\\ub294\\ud6c4\\uc720\\uc7a5\\ud574\\uc0c1\\ud0dc\\uac00\\ubc1c\\uc0dd \\ud558\\uc600\\uc744\\uacbd\\uc6b0\\uc5d0\\ub294\\uc9c1\\uc804\\uae4c\\uc9c0\\uc758\\ud6c4\\uc720\\uc7a5\\ud574\\uc5d0\\ub300\\ud55c\\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc774\\uc9c0\\uae09\\ub41c\\uac83\\uc73c\\ub85c\\ubcf4\\uace0 \\ucd5c\\uc885\\ud6c4\\uc720\\uc7a5\\ud574\\uc0c1\\ud0dc\\uc5d0\\ud574\\ub2f9\\ub418\\ub294\\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80% \\uc774\\uc0c1)\\uc0dd\\ud65c\\uc790\\uae08\\uc5d0\\uc11c\\uc774\\ub97c\\ucc28\\uac10\\ud558\\uc5ec\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2467 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00\\uc0c1\\ud574\\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08\\uc744 \\uc77c\\uc2dc\\uc5d0\\uc9c0\\uae09\\ubc1b\\uace0\\uc790\\uc694\\uccad\\ud55c\\ub54c\\uc5d0\\ub294\\ud68c\\uc0ac\\ub294\\uc774\\uacc4\\uc57d\\uc758 \\ud45c\\uc900\\uc774\\uc728\\uc744\\ubc18\\uc601\\ud558\\uc5ec\\uc5f0\\ub2e8\\uc704\\ubcf5\\ub9ac\\ub85c\\ud560\\uc778\\ud55c\\uae08\\uc561\\uc744 \\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\uc81c5\\uc870 (\\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud558\\uc9c0 \\uc54a\\ub294 \\uc0ac\\uc720) \\u2460 \\ud68c\\uc0ac\\ub294 \\ub2e4\\uc74c \\uc911 \\uc5b4\\ub290 \\ud55c \\uac00\\uc9c0\\ub85c \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\uac00 \\ubc1c\\uc0dd\\ud55c\\ub54c\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\uc2b5\\ub2c8\\ub2e4. 1.\\ud53c\\ubcf4\\ud5d8\\uc790\\uac00\\uace0\\uc758\\ub85c\\uc790\\uc2e0\\uc744\\ud574\\uce5c\\uacbd\\uc6b0.\\ub2e4\\ub9cc, \\ud53c\\ubcf4 \\ud5d8\\uc790\\uac00\\uc2ec\\uc2e0\\uc0c1\\uc2e4\\ub4f1\\uc73c\\ub85c\\uc790\\uc720\\ub85c\\uc6b4\\uc758\\uc0ac\\uacb0\\uc815\\uc744\\ud560 \\uc218\\uc5c6\\ub294\\uc0c1\\ud0dc\\uc5d0\\uc11c\\uc790\\uc2e0\\uc744\\ud574\\uce5c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. 2.\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\uace0\\uc758\\ub85c \\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c \\ud574\\uce5c \\uacbd\\uc6b0. \\ub2e4 \\ub9cc, \\uadf8 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00 \\ubcf4\\ud5d8\\uae08\\uc758 \\uc77c\\ubd80 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790 \\uc778\\uacbd\\uc6b0\\uc5d0\\ub294\\ub2e4\\ub978\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc5d0\\ub300\\ud55c\\ubcf4\\ud5d8\\uae08\\uc740\\uc9c0 \\uae09\\ud569\\ub2c8\\ub2e4. 3.\\uacc4\\uc57d\\uc790\\uac00\\uace0\\uc758\\ub85c\\ud53c\\ubcf4\\ud5d8\\uc790\\ub97c\\ud574\\uce5c\\uacbd\\uc6b0 4.\\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\uc784\\uc2e0, \\ucd9c\\uc0b0(\\uc81c\\uc655\\uc808\\uac1c\\ub97c \\ud3ec\\ud568\\ud569\\ub2c8 \\ub2e4), \\uc0b0\\ud6c4\\uae30. \\uadf8\\ub7ec\\ub098 \\ud68c\\uc0ac\\uac00\\ubcf4\\uc7a5\\ud558\\ub294\\ubcf4\\ud5d8\\uae08\\uc9c0 \\uae09\\uc0ac\\uc720\\ub85c\\uc778\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. 5.\\uc804\\uc7c1,\\uc678\\uad6d\\uc758\\ubb34\\ub825\\ud589\\uc0ac,\\ud601\\uba85,\\ub0b4\\ub780,\\uc0ac\\ubcc0,\\ud3ed\\ub3d9 \\u2461 \\ud68c\\uc0ac\\ub294 \\ub2e4\\ub978\\uc57d\\uc815\\uc774 \\uc5c6\\uc73c\\uba74 \\ud53c\\ubcf4\\ud5d8\\uc790\\uac00 \\uc9c1\\uc5c5, \\uc9c1\\ubb34 \\ub610\\ub294\\ub3d9\\ud638\\ud68c\\ud65c\\ub3d9\\ubaa9\\uc801\\uc73c\\ub85c\\uc544\\ub798\\uc5d0\\uc5f4\\uac70\\ub41c\\ud589\\uc704\\ub85c\\uc778 \\ud558\\uc5ec\\uc81c3\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uc9c0\\uae09\\uc0ac\\uc720)\\uc758\\uc0c1\\ud574\\uad00\\ub828\\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\ub54c\\uc5d0\\ub294\\ud574\\ub2f9\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud558\\uc9c0 \\uc54a\\uc2b5\\ub2c8\\ub2e4. 1.\\uc804\\ubb38\\ub4f1\\ubc18(\\uc804\\ubb38\\uc801\\uc778\\ub4f1\\uc0b0\\uc6a9\\uad6c\\ub97c\\uc0ac\\uc6a9\\ud558\\uc5ec\\uc554\\ubcbd\\ub610 \\ub294\\ube59\\ubcbd\\uc744\\uc624\\ub974\\ub0b4\\ub9ac\\uac70\\ub098\\ud2b9\\uc218\\ud55c\\uae30\\uc220,\\uacbd\\ud5d8, \\uc0ac\\uc804 \\ud6c8\\ub828\\uc744\\ud544\\uc694\\ub85c\\ud558\\ub294\\ub4f1\\ubc18\\uc744\\ub9d0\\ud569\\ub2c8\\ub2e4), \\uae00\\ub77c\\uc774\\ub354 \\uc870\\uc885, \\uc2a4\\uce74\\uc774\\ub2e4\\uc774\\ube59, \\uc2a4\\ucfe0\\ubc84\\ub2e4\\uc774\\ube59, \\ud589\\uae00\\ub77c\\uc774 \\ub529,\\uc218\\uc0c1\\ubcf4\\ud2b8,\\ud328\\ub7ec\\uae00\\ub77c\\uc774\\ub529 2.\\ubaa8\\ud130\\ubcf4\\ud2b8, \\uc790\\ub3d9\\ucc28\\ub610\\ub294\\uc624\\ud1a0\\ubc14\\uc774\\uc5d0\\uc758\\ud55c\\uacbd\\uae30,\\uc2dc \\ubc94, \\ud765\\ud589(\\uc774\\ub97c \\uc704\\ud55c\\uc5f0\\uc2b5\\uc744\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4) \\ub610\\ub294\\uc2dc \\uc6b4\\uc804(\\ub2e4\\ub9cc, \\uacf5\\uc6a9\\ub3c4\\ub85c\\uc0c1\\uc5d0\\uc11c\\uc2dc\\uc6b4\\uc804\\uc744\\ud558\\ub294\\ub3d9\\uc548 \\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uac00\\ubc1c\\uc0dd\\ud55c\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\uc7a5\\ud569\\ub2c8\\ub2e4) 3.\\uc120\\ubc15\\uc2b9\\ubb34\\uc6d0, \\uc5b4\\ubd80, \\uc0ac\\uacf5, \\uadf8 \\ubc16\\uc5d0 \\uc120\\ubc15\\uc5d0\\ud0d1\\uc2b9\\ud558 \\ub294 \\uac83\\uc744 \\uc9c1\\ubb34\\ub85c \\ud558\\ub294 \\uc0ac\\ub78c\\uc774 \\uc9c1\\ubb34\\uc0c1 \\uc120\\ubc15\\uc5d0 \\ud0d1\\uc2b9 \\ud558\\uace0\\uc788\\ub294\\ub3d9\\uc548\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c6\\uc870 (\\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\uc758 \\ud1b5\\uc9c0)\", \"Head\": \"\", \"text\": \"\\uacc4\\uc57d\\uc790\\ub610\\ub294\\ud53c\\ubcf4\\ud5d8\\uc790\\ub098\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\ub294\\uc81c3\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758 \\uc9c0\\uae09\\uc0ac\\uc720)\\uc5d0\\uc11c\\uc815\\ud55c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc758\\ubc1c\\uc0dd\\uc744\\uc548\\ub54c\\uc5d0 \\ub294\\uc9c0\\uccb4\\uc5c6\\uc774\\uadf8\\uc0ac\\uc2e4\\uc744\\ud68c\\uc0ac\\uc5d0\\uc54c\\ub824\\uc57c\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c7\\uc870 (\\ubcf4\\ud5d8\\uae08\\uc758 \\uccad\\uad6c)\", \"Head\": \"\", \"text\": \"\\u2460 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\ub294\\ub2e4\\uc74c\\uc758\\uc11c\\ub958\\ub97c\\uc81c\\ucd9c\\ud558\\uace0\\ubcf4\\ud5d8\\uae08\\uc744\\uccad \\uad6c\\ud558\\uc5ec\\uc57c\\ud569\\ub2c8\\ub2e4. 1.\\uccad\\uad6c\\uc11c(\\ud68c\\uc0ac\\uc591\\uc2dd) 2.\\uc0ac\\uace0\\uc99d\\uba85\\uc11c(\\uc9c4\\ub8cc\\ube44\\uacc4\\uc0b0\\uc11c,\\uc7a5\\ud574\\uc9c4\\ub2e8\\uc11c\\ub4f1) 3.\\uc2e0\\ubd84\\uc99d(\\uc8fc\\ubbfc\\ub4f1\\ub85d\\uc99d\\uc774\\ub098\\uc6b4\\uc804\\uba74\\ud5c8\\uc99d\\ub4f1\\uc0ac\\uc9c4\\uc774\\ubd99 \\uc740\\uc815\\ubd80\\uae30\\uad00\\ubc1c\\ud589\\uc2e0\\ubd84\\uc99d, \\ubcf8\\uc778\\uc774\\uc544\\ub2c8\\uba74\\ubcf8\\uc778\\uc758 \\uc778\\uac10\\uc99d\\uba85\\uc11c\\ud3ec\\ud568) 4.\\uae30\\ud0c0\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uac00\\ubcf4\\ud5d8\\uae08\\uc758\\uc218\\ub839\\uc5d0\\ud544\\uc694\\ud558\\uc5ec\\uc81c \\ucd9c\\ud558\\ub294\\uc11c\\ub958(\\ub2e8, \\ub2e8\\uccb4\\ucde8\\uae09\\ud2b9\\ubcc4\\uc57d\\uad00\\uc744\\ubd80\\uac00\\ud558\\ub294 \\uacbd\\uc6b0, \\uc0ac\\ub9dd\\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud560 \\ub54c \\ud53c\\ubcf4\\ud5d8\\uc790\\uc758 \\ubc95\\uc815 \\uc0c1\\uc18d\\uc778\\uc774\\uc544\\ub2cc\\uc790\\uac00\\uccad\\uad6c\\ud558\\ub294\\uacbd\\uc6b0\\ubc95\\uc815\\uc0c1\\uc18d\\uc778\\uc758 \\ud655\\uc778\\uc11c\\ub4f1) \\u2461 \\uc81c1\\ud56d \\uc81c2\\ud638\\uc758 \\uc0ac\\uace0\\uc99d\\uba85\\uc11c\\ub294\\uc758\\ub8cc\\ubc95\\uc81c3\\uc870(\\uc758\\ub8cc\\uae30 \\uad00)\\uc5d0\\uc11c \\uaddc\\uc815\\ud55c\\uad6d\\ub0b4\\uc758 \\ubcd1\\uc6d0\\uc774\\ub098 \\uc758\\uc6d0\\ub610\\ub294\\uad6d\\uc678\\uc758 \\uc758\\ub8cc\\uad00\\ub828\\ubc95\\uc5d0\\uc11c\\uc815\\ud55c\\uc758\\ub8cc\\uae30\\uad00\\uc5d0\\uc11c\\ubc1c\\uae09\\ud55c\\uac83\\uc774\\uc5b4 \\uc57c\\ud569\\ub2c8\\ub2e4.\"}\n{\"index\": {\"_index\": \"index_trial\"}}\n{\"title\": \"1. \\uc0c1\\ud574 \\uad00\\ub828 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subtitle\": \"1-1. \\uc0c1\\ud574 \\uace0\\ub3c4\\uc7a5\\ud574(80%\\uc774\\uc0c1) \\uc0dd\\ud65c\\uc790\\uae08 \\ud2b9\\ubcc4\\uc57d\\uad00\", \"subsubtitle\": \"\\uc81c8\\uc870 (\\ubcf4\\ud5d8\\uae08\\uc758 \\uc9c0\\uae09\\uc808\\ucc28)\", \"Head\": \"\", \"text\": \"\\u2460 \\ud68c\\uc0ac\\ub294\\uc81c7\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uccad\\uad6c)\\uc5d0\\uc11c\\uc815\\ud55c\\uc11c\\ub958\\ub97c\\uc811\\uc218 \\ud55c\\ub54c\\uc5d0\\ub294\\uc811\\uc218\\uc99d\\uc744\\ub4dc\\ub9ac\\uace0\\ud734\\ub300\\uc804\\ud654\\ubb38\\uc790\\uba54\\uc2dc\\uc9c0\\ub610 \\ub294\\uc804\\uc790\\uc6b0\\ud3b8\\ub4f1\\uc73c\\ub85c\\uc1a1\\ubd80\\ud558\\uba70, \\uadf8 \\uc11c\\ub958\\ub97c\\uc811\\uc218\\ud55c\\ub0a0 \\ubd80\\ud130 3\\uc601\\uc5c5\\uc77c\\uc774\\ub0b4\\uc5d0\\ubcf4\\ud5d8\\uae08\\uc744\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2461 \\ud68c\\uc0ac\\uac00\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\ub97c\\uc870\\uc0ac\\u00b7\\ud655\\uc778\\ud558\\uae30\\uc704\\ud574\\ud544 \\uc694\\ud55c \\uae30\\uac04\\uc774\\uc81c1\\ud56d\\uc758 \\uc9c0\\uae09\\uae30\\uc77c\\uc744\\ucd08\\uacfc\\ud560\\uac83\\uc774\\uba85\\ubc31 \\ud788\\uc608\\uc0c1\\ub418\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\uadf8\\uad6c\\uccb4\\uc801\\uc0ac\\uc720\\uc640\\uc9c0\\uae09\\uc608\\uc815\\uc77c \\ubc0f \\ubcf4\\ud5d8\\uae08 \\uac00\\uc9c0\\uae09 \\uc81c\\ub3c4(\\ud68c\\uc0ac\\uac00 \\ucd94\\uc815\\ud558\\ub294 \\ubcf4\\ud5d8\\uae08\\uc758 50% \\uc774\\ub0b4\\ub97c\\uc9c0\\uae09)\\uc5d0\\ub300\\ud558\\uc5ec\\ud53c\\ubcf4\\ud5d8\\uc790\\ub610\\ub294\\ubcf4\\ud5d8\\uc218\\uc775 \\uc790\\uc5d0\\uac8c\\uc989\\uc2dc\\ud1b5\\uc9c0\\ud569\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc9c0\\uae09\\uc608\\uc815\\uc77c\\uc740\\ub2e4\\uc74c \\uac01\\ud638\\uc758\\uc5b4\\ub290\\ud558\\ub098\\uc5d0\\ud574\\ub2f9\\ud558\\ub294\\uacbd\\uc6b0\\ub97c\\uc81c\\uc678\\ud558\\uace0\\ub294\\uc81c 7\\uc870(\\ubcf4\\ud5d8\\uae08\\uc758\\uccad\\uad6c)\\uc5d0\\uc11c\\uc815\\ud55c\\uc11c\\ub958\\ub97c\\uc811\\uc218\\ud55c\\ub0a0\\ubd80\\ud130 30\\uc601\\uc5c5\\uc77c\\uc774\\ub0b4\\uc5d0\\uc11c\\uc815\\ud569\\ub2c8\\ub2e4. 1.\\uc18c\\uc1a1\\uc81c\\uae30 2.\\ubd84\\uc7c1\\uc870\\uc815\\uc2e0\\uccad 3.\\uc218\\uc0ac\\uae30\\uad00\\uc758\\uc870\\uc0ac 4.\\ud574\\uc678\\uc5d0\\uc11c\\ubc1c\\uc0dd\\ud55c\\ubcf4\\ud5d8\\uc0ac\\uace0\\uc5d0\\ub300\\ud55c\\uc870\\uc0ac 5.\\uc81c6\\ud56d\\uc5d0\\ub530\\ub978\\ud68c\\uc0ac\\uc758\\uc870\\uc0ac\\uc694\\uccad\\uc5d0\\ub300\\ud55c\\ub3d9\\uc758\\uac70\\ubd80 \\ub4f1 \\uacc4\\uc57d\\uc790, \\ud53c\\ubcf4\\ud5d8\\uc790 \\ub610\\ub294 \\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc758 \\ucc45\\uc784\\uc788 \\ub294 \\uc0ac\\uc720\\ub85c \\ubcf4\\ud5d8\\uae08 \\uc9c0\\uae09\\uc0ac\\uc720\\uc758 \\uc870\\uc0ac \\ubc0f \\ud655\\uc778\\uc774 \\uc9c0 \\uc5f0\\ub418\\ub294\\uacbd\\uc6b0 6.\\uc81c4\\uc870(\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc5d0\\uad00\\ud55c\\uc138\\ubd80\\uaddc\\uc815) \\uc81c4\\ud56d\\uc5d0\\ub530 \\ub77c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc0ac\\uc720\\uc5d0\\ub300\\ud574\\uc81c3\\uc790\\uc758\\uc758\\uacac\\uc5d0\\ub530\\ub974 \\uae30\\ub85c\\ud55c\\uacbd\\uc6b0 \\u2462 \\uc81c2\\ud56d\\uc5d0 \\uc758\\ud558\\uc5ec\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc758\\ud310\\uc815\\ubc0f\\uc9c0\\uae09\\ud560\\ubcf4\\ud5d8 \\uae08\\uc758\\uacb0\\uc815\\uacfc\\uad00\\ub828\\ud558\\uc5ec\\ud655\\uc815\\ub41c\\uc7a5\\ud574\\uc9c0\\uae09\\ub960\\uc5d0\\ub530\\ub978\\ubcf4 \\ud5d8\\uae08\\uc744\\ucd08\\uacfc\\ud55c\\ubd80\\ubd84\\uc5d0\\ub300\\ud55c\\ubd84\\uc7c1\\uc73c\\ub85c\\ubcf4\\ud5d8\\uae08\\uc9c0\\uae09\\uc774 \\ub2a6\\uc5b4\\uc9c0\\ub294\\uacbd\\uc6b0\\uc5d0\\ub294\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc758\\uccad\\uad6c\\uc5d0\\ub530\\ub77c\\uc774\\ubbf8 \\ud655\\uc815\\ub41c\\ubcf4\\ud5d8\\uae08\\uc744\\uba3c\\uc800\\uac00\\uc9c0\\uae09\\ud569\\ub2c8\\ub2e4. \\u2463 \\uc81c2\\ud56d\\uc5d0\\uc758\\ud558\\uc5ec\\ucd94\\uac00\\uc801\\uc778\\uc870\\uc0ac\\uac00\\uc774\\ub8e8\\uc5b4\\uc9c0\\ub294\\uacbd\\uc6b0, \\ud68c\\uc0ac\\ub294\\ubcf4\\ud5d8\\uc218\\uc775\\uc790\\uc758\\uccad\\uad6c\\uc5d0\\ub530\\ub77c\\ud68c\\uc0ac\\uac00\\ucd94\\uc815\\ud558\\ub294 \\ubcf4\\ud5d8\\uae08\\uc758 50% \\uc0c1\\ub2f9\\uc561\\uc744\\uac00\\uc9c0\\uae09\\ubcf4\\ud5d8\\uae08\\uc73c\\ub85c\\uc9c0\\uae09\\ud569\\ub2c8 \\ub2e4. \\u2464 \\ud68c\\uc0ac\\ub294\\uc81c1\\ud56d\\uc758\\uaddc\\uc815\\uc5d0\\uc815\\ud55c\\uc9c0\\uae09\\uae30\\uc77c\\ub0b4\\uc5d0\\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud558\\uc9c0\\uc54a\\uc558\\uc744\\ub54c(\\uc81c2\\ud56d\\uc758\\uaddc\\uc815\\uc5d0\\uc11c\\uc815\\ud55c\\uc9c0\\uae09\\uc608 \\uc815\\uc77c\\uc744\\ud1b5\\uc9c0\\ud55c\\uacbd\\uc6b0\\ub97c\\ud3ec\\ud568\\ud569\\ub2c8\\ub2e4)\\uc5d0\\ub294\\uadf8\\ub2e4\\uc74c\\ub0a0\\ubd80 \\ud130 \\uc9c0\\uae09\\uc77c\\uae4c\\uc9c0\\uc758 \\uae30\\uac04\\uc5d0 \\ub300\\ud558\\uc5ec [\\ubcc4\\ud45c1] \\ubcf4\\ud5d8\\uae08\\uc744 \\uc9c0\\uae09\\ud560\\ub54c\\uc758\\uc801\\ub9bd\\uc774\\uc728\\uacc4\\uc0b0\\uc5d0\\uc11c\\uc815\\ud55c\\uc774\\uc728\\ub85c\\uacc4\\uc0b0\\ud55c \\uae08\\uc561\\uc744 \\ubcf4\\ud5d8\\uae08\\uc5d0 \\ub354\\ud558\\uc5ec \\uc9c0\\uae09\\ud569