A) \\ubd88\\uc785\\ud55c\\ub3c4\\ub294 \\uc5f0\\uac04 1,800 \\ub9cc\\uc6d0\\uc774\\uace0, \\uc138\\uc561\\uacf5\\uc81c\\ud55c\\ub3c4\\ub294 \\uc5f0\\uac04 \\ubd88\\uc785\\uc561\\uc5d0 \\ub300 \\ud558\\uc5ec \\uc18c\\ub4dd\\uc138\\ubc95\\uc774 \\uc815\\ud558\\ub294 \\uae08\\uc561(\\ud1f4\\uc9c1\\uc5f0\\uae08\\ubcf4\\ud5d8 \\uacc4\\uc57d\\uc790 \\ub0a9\\uc785\\ubd84 \\ud3ec\\ud568\\ud558\\uc5ec \\ub0a9\\uc785\\ud55c \\ubcf4\\ud5d8\\ub8cc \\uc911 \\uc5f0\\uac04 400 \\ub9cc\\uc6d0 \\ud55c\\ub3c4\\ub85c \\ub0a9\\uc785\\ubcf4\\ud5d8\\ub8cc\\uc758 12%)\\uc785\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\ud574\\ub2f9 \\uacfc\\uc138\\uae30\\uac04\\uc5d0 \\uc885\\ud569\\uc18c\\ub4dd\\uacfc\\uc138\\ud45c\\uc900\\uc744 \\uacc4\\uc0b0\\ud560 \\ub54c \\ud569\\uc0b0\\ud558\\ub294 \\uc885 \\ud569\\uc18c\\ub4dd\\uae08\\uc561\\uc774 4 \\ucc9c\\ub9cc\\uc6d0(\\uadfc\\ub85c\\uc18c\\ub4dd\\ub9cc \\uc788\\ub294 \\uacbd\\uc6b0\\uc5d0\\ub294 \\ucd1d \\uae09\\uc5ec\\uc561 5,500 \\ub9cc \\uc6d0) \\uc774\\ud558\\uc778 \\uacbd\\uc6b0 \\ub0a9\\uc785\\ubcf4\\ud5d8\\ub8cc\\uc758 15%\\uae4c\\uc9c0 \\uc138\\uc561\\uacf5\\uc81c\\ud55c\\ub3c4\\uac00 \\uc801\\uc6a9\\ub429\\ub2c8\\ub2e4. Q) \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1) \\uc5d0 \\uac00\\uc785\\ud558\\uc168\\ub2e4\\uac00 \\uc911\\ub3c4 \\ud574\\uc9c0\\ud558\\uac70\\ub098 \\uc77c\\uc2dc\\uae08\\uc73c\\ub85c \\uc218\\ub839\\ud558\\ub294 \\uacbd\\uc6b0 \\uc138\\uc81c\\uc0c1 \\uc758 \\uc5b4\\ub5a0\\ud55c \\ubd88\\uc774\\uc775\\uc774 \\uc788\\ub098\\uc694? A) \\uc911\\ub3c4\\uc5d0 \\ud574\\uc9c0\\ud558\\uac70\\ub098 \\uc77c\\uc2dc\\uae08 \\uc218\\ub839 \\ub4f1\\uc758 \\ubc29\\ubc95\\uc73c\\ub85c \\uc5f0\\uae08\\uc678\\uc218\\ub839\\uc2dc\\uc5d0\\ub294 \\uae30 \\ud0c0\\uc18c\\ub4dd\\uc138(16.5%, \\uc9c0\\ubc29\\uc18c\\ub4dd\\uc138 \\ud3ec\\ud568)\\uac00 \\ubd80\\uacfc\\ub429\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc5f0\\uae08\\uc218\\ub839\\ud55c \\ub3c4 \\uc774\\ub0b4\\ub294 \\uc5f0\\uae08\\uc18c\\ub4dd\\uc138(5.5~3.3%, \\uc9c0\\ubc29\\uc18c\\ub4dd\\uc138 \\ud3ec\\ud568)\\uac00 \\ubd80\\uacfc\\ub429\\ub2c8\\ub2e4. \\uad00\\ub828\\uc138\\ubc95\\uc5d0\\uc11c \\uc815\\ud558\\ub294 \\uc758\\ub8cc\\ube44\\uc778\\ucd9c\\uc5d0 \\ud574\\ub2f9\\ud558\\ub294 \\uae08\\uc561\\uacfc, \\uacc4\\uc57d\\uc790\\uc758 \\uc0ac\\ub9dd \\ub4f1 \\ubd80\\ub4dd\\uc774\\ud55c \\uc0ac\\uc720\\ub85c \\uc778\\ud55c \\uacbd\\uc6b0\\uc5d0\\ub294 \\uc885\\ud569\\uc18c\\ub4dd\\uc138 \\ub300\\uc0c1\\uc5d0 \\ud3ec\\ud568\\ub418\\uc9c0 \\uc54a\\uc73c \\uba70, \\uc5f0\\uae08\\uc18c\\ub4dd\\uc138(5.5~3.3%, \\uc9c0\\ubc29\\uc18c\\ub4dd\\uc138 \\ud3ec\\ud568)\\ub85c \\ubd84\\ub9ac\\uacfc\\uc138\\ub429\\ub2c8\\ub2e4. \\u203b \\uc138\\ubd80 \\uc0ac\\ud56d\\uc740 \\ub3d9 \\uc0c1\\ud488\\uc758 \\uc57d\\uad00\\uc744 \\ucc38\\uc870\\ud558\\uc2dc\\uae30 \\ubc14\\ub78d\\ub2c8\\ub2e4. Q) \\uc5f0\\uae08\\uc800\\ucd95\\uc190\\ud574\\ubcf4\\ud5d8 \\ubb34\\ubc30\\ub2f9 \\uc0bc\\uc131\\ud654\\uc7ac \\uc5f0\\uae08\\ubcf4\\ud5d8 \\uc544\\ub984\\ub2e4\\uc6b4\\uc0dd\\ud65c(1510.1)\\uc758 \\ubd88\\uc785\\uc644\\ub8cc \\ud6c4 \\uc5f0\\uae08 \\uc218\\ub839\\ud560 \\ub54c \\uc5b4\\ub5bb\\uac8c \\uacfc\\uc138\\uac00 \\ub418\\ub098\\uc694? A) \\uc5f0\\uae08\\uc800\\ucd95\\uc758 \\uac00\\uc785\\uc790\\uac00 \\uacc4\\uc57d\\ub0b4\\uc6a9\\uc5d0 \\ub530\\ub77c \\uc5f0\\uae08\\uc744 \\uc9c0\\uae09\\ubc1b\\ub294 \\uacbd\\uc6b0 \\uc5f0\\uae08\\uc218 \\ub839 \\ud55c\\ub3c4 \\uc774\\ub0b4\\uc758 \\uae08\\uc561\\uc740 \\uc5f0\\uae08\\uc18c\\ub4dd\\uc73c\\ub85c \\ubcf4\\uc544 \\uc5f0\\uae08\\uc18c\\ub4dd\\uc138\\uac00 \\uc6d0\\ucc9c\\uc9d5\\uc218 \\ub429 \\ub2c8\\ub2e4. \\uc5f0\\uae08\\uc218\\ub839 \\ud55c\\ub3c4\\ub97c \\ucd08\\uacfc\\ud55c \\uae08\\uc561\\uc740 \\uae30\\ud0c0\\uc18c\\ub4dd\\uc138\\uac00 \\uc6d0\\ucc9c\\uc9d5\\uc218 \\ub429\\ub2c8\\ub2e4. \\ub2e4\\ub9cc, \\uc138\\uc561\\uacf5\\uc81c \\ub0a9\\uc785\\ud55c\\ub3c4(\\uc5f0 400 \\ub9cc\\uc6d0)